    <TfrxDOAQuery Name="dstMain" UserName="dstMain" CloseDataSource="True" FieldAliases.Text="ORDER_ID=ORDER_ID&#13;&#10;WARENAME=WARENAME&#13;&#10;SERNUM=SERNUM&#13;&#10;BRAND_CODE=BRAND_CODE&#13;&#10;MODEL=MODEL&#13;&#10;IMEI=IMEI&#13;&#10;PBU_SUM=PBU_SUM&#13;&#10;FAMILY=FAMILY&#13;&#10;FIRSTNAME=FIRSTNAME&#13;&#10;MIDNAME=MIDNAME&#13;&#10;PASSPORTNUMBER=PASSPORTNUMBER&#13;&#10;PASSPORTGIVEN=PASSPORTGIVEN&#13;&#10;BIRTHDATE=BIRTHDATE&#13;&#10;SALEDATE=SALEDATE&#13;&#10;NAMEBASE=NAMEBASE&#13;&#10;PBU_SUM_STR=PBU_SUM_STR" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="SELECT &#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'ORDER_ID') AS ORDER_ID,&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'WARENAME') AS WARENAME,/*Íàèìåíîâàíèå òîâàðà*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'SERNUM') AS SERNUM,/*Ñåðèéíûé íîìåð*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'BRAND_CODE') AS BRAND_CODE,/*Áðýíä (èçãîòîâèòåëü)*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'MODEL') AS MODEL,/*Ìîäåëü*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'IMEI') AS IMEI,/*IMEI*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'PBU_SUM') AS PBU_SUM,/*Ñóììà*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'FAMILY') AS FAMILY,/*Ôàìèëèÿ*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'FIRSTNAME') AS FIRSTNAME,/*Èìÿ*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'MIDNAME') AS MIDNAME,/*Îò÷åñòâî*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'PASSPORTNUMBER') AS PASSPORTNUMBER,/*¹ ïàñïîðòà*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'PASSPORTGIVEN') AS PASSPORTGIVEN,/*Êåì êîãäà âûäàí*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'BIRTHDATE') AS BIRTHDATE,/*ÄÐ*/&#13;&#10;trunc(sysdate) as SALEDATE,/*Äàòà*/&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'FAMILY') ||' '||&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'FIRSTNAME') ||' '||&#13;&#10;(SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'MIDNAME') AS NAMEBASE,&#13;&#10;(SELECT 'Îäèí ðóáëü' FROM DUAl ) AS PBU_SUM_STR&#13;&#10;/*(SELECT STR_FUNC.FN_MONEYTOSTRFULL( (SELECT ZVALUE5 FROM TMP_TABLE WHERE ZVALUE1 = 'PBU' AND ZVALUE2 = 'PBU_SUM') ) FROM DUAl ) AS PBU_SUM_STR*/&#13;&#10;FROM DUAL&#13;&#10;UNION&#13;&#10;SELECT TO_CHAR (o.order_id) AS order_id, &#13;&#10;       g.name AS warename, &#13;&#10;        CASE&#13;&#10;          WHEN (SELECT COUNT(1)&#13;&#10;                  FROM specgoods s1&#13;&#10;                 WHERE s1.good_code = g.good_code&#13;&#10;                   AND s1.st_code IN ('PDS2', 'PDS3', 'PDS5')) = 0&#13;&#10;          THEN&#13;&#10;            pi.sernum&#13;&#10;          ELSE&#13;&#10;            pis.pdsno&#13;&#10;          END AS sernum,&#13;&#10;       g.brand_code AS brand_code, &#13;&#10;       pi.model AS MODEL,&#13;&#10;       opv4.opvalue as IMEI, &#13;&#10;       opv5.opvalue as PBU_SUM,&#13;&#10;       a.family,&#13;&#10;       a.firstname,&#13;&#10;       a.midname,&#13;&#10;       a.passportnumber,&#13;&#10;       a.passportgiven,   &#13;&#10;       to_char(a.birthdate, 'dd.mm.yyyy') as BIRTHDATE,   &#13;&#10;       pis.saledate AS saledate, &#13;&#10;       a.namebase,&#13;&#10;       STR_FUNC.FN_MONEYTOSTRFULL(opv5.opvalue) as PBU_SUM_STR&#13;&#10;  FROM orders o&#13;&#10;  JOIN order_status os ON os.os_code = o.os_code&#13;&#10;  JOIN o_good_item gi ON gi.ignore &#60;&#62; 'Y' AND gi.order_id = o.order_id&#13;&#10;  JOIN g_unit gu ON gi.g_unit_id = gu.g_unit_id&#13;&#10;  JOIN good g ON gu.good_code = g.good_code&#13;&#10;  JOIN STORAGE st ON st.storage_code = gi.storage_code AND st.werk_code = gi.werk_code&#13;&#10;  LEFT JOIN ogi_pi_link opl ON opl.og_item_id = gi.og_item_id&#13;&#10;  LEFT JOIN product_item pi ON opl.pi_guid = pi.pi_guid&#13;&#10;  LEFT JOIN p_item_states pis ON  opl.pis_id = pis.pis_id AND opl.werk_code = pis.werk_code&#13;&#10;  LEFT JOIN  address a ON o.address_id = a.address_id&#13;&#10;  LEFT JOIN w_legals wl ON wl.wl_id = o.wl_id&#13;&#10;  LEFT JOIN address al ON al.address_id = wl.address_id&#13;&#10;  LEFT JOIN op_values opv ON o.order_id = opv.order_id   AND opv.op_code = 'CORRINVNUM'&#13;&#10;  LEFT JOIN op_values opv2 ON o.order_id = opv2.order_id  AND opv2.op_code = 'PRNINVNUM'&#13;&#10;  LEFT JOIN op_values opv3 ON o.order_id = opv3.order_id  AND opv3.op_code = 'SCDIENNUM'&#13;&#10;  LEFT JOIN op_values opv4 ON o.order_id = opv4.order_id  AND opv4.op_code = 'IMEI'&#13;&#10;  LEFT JOIN op_values opv5 ON o.order_id = opv5.order_id  AND opv5.op_code = 'PBU_SUM'&#13;&#10;  LEFT JOIN orders o2 ON pis.zordrchange = o2.order_id &#13;&#10;  LEFT JOIN order_status os2 ON o2.os_code = os2.os_code&#13;&#10; WHERE o.ot_code IN ('PBU')&#13;&#10;  AND o.order_id = :order_id" PropData="05704C656674022E0470546F700226095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
