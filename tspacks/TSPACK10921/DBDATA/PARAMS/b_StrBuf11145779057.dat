    <TfrxDOAQuery Name="odsWerkADDR" UserName="odsWerkADDR" CloseDataSource="True" FieldAliases.Text="SHOP_NAME=SHOP_NAME&#13;&#10;SHOP_ADDR=SHOP_ADDR&#13;&#10;INN=INN&#13;&#10;CITY=CITY" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="SELECT&#13;&#10;      pk_franch.get_werkname('ORDERS', :order_id, 'LEGALADDR') AS shop_name,&#13;&#10;      a.CITY || CASE WHEN NOT a.CITY IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.STREET || CASE WHEN NOT a.STREET IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.HOUSE_NUM || CASE WHEN NOT a.HOUSE_NUM IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.house_corp IS NULL THEN 'êîðï. '||a.house_corp||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.BUILDING IS NULL THEN 'ñòð. '||a.BUILDING||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.ROOM_NUMBER IS NULL THEN 'ê.' ELSE '' END || a.ROOM_NUMBER&#13;&#10;        AS shop_addr,&#13;&#10;      'ÈÍÍ '||abn.INN AS inn,&#13;&#10;      a.CITY&#13;&#10;  FROM&#13;&#10;      address a, address au, addr_bninfo abn&#13;&#10; WHERE&#13;&#10;      a.address_id (+) = pk_franch.get_addr_id('ORDERS', :order_id, 'PHYSADDR') AND&#13;&#10;      au.address_id (+) = pk_franch.get_addr_id('ORDERS', :order_id, 'LEGALADDR') AND&#13;&#10;      abn.address_id (+) = au.address_id&#13;&#10;UNION&#13;&#10;SELECT&#13;&#10;      pk_franch.get_werkname('ORDERS', (select order_id from orders where rownum = 1), 'LEGALADDR') AS shop_name,&#13;&#10;      a.CITY || CASE WHEN NOT a.CITY IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.STREET || CASE WHEN NOT a.STREET IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.HOUSE_NUM || CASE WHEN NOT a.HOUSE_NUM IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.house_corp IS NULL THEN 'êîðï. '||a.house_corp||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.BUILDING IS NULL THEN 'ñòð. '||a.BUILDING||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.ROOM_NUMBER IS NULL THEN 'ê.' ELSE '' END || a.ROOM_NUMBER&#13;&#10;        AS shop_addr,&#13;&#10;      'ÈÍÍ '||abn.INN AS inn,&#13;&#10;      a.CITY&#13;&#10;  FROM&#13;&#10;      address a, address au, addr_bninfo abn, orders o&#13;&#10; WHERE&#13;&#10;      rownum &#60;= 1 AND&#13;&#10;      a.address_id (+) = pk_franch.get_addr_id('ORDERS', o.order_id, /*:order_id,*/ 'PHYSADDR') AND&#13;&#10;      au.address_id (+) = pk_franch.get_addr_id('ORDERS', o.order_id, /*:order_id,*/ 'LEGALADDR') AND&#13;&#10;      abn.address_id (+) = au.address_id" PropData="05704C6566740386000470546F700224095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
