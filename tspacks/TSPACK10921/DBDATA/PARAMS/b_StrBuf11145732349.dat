    <TfrxDOAQuery Name="odsAddr" UserName="odsAddr" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A250000000300000001000000090000003A4F524445525F494405000000040000003130360000000000" SQL.Text="select o.order_id,&#13;&#10;  o.ot_code AS OT_CODE, WT.WBLTYPE,&#13;&#10;  w.werk_code||' \ '||o.ORDER_ID as ORDER_ID_ZORDR,&#13;&#10;  w.werk_code||' \ '||oprnt.order_id as ORDER_ID_ZADDR,&#13;&#10;  case when (o.DESIREDDT is null) then&#13;&#10;  (select opvalue from op_values where order_id = o.order_id&#13;&#10;  and og_item_id is null and op_code = 'DLVDATE') else to_char(o.DESIREDDT, 'dd.mm.yyyy') end as DESIREDDT,&#13;&#10;  a.POST_CODE || case when not a.POST_CODE is null then ', ' else '' end ||&#13;&#10;  a.REGION || case when not a.REGION is null then ', ' else '' end ||&#13;&#10;  a.CITY || case when not a.CITY is null then ', ' else '' end ||&#13;&#10;  a.DISTRICT || case when not a.DISTRICT is null then ', ' else '' end ||&#13;&#10;  a.STREET || case when not a.STREET is null then ', ' else '' end ||&#13;&#10;  a.HOUSE_NUM || case when not a.HOUSE_NUM is null then ', ' else '' end ||&#13;&#10;  case when not a.house_corp is null then 'êîðï. '||a.house_corp||', ' else '' end ||&#13;&#10;  case when not a.BUILDING is null then 'ñòð. '||a.BUILDING||', ' else '' end ||&#13;&#10;  case when not a.ROOM_NUMBER is null then 'êâ.' else '' end || a.ROOM_NUMBER&#13;&#10;    AS FULLADDR,&#13;&#10;  nvl((SELECT GET_OP_VALUES(:ORDER_ID, null, 'FIO') FROM dual), t.NAME||' '||a.namebase) AS FULLNAME,&#13;&#10;  a.PASSPORTNUMBER||', '||a.PASSPORTGIVEN as PSPRTINFO,&#13;&#10;  dt.name||': '||a.DOCVALUE as DOCINFO,&#13;&#10;  '(Äîêóìåíò äîñòàâêè ¹ '||o.ORDER_ID||' )' as PRNT_INFO,&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.'&#13;&#10;   from staff s, p_staff ps&#13;&#10;  where s.user_code = nvl(oprnt.createusercode,o.createusercode)&#13;&#10;    and ps.id = s.p_staff_id) as salesman,&#13;&#10;  a.*&#13;&#10;from&#13;&#10;  address a, addr_title t, orders_link ol, orders o, addr_doctype dt, orders oprnt, werk w, w_type wt&#13;&#10;where&#13;&#10;  a.address_id in&#13;&#10;  ( select address_id from orders where order_id = :order_id )&#13;&#10;  and a.title_code = t.title_code(+)&#13;&#10;  and o.address_id = a.address_id&#13;&#10;  and o.order_id = :order_id&#13;&#10;  and ol.child_order_id(+) = o.order_id&#13;&#10;  and a.doctype_code = dt.doctype_code(+)&#13;&#10;  and oprnt.order_id(+) = ol.parent_order_id&#13;&#10;  and w.is_base = 'Y'&#13;&#10;  AND wt.wtype_code = w.wtype_code " PropData="05704C6566740360010470546F700238095661726961626C657301010C460000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22313036220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;QUANTITY=QUANTITY&#13;&#10;PRICE2=PRICE2&#13;&#10;AMOUNT=AMOUNT&#13;&#10;GQ_CODE=GQ_CODE&#13;&#10;STRGNAME=STRGNAME&#13;&#10;GQ_CODEEAN13=GQ_CODEEAN13&#13;&#10;GQ_CODE39=GQ_CODE39&#13;&#10;BONUS_SUM=BONUS_SUM&#13;&#10;LOYL_PAY_SUM=LOYL_PAY_SUM&#13;&#10;PAY_SUM=PAY_SUM&#13;&#10;VO=VO&#13;&#10;WRQST_VO=WRQST_VO" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A250000000300000001000000090000003A4F524445525F494405000000040000003130360000000000" SQL.Text="SELECT t1.good_code,&#13;&#10;       t1.name,&#13;&#10;       t1.quantity,&#13;&#10;       t1.price2,&#13;&#10;       t1.amount,&#13;&#10;       t1.gq_code,&#13;&#10;       t1.strgname,&#13;&#10;       t1.gq_codeean13,&#13;&#10;       t1.gq_code39,&#13;&#10;       t1.bonus_sum,&#13;&#10;/*       -- Óáèðàåì ëèøíèå ðóáëè îò îêðóãëåíèÿ&#13;&#10;      CASE&#13;&#10;        WHEN t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id) &#60; 0 THEN&#13;&#10;         CASE&#13;&#10;           WHEN t1.loyl_pay_sum + t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id) &#62; 0 THEN&#13;&#10;             t1.loyl_pay_sum + t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id)&#13;&#10;           ELSE 0&#13;&#10;         END&#13;&#10;        ELSE&#13;&#10;         t1.loyl_pay_sum&#13;&#10;      END AS loyl_pay_sum,&#13;&#10;*/&#13;&#10;     t1.loyl_pay_sum,&#13;&#10;      t1.amount - CASE&#13;&#10;        WHEN t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id) &#60; 0 THEN&#13;&#10;          CASE&#13;&#10;            WHEN t1.loyl_pay_sum + t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id) &#62; 0 THEN&#13;&#10;              t1.loyl_pay_sum + t1.loyl_pay_all_sum - SUM(t1.loyl_pay_sum) OVER(ORDER BY t1.og_item_id)&#13;&#10;            ELSE 0&#13;&#10;          END&#13;&#10;        ELSE&#13;&#10;         t1.loyl_pay_sum&#13;&#10;      END AS pay_sum,&#13;&#10;       t1.VO,&#13;&#10;       t1.WRQST_VO&#13;&#10;  FROM (SELECT ogi.og_item_id,&#13;&#10;               g.good_code,&#13;&#10;               g.name,&#13;&#10;               ogi.quantity,&#13;&#10;               ogi.price2 AS price2,&#13;&#10;               ogi.quantity * ogi.price2 AS amount,&#13;&#10;               CASE&#13;&#10;                 WHEN ogi.st_code = 'STD' AND ogi.price1 = ogi.price2 THEN&#13;&#10;                  '22' || lpad(TRIM(g.good_code), 8, '0') ||&#13;&#10;                  TRIM(substr(TO_CHAR(ogi.quantity, '09'), 2, 2))&#13;&#10;                 WHEN (ogi.st_code &#60;&#62; 'STD' OR&#13;&#10;                      (ogi.st_code = 'STD' AND ogi.price1 &#60;&#62; ogi.price2)) THEN&#13;&#10;                  '139' || lpad(TRIM(g.good_code), 8, '0') || '01' ||&#13;&#10;                  TRIM(TO_CHAR(NVL(ogi.price2, 0) * 100, '00000009')) ||&#13;&#10;                  TRIM(TO_CHAR(ogi.quantity, '09'))&#13;&#10;                 ELSE&#13;&#10;                  '000000000000'&#13;&#10;               END AS gq_code,&#13;&#10;               '/ Ñêëàä: ' || st.shortname || ' /' AS strgname,&#13;&#10;               digittoean13('22' || lpad(TRIM(g.good_code), 8, '0') ||&#13;&#10;                            TRIM(substr(TO_CHAR(ogi.quantity, '09'), 2, 2))) AS gq_codeean13,&#13;&#10;               digittocode39('139' || lpad(TRIM(g.good_code), 8, '0') || '01' ||&#13;&#10;                             TRIM(TO_CHAR(NVL(ogi.price2, 0) * 100,&#13;&#10;                                          '00000009')) ||&#13;&#10;                             TRIM(TO_CHAR(ogi.quantity, '09'))) AS gq_code39,&#13;&#10;               nvl(c.amount, 0) * ogi.quantity AS bonus_sum,&#13;&#10;               -- Ñóììà áàëëîâ ïðèõîäÿùàÿñÿ íà ýòó ïîçèöèþ ñ îêðóãëåíèåì äî öåëûõ&#13;&#10;/*               CASE&#13;&#10;                 WHEN NVL(f.docsum, 0) &#62; 0 AND&#13;&#10;                      pk_orders.can_good_pay_loylty(g.good_code) = 'Y' THEN&#13;&#10;                  ROUND(f.docsum * ogi.amount /&#13;&#10;                        SUM(DECODE(pk_orders.can_good_pay_loylty(g.good_code),&#13;&#10;                                   'Y',&#13;&#10;                                   ogi.amount,&#13;&#10;                                   0)) OVER())&#13;&#10;                 ELSE&#13;&#10;                  0&#13;&#10;               END loyl_pay_sum,&#13;&#10;*/&#13;&#10;               nvl(oc_b.amount, 0) as loyl_pay_sum,&#13;&#10;               NVL(f.docsum, 0) AS loyl_pay_all_sum,&#13;&#10;               nvl(VO.opvalue, 'N') as VO,&#13;&#10;               nvl(ov2.opvalue, 'N') as WRQST_VO&#13;&#10;          FROM o_good_item ogi&#13;&#10;          JOIN g_unit gu&#13;&#10;            ON gu.g_unit_id = ogi.g_unit_id&#13;&#10;          JOIN good g&#13;&#10;            ON g.good_code = gu.good_code&#13;&#10;          LEFT JOIN storage st&#13;&#10;            ON st.storage_code = ogi.storage_code&#13;&#10;           AND st.werk_code = ogi.werk_code&#13;&#10;          LEFT JOIN money_fict f&#13;&#10;            ON f.order_id = ogi.order_id&#13;&#10;           AND f.mtnmbr = get_tsoptions('CRM_CARD_MTNMBR')&#13;&#10;           AND f.is_paid = 'N'&#13;&#10;          LEFT JOIN o_condition c&#13;&#10;            ON c.order_id = ogi.order_id&#13;&#10;           AND c.og_item_id = ogi.og_item_id&#13;&#10;           AND c.sapcode = 'CMSS'&#13;&#10;         -- TS-2631  Íàäïèñü &#34;Âèòðèííûé îáðàçåö íà ïå÷àòíîé ôîðìå&#34;&#13;&#10;          left join op_values VO&#13;&#10;            on VO.op_code = 'VITRINE'&#13;&#10;           and VO.og_item_id = ogi.og_item_id&#13;&#10;          join orders o&#13;&#10;            on ogi.order_id = o.order_id&#13;&#10;          left join orders o2&#13;&#10;            on o2.chainnum = o.chainnum&#13;&#10;           and o2.ot_code = 'WRQST'&#13;&#10;          left join ogi_link ogl &#13;&#10;            on ogl.og_child_item = ogi.og_item_id &#13;&#10;	   and  ogl.link_type ='PARDOCITEM'&#13;&#10;          left join o_good_item ogi2&#13;&#10;            on ogi2.order_id = o2.order_id        &#13;&#10;	   and ogi2.g_unit_id = ogl.og_parent_item&#13;&#10;          left join op_values ov2&#13;&#10;            on ogi2.og_item_id = ov2.og_item_id&#13;&#10;           and ov2.op_code = 'VITRINE'&#13;&#10;         -- TS-2631  &#13;&#10;        left join o_condition oc_b on oc_b.og_item_id = ogi.og_item_id&#13;&#10;         and oc_b.sapcode in ('CRMS', 'CBMS')&#13;&#10;         WHERE ogi.order_id = :order_id&#13;&#10;           AND ogi.ignore &#60;&#62; 'Y'&#13;&#10;           AND ogi.quantity &#62; 0&#13;&#10;         ORDER BY ogi.og_item_id&#13;&#10;        ) t1" PropData="05704C65667403C8000470546F700238095661726961626C657301010C460000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22313036220000"/>
    <TfrxDOAQuery Name="odsAComm" UserName="odsAComm" CloseDataSource="True" FieldAliases.Text="NAME=NAME&#13;&#10;VALUE=VALUE&#13;&#10;ADDITIONAL_INFO=ADDITIONAL_INFO&#13;&#10;COMM_PRIORITY=COMM_PRIORITY" BCDToCurrency="False" Master="odsAddr" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="address_id;order_id" Params.PropData="04446174610A3C00000003000000020000000B0000003A414444524553535F4944050000000000000000000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="SELECT 'Òåëåôîí' AS name, &#13;&#10;       GET_OP_VALUES(:ORDER_ID, null, 'CONTACTTEL') AS value,&#13;&#10;       '' AS additional_info,&#13;&#10;       -1 as comm_priority&#13;&#10;  FROM dual&#13;&#10;UNION ALL&#13;&#10;SELECT ct.name,&#13;&#10;       ac.value,&#13;&#10;       CASE&#13;&#10;          WHEN NOT ac.additional_info IS NULL THEN&#13;&#10;           ' / ' || ac.additional_info || ' /'&#13;&#10;        END AS additional_info,&#13;&#10;       ac.comm_priority&#13;&#10;  FROM addr_comm ac, comm_type ct&#13;&#10; WHERE ac.address_id = :address_id&#13;&#10;   AND ac.ct_code = ct.ct_code&#13;&#10;   AND ac.ct_code &#60;&#62; 'INT'&#13;&#10;   AND trim(ac.value) &#60;&#62; trim(GET_OP_VALUES(:ORDER_ID, null, 'CONTACTTEL'))&#13;&#10; ORDER BY comm_priority ASC" PropData=""/>
