    <TfrxDOAQuery Name="dstOrder" UserName="dstOrder" CloseDataSource="True" FieldAliases.Text="" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;&#13;&#10;  nvl((SELECT opvalue FROM op_values opv&#13;&#10;       WHERE opv.order_id = :order_id&#13;&#10;         AND op_code = 'OUTDOCNUM'), 0 ) AS OutNo,&#13;&#10;&#13;&#10;  (SELECT max(operationdate)&#13;&#10;   FROM o_status_log osl, orders o&#13;&#10;   WHERE o.order_id = :order_id&#13;&#10;     AND osl.order_id = o.order_id&#13;&#10;     AND (osl.os_code IN ('ZBNAL_PAYD','ZORDR_PAYD','ZPDS_PAYD')&#13;&#10;     OR o.ot_code IN ('GISSUE'))) AS OutDate,&#13;&#10;  o.order_id||lower(get_werk_base) AS recordnumber,&#13;&#10;  o.*,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCDAT') AS PayDocDat,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCNUM') AS PayDocNum,&#13;&#10;&#13;&#10;  (SELECT&#13;&#10;     ps.family || ' ' ||&#13;&#10;     substr( ps.fname, 0, 1) || '.' ||&#13;&#10;     substr( ps.lname, 0, 1) || '.'&#13;&#10;   FROM p_staff ps, staff s, o_status_log osl&#13;&#10;   WHERE ps.id = s.p_staff_id&#13;&#10;     AND s.user_code = osl.changeusercode&#13;&#10;     AND osl.order_id = :order_id&#13;&#10;     AND osl.os_code = 'ZBNAL_SHPD' ) as shipper,&#13;&#10;  (select osl.operationdate&#13;&#10;     from o_status_log osl&#13;&#10;    where osl.order_id = (select o.parent_order_id from orders o where o.order_id=:order_id)&#13;&#10;      and osl.os_code IN('ZDLVGI_CR')&#13;&#10;      and rownum &#60;=1) AS ShipDate,&#13;&#10;     opv.OPVALUE AS SAPORDNUM&#13;&#10;FROM orders o, staff s&#13;&#10;     ,op_values opv&#13;&#10;WHERE o.order_id = :order_id&#13;&#10;  AND o.createusercode = s.user_code(+)&#13;&#10;  and opv.order_id (+) = o.parent_order_id&#13;&#10;  and opv.op_code (+) = 'SAPORDNUM'" PropData="05704C656674023C0470546F700264095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="dstWerk" UserName="dstWerk" CloseDataSource="True" FieldAliases.Text="DIRECTOR=DIRECTOR&#13;&#10;CHIEFACC=CHIEFACC&#13;&#10;MAXOUTNO=MAXOUTNO&#13;&#10;WERK_CODE=WERK_CODE&#13;&#10;NDS=NDS" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  pk_franch.get_chief_names('ORDERS',o.order_id,'DIRECTOR','LEGALADDRC') as director,&#13;&#10;  pk_franch.get_chief_names('ORDERS',o.order_id,'CHIEFACC','LEGALADDRC') as chiefacc,&#13;&#10;  TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'OUTDOCNUM', 'ZBNAL', 'N' ) maxoutno,&#13;&#10;  w.werk_code as werk_code,&#13;&#10;  to_number( get_tsoptions( 'NDSTAXRATE' ) ) AS nds&#13;&#10;FROM werk w, orders o&#13;&#10;WHERE w.is_base = 'Y'&#13;&#10;AND o.order_id = :order_id&#13;&#10;and not exists (SELECT zvalue1 FROM tmp_table)&#13;&#10;union all&#13;&#10;SELECT&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.' &#13;&#10;     from p_staff ps where ps.id=wp.director_id) as director,&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.'&#13;&#10;     from p_staff ps where ps.id=wp.chiefacc_id) as chiefacc,&#13;&#10;  TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'OUTDOCNUM', 'ZBNAL', 'N' ) maxoutno,&#13;&#10;  wp.werk_code as werk_code,&#13;&#10;  to_number( get_tsoptions( 'NDSTAXRATE' ) ) AS nds&#13;&#10;FROM wp_link wp&#13;&#10;WHERE wp.werk_code = get_werk_base&#13;&#10;AND wp.wp_code = 'LEGALADDRC'&#13;&#10;and exists (SELECT zvalue1 FROM tmp_table)" PropData="05704C65667403D8000470546F700264095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
