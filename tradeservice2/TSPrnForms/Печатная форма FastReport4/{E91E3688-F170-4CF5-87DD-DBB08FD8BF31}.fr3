<TfrxReport Version="4.6.30" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38491,4042748958" ReportOptions.Description.Text="" ReportOptions.LastChange="39465,5927989583" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  pRecCount : Integer;                                   &#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eOutDate.Date := &#60;dstOrder.&#34;OUTDATE&#34;&#62;;&#13;&#10;  eOutNo.Text   := VarToStr(&#60;dstOrder.&#34;ORDER_ID&#34;&#62;) + 'ВН';&#13;&#10;  eDateDov.Date := Now;           &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  pRecCount := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DetailDataOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  pRecCount := pRecCount + 1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo20OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dstStrFunc.Close;        &#13;&#10;  dstStrFunc.SetVariable( 'num', &#60;SUM(&#60;odsGItems.&#34;SUM_TOTAL&#34;&#62;, DetailData)&#62; );&#13;&#10;  dstStrFunc.Open;            &#13;&#10;  //dstStrFunc.ExecSQL;&#13;&#10;  //Memo20.Memo.Text := &#60;dstStrFunc.&#34;STRM&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo31OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;dstOrder.&#34;OT_CODE&#34;&#62; = 'GISSUE' then&#13;&#10;    Memo31.Memo.Text := 'Видаткова накладна № ' + VarToStr(&#60;dstOrder.&#34;SAPORDNUM&#34;&#62;)  &#13;&#10;  else&#13;&#10;    Memo31.Memo.Text := 'Видаткова накладна № ' + VarToStr(&#60;dstOrder.&#34;RECORDNUMBER&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo32OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dstDateStrFunc.Close;&#13;&#10;  if &#60;dstOrder.&#34;OT_CODE&#34;&#62; = 'GISSUE' then&#13;&#10;    dstDateStrFunc.SetVariable('value',&#60;dstOrder.&#34;SHIPDATE&#34;&#62;)&#13;&#10;  else&#13;&#10;    dstDateStrFunc.SetVariable('value',DateToStr(eOutDate.Date));&#13;&#10;  dstDateStrFunc.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo37OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsConsignee.&#34;ID&#34;&#62; &#60;&#62; '' then&#13;&#10;    Memo37.Memo.Text := VarToStr(&#60;odsConsignee.&#34;ADDRESS&#34;&#62;) + ' Тел. '+ VarToStr(&#60;odsConsignee.&#34;PHONE_A&#34;&#62;)  &#13;&#10;  else              &#13;&#10;    //Memo37.Memo.Text :=  &#60;odsFIO.&#34;NAME&#34;&#62;;&#13;&#10;    if ((&#60;dstOrder.&#34;OT_CODE&#34;&#62; = 'ZORDR') or (&#60;dstOrder.&#34;OT_CODE&#34;&#62; = 'ZPDS') and (&#60;odsFIO.&#34;NAME&#34;&#62; = '')) then&#13;&#10;      Memo37.Memo.Text := 'Роздрібний покупець'                 &#13;&#10;    else                &#13;&#10;      Memo37.Memo.Text := &#60;odsFIO.&#34;NAME&#34;&#62; ;         &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo38OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;dstOrder.&#34;OT_CODE&#34;&#62; = 'GISSUE' then&#13;&#10;    Memo38.Memo.Text := VarToStr(&#60;odsVENDOR.&#34;NAME_F&#34;&#62;)+', '+VarToStr(&#60;odsVENDOR.&#34;ADDRESS_F&#34;&#62;)                                                 &#13;&#10;  else          &#13;&#10;    if &#60;odsCustomer.&#34;NAME&#34;&#62; &#60;&#62; '' then&#13;&#10;      Memo38.Memo.Text := VarToStr(&#60;odsCustomer.&#34;NAME&#34;&#62;) + ' Тел. ' + VarToStr(&#60;odsCustomer.&#34;PHONE_A&#34;&#62;)&#13;&#10;    else&#13;&#10;      Memo38.Memo.Text := VarToStr(&#60;odsFIO.&#34;NAME&#34;&#62;) +   'адреса: ' + VarToStr(&#60;odsFIO.&#34;ADDRESS&#34;&#62;);                                                        &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
