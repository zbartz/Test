    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="select&#13;&#10;  g.good_code,&#13;&#10;  g.name,&#13;&#10;  (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;  gi.quantity as quantity,&#13;&#10;  round( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as price,&#13;&#10;  gi.quantity*round( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2) as sum,&#13;&#10;  gi.quantity*round( gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as sum_nds,&#13;&#10;  round( gi.quantity*gi.price2, 2 ) as sum_total&#13;&#10;from&#13;&#10;  good g, g_unit gu,&#13;&#10;  (select&#13;&#10;     ogi.g_unit_id,&#13;&#10;     ogi.quantity as quantity,&#13;&#10;     ogi.price2&#13;&#10;   from o_good_item ogi&#13;&#10;   where ogi.order_id = :order_id&#13;&#10;     and ogi.ignore &#60;&#62; 'Y'&#13;&#10;   order by ogi.og_item_id&#13;&#10;  ) gi&#13;&#10;where gu.g_unit_id = gi.g_unit_id&#13;&#10;  and gu.good_code = g.good_code" PropData="05704C656674023C0470546F70039C00095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
