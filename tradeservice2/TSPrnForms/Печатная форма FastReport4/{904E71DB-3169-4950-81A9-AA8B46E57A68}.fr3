    <TfrxDOAQuery Name="odsMain" UserName="odsMain" CloseDataSource="True" FieldAliases.Text="ORDER_ID=ORDER_ID&#13;&#10;FIO=FIO&#13;&#10;NAME=NAME&#13;&#10;NAMEALL=NAMEALL&#13;&#10;BRAND_MODEL=BRAND_MODEL&#13;&#10;BZPRICE=BZPRICE&#13;&#10;HANDDISCOUNT=HANDDISCOUNT&#13;&#10;PRICE2=PRICE2&#13;&#10;DOCDATE=DOCDATE&#13;&#10;AC_NUM=AC_NUM&#13;&#10;SERIALNUM=SERIALNUM&#13;&#10;DEFECTS_LIST=DEFECTS_LIST" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033313936360000000000" SQL.Text="SELECT ogi.order_id, &#13;&#10;       ov.opvalue AS fio,&#13;&#10;       g.NAME,&#13;&#10;       gf1.value AS nameall, &#13;&#10;       g.brand_code||' '||gf.VALUE AS brand_model,&#13;&#10;       --get_price_on_date(o.docdate, ogi.g_unit_id) AS bzprice,&#13;&#10;       ogi.price1 AS bzprice,&#13;&#10;       (select sum(oc.amount) from o_condition oc &#13;&#10;         where oc.order_id = o.order_id and &#13;&#10;               oc.og_item_id = ogi.og_item_id and&#13;&#10;               oc.sapcode in ('R02G','UC01','3%D','5%D','10%D','PCHG','P%CH','ACDP'))&#13;&#10;           as handdiscount,&#13;&#10;       ogi.price2, &#13;&#10;       o.docdate, ogi.ac_num,&#13;&#10;       (select max(d.defect) &#13;&#10;         from nsc_tmp_print_def d &#13;&#10;        where d.ac_num = ogi.ac_num &#13;&#10;          and d.deftype = 'SERIALNUM') as serialnum,&#13;&#10;       pk_discount.GetDefectsForPrint(ogi.ac_num) as defects_list&#13;&#10;  FROM orders o,&#13;&#10;       o_good_item ogi,&#13;&#10;       op_values ov,&#13;&#10;       g_unit gu,&#13;&#10;       good g,&#13;&#10;       g_feature gf, g_feature gf1&#13;&#10; WHERE o.order_id = :order_id&#13;&#10;   AND ogi.order_id = o.order_id&#13;&#10;  AND (ogi.storage_code like '%91' &#13;&#10;       or (ogi.storage_code like '%90' and &#13;&#10;           Exists(select * from o_condition oc &#13;&#10;                  where oc.order_id = o.order_id and &#13;&#10;                  oc.og_item_id = ogi.og_item_id and&#13;&#10;                  oc.sapcode in ('R02G'))))&#13;&#10;   AND ov.order_id (+) = o.order_id&#13;&#10;   AND ov.op_code (+) = 'FIO'&#13;&#10;   AND gu.g_unit_id = ogi.g_unit_id&#13;&#10;   AND gu.is_base = 'Y'&#13;&#10;   AND g.good_code = gu.good_code&#13;&#10;   AND gf.good_code (+) = g.good_code&#13;&#10;   AND gf.feature_code (+) = 'ZKODMOD'&#13;&#10;   AND gf1.good_code (+) = g.good_code&#13;&#10;   AND gf1.feature_code (+) = 'ZNAMEALL'" PropData="05704C65667402340470546F700234095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223331393636220000"/>
    <TfrxDOAQuery Name="dstLogo" UserName="dstLogo" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="SELECT&#13;&#10;  tsdf.* --df_value&#13;&#10;FROM&#13;&#10;  tsys_datafiles tsdf,&#13;&#10;  (SELECT wt.wbltype FROM w_type wt, werk w WHERE wt.wtype_code = w.wtype_code AND w.is_base = 'Y') wtc&#13;&#10;WHERE&#13;&#10;  tsdf.df_guid IN (&#13;&#10;  '{1F73DB37-363A-4A98-8846-E9CE9857B27C}',&#13;&#10;  '{031FE881-FC68-41A7-BAF6-E342B446448B}')&#13;&#10;  AND tsdf.df_note = wtc.wbltype" PropData="05704C65667403B4000470546F700234095661726961626C65730100"/>
    <TfrxDOAQuery Name="odsADDR" UserName="odsADDR" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;      pk_franch.get_werkname('ORDERS', :order_id, 'LEGALADDR') AS shop_name,&#13;&#10;      a.CITY || CASE WHEN NOT a.CITY IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.STREET || CASE WHEN NOT a.STREET IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      a.HOUSE_NUM || CASE WHEN NOT a.HOUSE_NUM IS NULL THEN ', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.house_corp IS NULL THEN 'êîðï. '||a.house_corp||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.BUILDING IS NULL THEN 'ñòð. '||a.BUILDING||', ' ELSE '' END ||&#13;&#10;      CASE WHEN NOT a.ROOM_NUMBER IS NULL THEN 'ê.' ELSE '' END || a.ROOM_NUMBER&#13;&#10;        AS shop_addr,&#13;&#10;      'ÈÍÍ '||abn.INN AS inn&#13;&#10;  FROM&#13;&#10;      address a, address au, addr_bninfo abn&#13;&#10; WHERE&#13;&#10;      a.address_id (+) = pk_franch.get_addr_id('ORDERS', :order_id, 'PHYSADDR') AND&#13;&#10;      au.address_id (+) = pk_franch.get_addr_id('ORDERS', :order_id, 'LEGALADDR') AND&#13;&#10;      abn.address_id (+) = au.address_id" PropData="05704C65667402740470546F700234095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
