    <TfrxDOAQuery Name="odsCustomer" UserName="odsCustomer" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  C.BASENAME AS NAME,&#13;&#10;    CASE WHEN a.REGION IS NULL THEN '' ELSE a.REGION || ', ' END ||&#13;&#10;    a.POST_CODE || &#13;&#10;    CASE WHEN a.POST_CODE IS NULL THEN '' ELSE ', ' END || &#13;&#10;    a.CITY || &#13;&#10;    CASE WHEN a.STREET IS NULL THEN '' ELSE ', ' END || &#13;&#10;    a.STREET || &#13;&#10;    CASE WHEN a.HOUSE_NUM IS NULL THEN '' ELSE ', д.' END || &#13;&#10;    a.HOUSE_NUM || &#13;&#10;    CASE WHEN a.HOUSE_CORP IS NULL THEN '' ELSE ', корп.' END || &#13;&#10;    a.HOUSE_CORP || &#13;&#10;    CASE WHEN a.BUILDING IS NULL THEN '' ELSE ', стр.' END || &#13;&#10;    a.BUILDING || CASE WHEN a.ROOM_NUMBER IS NULL THEN '' ELSE ', к.' END || &#13;&#10;    a.ROOM_NUMBER&#13;&#10;  AS ADDRESS,&#13;&#10;  C.INN,&#13;&#10;  C.KPP,&#13;&#10;  C.OKATO,&#13;&#10;  abn.*&#13;&#10;FROM&#13;&#10;  ADDRESS a,&#13;&#10;  ADDR_BNINFO abn,&#13;&#10;  CONTRAGENTS C,&#13;&#10;  O_CONTRAGENTS oc,&#13;&#10;  ORDERS o&#13;&#10;WHERE&#13;&#10;  o.ORDER_ID = :order_id&#13;&#10;  AND oc.ORDER_ID = o.ORDER_ID&#13;&#10;  AND oc.CNTTYPE_CODE = 'CASHLESS'&#13;&#10;  AND C.CONTRAGENT_ID = PK_PRINT.GET_ACTUAL_CONTRAGENT(oc.CONTRAGENT_ID)&#13;&#10;  AND a.ADDRESS_ID = C.ADDRESS_ID&#13;&#10;  AND abn.ADDRESS_ID (+) = C.ADDRESS_ID&#13;&#10;--  AND NOT EXISTS (SELECT zvalue1 FROM TMP_TABLE)" PropData="05704C65667403CC000470546F700234095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsOrder" UserName="odsOrder" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000313031333234390000000000" SQL.Text="select&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;  NVL((SELECT *&#13;&#10;        FROM&#13;&#10;              (select osl.operationdate&#13;&#10;               from o_status_log osl, orders o&#13;&#10;               where osl.order_id = :order_id&#13;&#10;                 and osl.os_code IN('ZBNAL_SHPD','ZPMNT_SALE', 'ZBNAL_CRTD', 'ZBNAL_PAYC')&#13;&#10;                 AND o.order_id = :order_id&#13;&#10;                 AND o.os_code = osl.os_code&#13;&#10;               order BY osl.osl_id DESC)&#13;&#10;        WHERE ROWNUM &#60;=1 ),&#13;&#10;       CASE WHEN o.ot_code in ('ZRTRN','GISSUE','GOODMV', 'ZBNAL') THEN o.docdate ELSE null END ) AS InvoiceDate,&#13;&#10;  pk_common.GetDocOutNum(o.parent_order_id,'SAPORDNUM') as SAPORDNUM,&#13;&#10;  o.*,&#13;&#10;  (select opv.opvalue from op_values opv&#13;&#10;                     where opv.order_id=o.parent_order_id&#13;&#10;                       and opv.op_code='SAPORDTYPE') AS SAPORDTYPE,&#13;&#10;  case when (SELECT count(*) FROM tmp_table)&#62;1 then 'Y' else 'N' end as multy  &#13;&#10;--NVL( (SELECT MAX('Y') FROM tmp_table), 'N' ) AS multy&#13;&#10;from orders o, staff s&#13;&#10;where o.order_id = :order_id&#13;&#10;  and o.createusercode = s.user_code(+)" PropData="05704C65667402380470546F700234095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2231303133323439220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;UNAME=UNAME&#13;&#10;NUM_CODE=NUM_CODE&#13;&#10;QUANTITY=QUANTITY&#13;&#10;QUANTITY_SAP=QUANTITY_SAP&#13;&#10;PRICE_SAP=PRICE_SAP&#13;&#10;PRICE=PRICE&#13;&#10;SUM_SAP=SUM_SAP&#13;&#10;SUM=SUM&#13;&#10;SUM_NDS_SAP=SUM_NDS_SAP&#13;&#10;SUM_NDS=SUM_NDS&#13;&#10;SUM_TOTAL_SAP=SUM_TOTAL_SAP&#13;&#10;SUM_TOTAL=SUM_TOTAL&#13;&#10;NDSTAXRATE_SAP=NDSTAXRATE_SAP&#13;&#10;NDSTAXRATE=NDSTAXRATE&#13;&#10;AC_NUM=AC_NUM" BCDToCurrency="False" Master="dstOrders" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A280000000300000001000000090000003A4F524445525F494405000000070000003235373437340000000000" SQL.Text="select g_o.good_code,&#13;&#10;       g_o.name,&#13;&#10;       (select short_name from unit_measure where um_code = g_o.unit_code) as uname,&#13;&#10;       (select num_code from unit_measure where um_code = g_o.unit_code) as num_code,&#13;&#10;       sum(g_o.quantity) as quantity ,  &#13;&#10;       sum(g_o.quantity_SAP) as quantity_SAP, &#13;&#10;       g_o.price_SAP as price_SAP /*Цена - Цена без НДС (PRICENONDS) (ZCP)*/,&#13;&#10;       round( g_o.price2 - g_o.price2*get_taxrate(g_o.good_code)/(100+get_taxrate (g_o.good_code)), 2 ) as price,&#13;&#10;       sum(g_o.sum_SAP) as sum_SAP  /*Сумма без учета НДС - Стоимость без НДС (COSTNONDS) (SPNET)*/,       &#13;&#10;       round(sum(g_o.quantity) * (g_o.price2 - g_o.price2*get_taxrate (g_o.good_code)/(100+get_taxrate (g_o.good_code))), 2 ) as sum,&#13;&#10;       sum(g_o.sum_nds_SAP) as sum_nds_SAP /*Сумма НДС - Сумма НДС (SUMNDS) (SMW01)*/,       &#13;&#10;       round(sum(g_o.quantity) * g_o.price2*get_taxrate (g_o.good_code)/(100+get_taxrate (g_o.good_code)), 2 ) as sum_nds,&#13;&#10;       sum(g_o.sum_total_SAP) as sum_total_SAP /*Сумма с учетом НДС - Стоимость с НДС (COSTNDS) (STOTAL)*/,       &#13;&#10;       round( sum(g_o.quantity)*g_o.price2, 2 ) as sum_total,&#13;&#10;       g_o.ndstaxrate_SAP /*Ст. НДС - Ставка НДС (NDS) (MWI1)*/,       &#13;&#10;       to_char(get_taxrate (g_o.good_code))||'%' as ndstaxrate,       &#13;&#10;       g_o.ac_num   &#13;&#10;from (select &#13;&#10;       g.good_code,&#13;&#10;       g.name,&#13;&#10;       gu.unit_code,&#13;&#10;--       ogi.quantity as quantity,  &#13;&#10;       ( case  when  (lower(l.country) like lower('%россия%')) or (ogil.quantity is null)  &#13;&#10;         then  ogi.quantity&#13;&#10;         else -- Разбиваем по ГТД партиям, но если партий не хватает, то в последнюю добавляем недостающий товар &#13;&#10;               decode(row_number () over (partition by ogi.g_unit_id, ogil.og_item_id order by ogil.og_item_id),&#13;&#10;               (select count (1) from ogi_lot where og_item_id = ogi.og_item_id ),&#13;&#10;               (ogil.quantity + (ogi.quantity - (select sum (quantity) from ogi_lot where og_item_id = ogi.og_item_id))),&#13;&#10;                ogil.quantity)&#13;&#10;          end)    as quantity, &#13;&#10;          &#13;&#10;        (case when lower(l.country) like '%россия%' then '' else l.lot_value &#13;&#10;                end) as gtd, &#13;&#10;&#13;&#10;       ogi.price2 as price2,&#13;&#10;       (select ogi_par.quantity &#13;&#10;        from  o_good_item ogi_par join  ogi_link ol on ol.og_parent_item = ogi_par.og_item_id&#13;&#10;        where ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID&#13;&#10;        ) as quantity_SAP, &#13;&#10;       get_op_values(o.PARENT_ORDER_ID, &#13;&#10;                     (select ol.og_parent_item from ogi_link ol, o_good_item ogi_par &#13;&#10;                      where ogi_par.og_item_id = ol.og_parent_item and ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID),&#13;&#10;                     'PRICENONDS') as price_SAP /*Цена - Цена без НДС (PRICENONDS) (ZCP)*/,&#13;&#10;       get_op_values(o.PARENT_ORDER_ID, &#13;&#10;                     (select ol.og_parent_item from ogi_link ol, o_good_item ogi_par &#13;&#10;                      where ogi_par.og_item_id = ol.og_parent_item and ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID),&#13;&#10;                     'COSTNONDS') as sum_SAP /*Сумма без учета НДС - Стоимость без НДС (COSTNONDS) (SPNET)*/,       &#13;&#10;       get_op_values(o.PARENT_ORDER_ID, &#13;&#10;                     (select ol.og_parent_item from ogi_link ol, o_good_item ogi_par &#13;&#10;                      where ogi_par.og_item_id = ol.og_parent_item and ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID),&#13;&#10;                     'SUMNDS') as sum_nds_SAP /*Сумма НДС - Сумма НДС (SUMNDS) (SMW01)*/,       &#13;&#10;       get_op_values(o.PARENT_ORDER_ID, &#13;&#10;                     (select ol.og_parent_item from ogi_link ol, o_good_item ogi_par &#13;&#10;                      where ogi_par.og_item_id = ol.og_parent_item and ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID),&#13;&#10;                     'COSTNDS') as sum_total_SAP /*Сумма с учетом НДС - Стоимость с НДС (COSTNDS) (STOTAL)*/,       &#13;&#10;       get_op_values(o.PARENT_ORDER_ID, &#13;&#10;                     (select ol.og_parent_item from ogi_link ol, o_good_item ogi_par &#13;&#10;                      where ogi_par.og_item_id = ol.og_parent_item and ogi_par.order_id = o.PARENT_ORDER_ID and ol.og_child_item = ogi.OG_ITEM_ID),&#13;&#10;                     'NDS') as ndstaxrate_SAP /*Ст. НДС - Ставка НДС (NDS) (MWI1)*/,       &#13;&#10;        nvl( ogi.ac_num,&#13;&#10;             pk_discount.GetACNumByOGI(ogi.og_item_id,ogi.g_unit_id,ogi.item_no)&#13;&#10;            ) as ac_num   &#13;&#10;      from o_good_item ogi&#13;&#10;       join orders o on o.order_id = ogi.order_id&#13;&#10;       join g_unit gu on gu.g_unit_id = ogi.g_unit_id&#13;&#10;       join good g on g.good_code = gu.good_code&#13;&#10;       left join ogi_lot ogil on ogil.og_item_id = ogi.og_item_id&#13;&#10;       left join lot l        on l.lot_id = ogil.lot_id&#13;&#10;      where ogi.order_id = :order_id /*257474--257507--269115--257466-- */&#13;&#10;       and ((o.ot_code = 'GISSUE') or (ogi.ignore &#60;&#62; 'Y'))&#13;&#10;       and ogi.quantity &#60;&#62; 0&#13;&#10;       and g.g_type_code &#60;&#62; 'DIEN'&#13;&#10;      ) g_o&#13;&#10;-- схлопывание строк с учетом по товарам с учетом гос. тамож. декларации(gtd)&#13;&#10;group by g_o.good_code,g_o.name,g_o.unit_code,g_o.price2,g_o.gtd, &#13;&#10;         g_o.price_SAP,ndstaxrate_SAP,g_o.ac_num    &#13;&#10;order by g_o.unit_code" PropData="05704C65667402370470546F7003B000095661726961626C657301010C490000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22323537343734220000"/>
    <TfrxDOAQuery Name="odsStrFunc" UserName="odsStrFunc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A310000000300000002000000040000003A4E554D050000000000000000000000050000003A4E554D4D050000000000000000000000" SQL.Text="select str_func.fn_integertostr(:num) as str&#13;&#10;       ,str_func.fn_moneytostr(:numm) as strm&#13;&#10;from dual" PropData="05704C65667403BC010470546F700234095661726961626C657301010C350000002044617461547970653D226674537472696E6722204E616D653D224E554D2220506172616D547970653D227074556E6B6E6F776E2200010C360000002044617461547970653D226674537472696E6722204E616D653D224E554D4D2220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsWerk" UserName="odsWerk" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  pk_franch.get_chief_names('ORDERS',:order_id,'DIRECTOR','LEGALADDRC') as director,&#13;&#10;  pk_franch.get_chief_names('ORDERS',:order_id,'CHIEFACC','LEGALADDRC') as chiefacc,&#13;&#10;  w.werk_code,&#13;&#10;  (select shortname from currency c where c.currency_code = w.currency_code) as shortname,&#13;&#10;  (select shortnamex from currency c where c.currency_code = w.currency_code) as shortnamex&#13;&#10;FROM&#13;&#10;  werk w&#13;&#10;WHERE w.is_base='Y'" PropData="05704C6566740380000470546F700234095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="dstCustomerF" UserName="dstCustomerF" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  a.namebase AS name,&#13;&#10;  CASE WHEN a.region IS NULL THEN '' ELSE a.region || ', ' END  ||&#13;&#10;  a.post_code ||&#13;&#10;  CASE WHEN a.post_code IS NULL THEN '' ELSE ', ' END || a.city ||&#13;&#10;  CASE WHEN a.street IS NULL THEN '' ELSE ', ' END || a.street ||&#13;&#10;  CASE WHEN a.house_num IS NULL THEN '' ELSE ', д.' END || a.house_num ||&#13;&#10;  CASE WHEN a.house_corp IS NULL THEN '' ELSE ', корп.' END || a.house_corp ||&#13;&#10;  CASE WHEN a.building IS NULL THEN '' ELSE ', стр.' END || a.building ||&#13;&#10;  CASE WHEN a.room_number IS NULL THEN '' ELSE ', к.' END || a.room_number&#13;&#10;  AS address,&#13;&#10;&#13;&#10;  CASE&#13;&#10;  WHEN NOT ac.value IS NULL THEN ', тел. '||ac.VALUE&#13;&#10;  ELSE ''&#13;&#10;  END AS phone,&#13;&#10;  'возврат товара заявление №          от '||to_char(o.docdate,'DD.MM.YYYY') AS osn,&#13;&#10;&#13;&#10;  abn.*&#13;&#10;FROM address a, addr_bninfo abn, orders o, addr_comm ac&#13;&#10;WHERE o.order_id = :order_id&#13;&#10;  AND a.address_id = o.address_id&#13;&#10;  AND ac.address_id(+) = a.address_id&#13;&#10;  AND ac.ct_code(+) = 'TEL'&#13;&#10;  and abn.address_id(+) = o.address_id&#13;&#10;--  AND NOT EXISTS(SELECT zvalue1 FROM tmp_table)" PropData="05704C65667403CC000470546F700274095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="dstOrders" UserName="dstOrders" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000313031333234390000000000" SQL.Text="SELECT T.order_id, T.os_code, T.ot_code FROM&#13;&#10;(&#13;&#10;SELECT order_id, os_code, ot_code&#13;&#10;FROM orders WHERE order_id = :order_id &#13;&#10;AND NOT EXISTS( SELECT zvalue1 FROM tmp_table)&#13;&#10;UNION ALL&#13;&#10;SELECT order_id, os_code, ot_code &#13;&#10;--AS order_id&#13;&#10;FROM orders o, tmp_table tt&#13;&#10;WHERE zvalue1 = o.order_id&#13;&#10;) T&#13;&#10;WHERE T.ot_code in ('ZRTRN','GISSUE', 'GOODMV') OR T.os_code IN ('ZBNAL_PAYD','ZBNAL_SHPD','ZPMNT_SALE', 'ZBNAL_CRTD', 'ZBNAL_PAYC')" PropData="05704C65667402380470546F700274095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2231303133323439220000"/>
    <TfrxDOAQuery Name="dstCustomerR" UserName="dstCustomerR" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A2600000003000000010000000E0000003A434F4E54524147454E545F4944050000000000000000000000" SQL.Text="SELECT&#13;&#10;  c.BASENAME AS NAME,&#13;&#10;    CASE WHEN a.REGION IS NULL THEN '' ELSE a.REGION || ', ' END ||&#13;&#10;    a.POST_CODE || &#13;&#10;    CASE WHEN a.POST_CODE IS NULL THEN '' ELSE ', ' END || &#13;&#10;    a.CITY || &#13;&#10;    CASE WHEN a.STREET IS NULL THEN '' ELSE ', ' END || &#13;&#10;    a.STREET || &#13;&#10;    CASE WHEN a.HOUSE_NUM IS NULL THEN '' ELSE ', д.' END || &#13;&#10;    a.HOUSE_NUM || &#13;&#10;    CASE WHEN a.HOUSE_CORP IS NULL THEN '' ELSE ', корп.' END || &#13;&#10;    a.HOUSE_CORP || &#13;&#10;    CASE WHEN a.BUILDING IS NULL THEN '' ELSE ', стр.' END || &#13;&#10;    a.BUILDING || &#13;&#10;    CASE WHEN a.ROOM_NUMBER IS NULL THEN '' ELSE ', к.' END || &#13;&#10;    a.ROOM_NUMBER &#13;&#10;  AS ADDRESS,&#13;&#10;  c.INN,&#13;&#10;  C.KPP,&#13;&#10;  C.OKATO,&#13;&#10;  abn.*&#13;&#10;FROM&#13;&#10;  ADDRESS a,&#13;&#10;  ADDR_BNINFO abn,&#13;&#10;  CONTRAGENTS C&#13;&#10;WHERE&#13;&#10;  C.CONTRAGENT_ID = PK_PRINT.GET_ACTUAL_CONTRAGENT(:contragent_id)&#13;&#10;  AND a.ADDRESS_ID = C.ADDRESS_ID&#13;&#10;  AND abn.ADDRESS_ID (+) = C.ADDRESS_ID" PropData="05704C65667403CC000470546F7003B000095661726961626C657301010C3F0000002044617461547970653D226674537472696E6722204E616D653D22434F4E54524147454E545F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsGoodmv" UserName="odsGoodmv" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="select&#13;&#10;--Получатель (юрадрес)&#13;&#10;  ain.namebase as name_in,&#13;&#10;  ain.post_code || case when not ain.post_code is null then ', ' else '' end ||&#13;&#10;  ain.region || case when not ain.region is null then ', ' else '' end ||&#13;&#10;  ain.city || case when not ain.city is null then ', ' else '' end ||&#13;&#10;  ain.street || case when not ain.street is null then ', ' else '' end ||&#13;&#10;  ain.house_num || case when not ain.house_num is null then ', ' else '' end ||&#13;&#10;  case when not ain.house_corp is null then 'корп. '||ain.house_corp||', ' else '' end ||&#13;&#10;  case when not ain.building is null then 'стр. '||ain.building||', ' else '' end ||&#13;&#10;  case when not ain.room_number is null then 'к.' else '' end || ain.room_number&#13;&#10;    as address_in,&#13;&#10;  abnin.bankname AS bankname_in,&#13;&#10;  abnin.inn AS inn_in,&#13;&#10;  abnin.kpp AS kpp_in,&#13;&#10;  abnin.okato AS okato_in,&#13;&#10;  abnin.bik AS bik_in,&#13;&#10;  abnin.sacnt AS sacnt_in,&#13;&#10;  abnin.okpo AS okpo_in,&#13;&#10;  abnin.koracnt AS koracnt_in,&#13;&#10;&#13;&#10;--Исх.склад (юрадрес)&#13;&#10;  aout.namebase as name_out,&#13;&#10;  aout.post_code || case when not aout.post_code is null then ', ' else '' end ||&#13;&#10;  aout.region || case when not aout.region is null then ', ' else '' end ||&#13;&#10;  aout.city || case when not aout.city is null then ', ' else '' end ||&#13;&#10;  aout.street || case when not aout.street is null then ', ' else '' end ||&#13;&#10;  aout.house_num || case when not aout.house_num is null then ', ' else '' end ||&#13;&#10;  case when not aout.house_corp is null then 'корп. '||aout.house_corp||', ' else '' end ||&#13;&#10;  case when not aout.building is null then 'стр. '||aout.building||', ' else '' end ||&#13;&#10;  case when not aout.room_number is null then 'к.' else '' end || aout.room_number&#13;&#10;    as address_out,&#13;&#10;  abnout.bankname AS bankname_out,&#13;&#10;  abnout.inn AS inn_out,&#13;&#10;  abnout.kpp AS kpp_out,&#13;&#10;  abnout.okato AS okato_out,&#13;&#10;  abnout.bik AS bik_out,&#13;&#10;  abnout.sacnt AS sacnt_out,&#13;&#10;  abnout.okpo AS okpo_out,&#13;&#10;  abnout.koracnt AS koracnt_out&#13;&#10;&#13;&#10;from address ain, addr_bninfo abnin, address aout, addr_bninfo abnout&#13;&#10;where ain.address_id =&#13;&#10;(select NVL(&#13;&#10;  (select wl.address_id from w_legals wl where wl.wl_id =&#13;&#10;    (select wl_id from storage where strg_id=&#13;&#10;      (select ogi.strg_id from o_good_item ogi where ogi.order_id=:order_id and rownum=1))),&#13;&#10;  (select wl.address_id from wp_link wl where wl.werk_code=get_werk_base and wl.wp_code='LEGALADDR')&#13;&#10;) from dual)&#13;&#10;and abnin.address_id(+) = ain.address_id&#13;&#10;and aout.address_id =&#13;&#10;(select NVL(&#13;&#10;  (select wl.address_id from w_legals wl where wl.wl_id =&#13;&#10;    (select wl_id from storage where strg_id=&#13;&#10;      (select o.strg_id from orders o where o.order_id=:order_id and rownum=1))),&#13;&#10;  (select wl.address_id from wp_link wl where wl.werk_code=get_werk_base and wl.wp_code='LEGALADDR')&#13;&#10;) from dual)&#13;&#10;and abnout.address_id(+) = aout.address_id" PropData="05704C6566740374010470546F700234095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsCustomerSel" UserName="odsCustomerSel" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="select c.contragent_id||' '||c.basename AS INVOICEPAY, upper(c.basename) as Name, rownum as rn&#13;&#10;  from contragents c, contragents_roles cr&#13;&#10; where cr.cnttype_code='INVOICEPAY'&#13;&#10;   and cr.contragent_id=c.contragent_id" PropData="05704C65667403CC000470546F7003F400095661726961626C65730100"/>
