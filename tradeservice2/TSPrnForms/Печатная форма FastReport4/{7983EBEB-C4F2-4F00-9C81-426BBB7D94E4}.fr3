in&#13;&#10;  (*Сумма с учетом НДС*)&#13;&#10;  if VarToStr(&#60;odsGItems.&#34;price_SAP&#34;&#62;) &#60;&#62; '' then begin&#13;&#10;    if  VarToStr(&#60;odsGItems.&#34;quantity&#34;&#62;) = VarToStr(&#60;odsGItems.&#34;quantity_SAP&#34;&#62;) then begin&#13;&#10;      v_sum_total_SAP := &#60;odsGItems.&#34;sum_total_SAP&#34;&#62;;                                  &#13;&#10;    end              &#13;&#10;    else begin                           &#13;&#10;      v_sum_total_SAP := v_sum_SAP+v_sum_SAP*v_ndstaxrate_SAP/100;&#13;&#10;    end;&#13;&#10;      v_sum_total_ITOGO := v_sum_total_ITOGO+v_sum_total_SAP;          &#13;&#10;      memSum_total.Memo.Text := FormatFloat('### ### ##0.00р',v_sum_total_SAP);&#13;&#10;  end;//if VarToStr(&#60;odsGItems.&#34;price_SAP&#34;&#62;) &#60;&#62; ''                  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Page3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  v_sum_total_ITOGO := 0;&#13;&#10;  v_sum_nds_ITOGO := 0;&#13;&#10;  v_sum_ITOGO := 0;                    &#13;&#10;end; &#13;&#10;&#13;&#10;procedure Memo100OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if VarToStr(&#60;odsGItems.&#34;price_SAP&#34;&#62;) &#60;&#62; '' then begin           &#13;&#10;     Memo100.Memo.Text := FormatFloat('### ### ##0.00р', v_sum_total_ITOGO );&#13;&#10;   end;               &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo25OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;odsGItems.&#34;price_SAP&#34;&#62;) &#60;&#62; '' then begin      &#13;&#10;    Memo25.Memo.Text :=  FormatFloat('### ### ##0.00р', v_sum_ITOGO );&#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportOnRunDialogs(var Result: Boolean);&#13;&#10;begin&#13;&#10;  odsOrder.Open;&#13;&#10;  Result:=(odsOrder.FieldByName('OT_CODE').AsString &#60;&#62; 'ZBNAL');  &#13;&#10;  if not Result then&#13;&#10;    begin&#13;&#10;      odsGItems.Open;                        &#13;&#10;      odsGift.SetVariable('order_id', odsOrder.FieldByName('order_id').AsString);&#13;&#10;      odsGift.Open;&#13;&#10;      Result:=odsGift.FieldByName('GiftCount').AsInteger = odsGItems.RecordCount;&#13;&#10;      odsGift  .Close;&#13;&#10;      odsGItems.Close;                      &#13;&#10;    end;                &#13;&#10;  odsOrder.Close;&#13;&#10;&#13;&#10;  if not Result then&#13;&#10;    MessageDlg('Заказ содержит подарочные карты, документы ТОРГ-12 и Счет-фактура на продажу ПК не выдаются.', mtWarning, mbOk, 0)&#13;&#10;  else &#13;&#10;    Result := Page2.ShowModal = mrOK;&#13;&#10;end;&#13;&#10;  &#13;&#10;procedure Memo105OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  &#13;&#10;  if eDovNum.Text &#60;&#62; '' then begin             &#13;&#10;    Memo105.Memo.Text := FORMATDATETIME('DD.MM.YYYY',eDovDate.Date);  &#13;&#10;  end;&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  mApprovedBy_position.memo.text := eApprovedBy_position.Text;&#13;&#10;  mApprovedBy_person.memo.text := eApprovedBy_person.Text;&#13;&#10;  mChiefAccountant.memo.text := eChiefAccountant.Text;                                                                                                              &#13;&#10;  mGivenOutBy_position.memo.text := eGivenoutBy_position.Text;&#13;&#10;  if (eGivenoutBy_person.Text &#60;&#62; '') then                        &#13;&#10;    mGivenOutBy_person.memo.text:= eGivenoutBy_person.Text&#13;&#10;  else                 &#13;&#10;    if &#60;odsOrder.&#34;OT_CODE&#34;&#62; = 'ZRTRN' then&#13;&#10;      mGivenOutBy_person.memo.text := &#60;dstCustomerF.&#34;NAME&#34;&#62;&#13;&#10;    else                      &#13;&#10;      mGivenOutBy_person.memo.text := '';                 &#13;&#10;end;&#13;&#10;&#13;&#10;begin          &#13;&#10;end." StoreInDFM="False" OnRunDialogs="ReportOnRunDialogs" PropData="">
