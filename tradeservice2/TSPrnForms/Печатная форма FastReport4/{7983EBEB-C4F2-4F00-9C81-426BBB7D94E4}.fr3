en 'корп. '||a_f.house_corp||', ' else '' end ||&#13;&#10;  case when not a_f.building is null then 'стр. '||a_f.building||', ' else '' end ||&#13;&#10;  case when not a_f.room_number is null then 'к.' else '' end || a_f.room_number&#13;&#10;    as address_f,&#13;&#10;&#13;&#10;  abnf.bankname AS bankname_f,&#13;&#10;  abnf.inn AS inn_f,&#13;&#10;  abnf.kpp AS kpp_f,&#13;&#10;  abnf.okato AS okato_f,&#13;&#10;  abnf.bik AS bik_f,&#13;&#10;  abnf.sacnt AS sacnt_f,&#13;&#10;  abnf.okpo AS okpo_f,&#13;&#10;  abnf.koracnt AS koracnt_f&#13;&#10;&#13;&#10;FROM&#13;&#10;  address aub, address a_f, addr_bninfo abn, addr_bninfo abnf&#13;&#10;WHERE aub.address_id = pk_franch.get_addr_id('ORDERS',:order_id,'LEGALADDRC')&#13;&#10;  AND abn.address_id(+) = aub.address_id&#13;&#10;  AND a_f.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'PHYSADDR')&#13;&#10;  AND abnf.address_id(+) = a_f.address_id&#13;&#10;  AND NOT EXISTS(SELECT zvalue1 FROM tmp_table)&#13;&#10;UNION ALL&#13;&#10;SELECT&#13;&#10;  aub.namebase as name_ub,&#13;&#10;  aub.post_code || case when not aub.post_code is null then ', ' else '' end ||&#13;&#10;  aub.region || case when not aub.region is null then ', ' else '' end ||&#13;&#10;  aub.city || case when not aub.city is null then ', ' else '' end ||&#13;&#10;  aub.street || case when not aub.street is null then ', ' else '' end ||&#13;&#10;  aub.house_num || case when not aub.house_num is null then ', ' else '' end ||&#13;&#10;  case when not aub.house_corp is null then 'корп. '||aub.house_corp||', ' else '' end ||&#13;&#10;  case when not aub.building is null then 'стр. '||aub.building||', ' else '' end ||&#13;&#10;  case when not aub.room_number is null then 'к.' else '' end || aub.room_number&#13;&#10;    as address_ub,&#13;&#10;&#13;&#10;  abn.bankname,&#13;&#10;  abn.inn,&#13;&#10;  abn.kpp,&#13;&#10;  abn.okato,&#13;&#10;  abn.bik,&#13;&#10;  abn.sacnt,&#13;&#10;  abn.okpo,&#13;&#10;  abn.koracnt AS koracnt,&#13;&#10;&#13;&#10;  a_f.namebase as name_f,&#13;&#10;  a_f.post_code || case when not a_f.post_code is null then ', ' else '' end ||&#13;&#10;  a_f.region || case when not a_f.region is null then ', ' else '' end ||&#13;&#10;  a_f.city || case when not a_f.city is null then ', ' else '' end ||&#13;&#10;  a_f.street || case when not a_f.street is null then ', ' else '' end ||&#13;&#10;  a_f.house_num || case when not a_f.house_num is null then ', ' else '' end ||&#13;&#10;  case when not a_f.house_corp is null then 'корп. '||a_f.house_corp||', ' else '' end ||&#13;&#10;  case when not a_f.building is null then 'стр. '||a_f.building||', ' else '' end ||&#13;&#10;  case when not a_f.room_number is null then 'к.' else '' end || a_f.room_number&#13;&#10;    as address_f,&#13;&#10;&#13;&#10;  abnf.bankname AS bankname_f,&#13;&#10;  abnf.inn AS inn_f,&#13;&#10;  abnf.kpp AS kpp_f,&#13;&#10;  abnf.okato AS okato_f,&#13;&#10;  abnf.bik AS bik_f,&#13;&#10;  abnf.sacnt AS sacnt_f,&#13;&#10;  abnf.okpo AS okpo_f,&#13;&#10;  abnf.koracnt AS koracnt_f&#13;&#10;&#13;&#10;FROM&#13;&#10;  address aub, address a_f, addr_bninfo abn, addr_bninfo abnf&#13;&#10;WHERE aub.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'LEGALADDRC')&#13;&#10;  AND abn.address_id(+) = aub.address_id&#13;&#10;  AND a_f.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'PHYSADDR')&#13;&#10;  AND abnf.address_id(+) = a_f.address_id&#13;&#10;  AND EXISTS(SELECT zvalue1 FROM tmp_table)" PropData="05704C6566740380000470546F700274095661726