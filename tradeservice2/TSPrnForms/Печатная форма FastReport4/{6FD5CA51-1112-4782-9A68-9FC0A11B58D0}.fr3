    <TfrxDOAQuery Name="odsVENDOR" UserName="odsVENDOR" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000343933383131380000000000" SQL.Text="SELECT&#13;&#10;  --aub.namebase as name_ub,&#13;&#10;  pk_franch.get_werkname('ORDERS',:order_id,'LEGALADDR')  as name_ub,&#13;&#10;  aub.post_code || case when not aub.post_code is null then ', ' else '' end ||&#13;&#10;  aub.region || case when not aub.region is null then ', ' else '' end ||&#13;&#10;  aub.city || case when not aub.city is null then ', ' else '' end ||&#13;&#10;  aub.street || case when not aub.street is null then ', ' else '' end ||&#13;&#10;  aub.house_num || case when not aub.house_num is null then ', ' else '' end ||&#13;&#10;  case when not aub.house_corp is null then 'корп. '||aub.house_corp||', ' else '' end ||&#13;&#10;  case when not aub.building is null then 'стр. '||aub.building||', ' else '' end ||&#13;&#10;  case when not aub.room_number is null then 'к.' else '' end || aub.room_number&#13;&#10;    as address_ub,&#13;&#10;&#13;&#10;  --a_f.namebase as name_f,&#13;&#10;  pk_franch.get_werkname('ORDERS',:order_id,'PHYSADDR') as name_f,&#13;&#10;  a_f.post_code || case when not a_f.post_code is null then ', ' else '' end ||&#13;&#10;  a_f.region || case when not a_f.region is null then ', ' else '' end ||&#13;&#10;  a_f.city || case when not a_f.city is null then ', ' else '' end ||&#13;&#10;  a_f.street || case when not a_f.street is null then ', ' else '' end ||&#13;&#10;  a_f.house_num || case when not a_f.house_num is null then ', ' else '' end ||&#13;&#10;  case when not a_f.house_corp is null then 'корп. '||a_f.house_corp||', ' else '' end ||&#13;&#10;  case when not a_f.building is null then 'стр. '||a_f.building||', ' else '' end ||&#13;&#10;  case when not a_f.room_number is null then 'к.' else '' end || a_f.room_number&#13;&#10;    as address_f,&#13;&#10;&#13;&#10;  abn.inn,&#13;&#10;  abn.kpp,&#13;&#10;  abn.okato&#13;&#10;FROM&#13;&#10;  address aub, address a_f, addr_bninfo abn&#13;&#10;WHERE aub.address_id = pk_franch.get_addr_id('ORDERS',:order_id,'LEGALADDR')&#13;&#10;  AND abn.address_id(+) = aub.address_id&#13;&#10;  AND a_f.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'PHYSADDR')&#13;&#10;  and not exists (SELECT zvalue1 FROM tmp_table)&#13;&#10;union all&#13;&#10;SELECT&#13;&#10;  aub.namebase as name_ub,&#13;&#10;  aub.post_code || case when not aub.post_code is null then ', ' else '' end ||&#13;&#10;  aub.region || case when not aub.region is null then ', ' else '' end ||&#13;&#10;  aub.city || case when not aub.city is null then ', ' else '' end ||&#13;&#10;  aub.street || case when not aub.street is null then ', ' else '' end ||&#13;&#10;  aub.house_num || case when not aub.house_num is null then ', ' else '' end ||&#13;&#10;  case when not aub.house_corp is null then 'корп. '||aub.house_corp||', ' else '' end ||&#13;&#10;  case when not aub.building is null then 'стр. '||aub.building||', ' else '' end ||&#13;&#10;  case when not aub.room_number is null then 'к.' else '' end || aub.room_number&#13;&#10;    as address_ub,&#13;&#10;&#13;&#10;  a_f.namebase as name_f,&#13;&#10;  a_f.post_code || case when not a_f.post_code is null then ', ' else '' end ||&#13;&#10;  a_f.region || case when not a_f.region is null then ', ' else '' end ||&#13;&#10;  a_f.city || case when not a_f.city is null then ', ' else '' end ||&#13;&#10;  a_f.street || case when not a_f.street is null then ', ' else '' end ||&#13;&#10;  a_f.house_num || case when not a_f.house_num is null then ', ' else '' end ||&#13;&#10;  case when not a_f.house_corp is null then 'корп. '||a_f.house_corp||', ' else '' end ||&#13;&#10;  case when not a_f.building is null then 'стр. '||a_f.building||', ' else '' end ||&#13;&#10;  case when not a_f.room_number is null then 'к.' else '' end || a_f.room_number&#13;&#10;    as address_f,&#13;&#10;&#13;&#10;  abn.inn,&#13;&#10;  abn.kpp,&#13;&#10;  abn.okato&#13;&#10;FROM &#13;&#10;  address aub, address a_f, addr_bninfo abn&#13;&#10;WHERE aub.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'LEGALADDR')&#13;&#10;  AND abn.address_id(+) = aub.address_id&#13;&#10;  AND a_f.address_id = (select address_id from werk w,wp_link wp&#13;&#10;                        where is_base='Y'&#13;&#10;                          and w.werk_code = wp.werk_code&#13;&#10;                          and wp.wp_code = 'PHYSADDR')&#13;&#10;  and exists (SELECT zvalue1 FROM tmp_table)" PropData="05704C6566740384000470546F70026C095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2234393338313138220000"/>
    <TfrxDOAQuery Name="odsCustomer" UserName="odsCustomer" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000343933383131380000000000" SQL.Text="select&#13;&#10;  c.basename as name,&#13;&#10;  case when not a.region is null then a.region || ', ' end&#13;&#10;  || case when not a.post_code is null then a.post_code || ', ' end&#13;&#10;  || a.city&#13;&#10;  || ', '&#13;&#10;  || a.street&#13;&#10;  || ', д.'&#13;&#10;  || a.house_num&#13;&#10;  || case when not a.house_corp is null then ', корп.' || a.house_corp end&#13;&#10;  || case when not a.building is null then ', стр.' || a.building end&#13;&#10;  || case when not a.room_number is null then ', к.' || a.room_number end as address,&#13;&#10;  c.inn,&#13;&#10;  c.kpp,&#13;&#10;  c.okato&#13;&#10;from address a, addr_bninfo abn, contragents c, o_contragents oc, orders o&#13;&#10;where o.order_id = :order_id&#13;&#10;  and oc.order_id = o.order_id&#13;&#10;  and oc.cnttype_code = 'CASHLESS'&#13;&#10;  and oc.contragent_id = c.contragent_id&#13;&#10;  and a.address_id = c.address_id&#13;&#10;  and abn.address_id(+) = o.address_id&#13;&#10;--  and not exists (SELECT zvalue1 FROM tmp_table)" PropData="05704C6566740314010470546F700238095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2234393338313138220000"/>
    <TfrxDOAQuery Name="odsOrder" UserName="odsOrder" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000343933383131380000000000" SQL.Text="select&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;  nvl((select operationdate&#13;&#10;       from o_status_log&#13;&#10;       where order_id = :order_id&#13;&#10;         and os_code IN('ZBNAL_SHPD','ZPMNT_SALE')), o.docdate) as InvoiceDate,&#13;&#10;  nvl((select max(opvalue) from op_values opv&#13;&#10;       where opv.order_id = :order_id&#13;&#10;         and op_code = 'INVOICENUM'), 0 ) as InvoiceNo,&#13;&#10;  o.*,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCDAT') as PayDocDat,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCNUM') as PayDocNum,&#13;&#10;  case when (SELECT count(*) FROM tmp_table)&#62;1 then 'Y' else 'N' end as multy&#13;&#10;  --NVL( (SELECT MAX('Y') FROM tmp_table), 'N' ) as multy&#13;&#10;from orders o,staff s&#13;&#10;where o.order_id = :order_id&#13;&#10;  and o.createusercode = s.user_code(+)" PropData="05704C65667402380470546F700238095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2234393338313138220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;UNAME=UNAME&#13;&#10;NUM_CODE=NUM_CODE&#13;&#10;QUANTITY=QUANTITY&#13;&#10;PRICE=PRICE&#13;&#10;SUM=SUM&#13;&#10;SUM_NDS=SUM_NDS&#13;&#10;SUM_TOTAL=SUM_TOTAL&#13;&#10;NDSTAXRATE=NDSTAXRATE&#13;&#10;GTD=GTD&#13;&#10;COUNTRY=COUNTRY&#13;&#10;COUNTRY_ISO=COUNTRY_ISO&#13;&#10;COUNTRY_NAME=COUNTRY_NAME" BCDToCurrency="False" Master="dstOrders" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A280000000300000001000000090000003A4F524445525F494405000000070000003235373437340000000000" SQL.Text="select g.good_code,&#13;&#10;       g.name,&#13;&#10;       (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;       (select num_code from unit_measure where um_code = gu.unit_code) as num_code,&#13;&#10;       gi.quantity as quantity,&#13;&#10;       round(gi.price2 - gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2) as price,&#13;&#10;       round(gi.quantity * ( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code))), 2) as sum,&#13;&#10;       round(gi.quantity *  gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2) as sum_nds,&#13;&#10;       round(gi.quantity * gi.price2, 2) as sum_total,&#13;&#10;       to_char(get_taxrate (g.good_code))||'%' as ndstaxrate,&#13;&#10;       gi.gtd,&#13;&#10;       gi.country, &#13;&#10;       gi.country_iso,&#13;&#10;       gi.country_name&#13;&#10;  from good g &#13;&#10;  join g_unit gu on gu.good_code = g.good_code&#13;&#10;  join (&#13;&#10;     select gi_in.g_unit_id,  sum(gi_in.quantity) as quantity, gi_in.price2,  gi_in.gtd, gi_in.country, gi_in.country_iso, gi_in.country_name&#13;&#10;     from(&#13;&#10;           select  &#13;&#10;               ogi.g_unit_id,&#13;&#10;               ( case  when  (lower(l.country) like lower('%россия%')) or (ogil.quantity is null)  &#13;&#10;                 then  ogi.quantity&#13;&#10;                 else -- Разбиваем по ГТД партиям, но если партий не хватает, то в последнюю добавляем недостающий товар &#13;&#10;                    decode(row_number () over (partition by ogi.g_unit_id, ogil.og_item_id order by ogil.og_item_id),&#13;&#10;                          (select count (1) from ogi_lot where og_item_id = ogi.og_item_id ),&#13;&#10;                          (ogil.quantity + (ogi.quantity - (select sum (quantity) from ogi_lot where og_item_id = ogi.og_item_id))),&#13;&#10;                           ogil.quantity)&#13;&#10;                  end)    as quantity, &#13;&#10;               ogi.price2,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else l.lot_value &#13;&#10;                end) as gtd,&#13;&#10;               l.country,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else&#13;&#10;                 (select ISO from country c where lower(c.country_name) = lower(l.country) ) &#13;&#10;                end) as country_iso,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else&#13;&#10;                 (select COUNTRY_NAME from country c where lower(c.country_name) = lower(l.country) ) &#13;&#10;                end) as country_name               &#13;&#10;          from o_good_item ogi&#13;&#10;          left join ogi_lot ogil on  ogil.og_item_id = ogi.og_item_id&#13;&#10;          left join lot l        on l.lot_id = ogil.lot_id&#13;&#10;         where 1=1&#13;&#10;           and ogi.order_id = :order_id -- 257474--5041583--&#13;&#10;           and ogi.ignore &#60;&#62; 'Y'&#13;&#10;         ) gi_in&#13;&#10;       group by gi_in.g_unit_id,  gi_in.price2,  gi_in.gtd, gi_in.country, gi_in.country_iso, gi_in.country_name&#13;&#10;  ) gi&#13;&#10;  on  gi.g_unit_id = gu.g_unit_id &#13;&#10; where gi.quantity &#60;&#62; 0&#13;&#10; order by gi.g_unit_id" PropData="05704C65667402380470546F7003A300095661726961626C657301010C490000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22323537343734220000"/>
    <TfrxDOAQuery Name="odsWerk" UserName="odsWerk" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000343933383131380000000000" SQL.Text="select (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = pk_franch.get_chief_id('ORDERS',:order_id,'DIRECTOR','LEGALADDRC')&#13;&#10;               ) as director,&#13;&#10;       (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = pk_franch.get_chief_id('ORDERS',:order_id,'CHIEFACC','LEGALADDRC')&#13;&#10;               ) as chiefacc,&#13;&#10;       TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'INVOICENUM', 'ZBNAL', 'N' ) maxinvoiceno,&#13;&#10;       w.werk_code&#13;&#10;  from werk w&#13;&#10; where w.is_base = 'Y'&#13;&#10;   and not exists (SELECT zvalue1 FROM tmp_table)&#13;&#10;union all&#13;&#10;select (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = wpl.director_id&#13;&#10;               ) as director,&#13;&#10;       (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = wpl.chiefacc_id&#13;&#10;               ) as chiefacc,&#13;&#10;       TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'INVOICENUM', 'ZBNAL', 'N' ) maxinvoiceno,&#13;&#10;       w.werk_code&#13;&#10;  from werk w, wp_link wpl&#13;&#10; where w.is_base = 'Y'&#13;&#10;   and wpl.werk_code = w.werk_code&#13;&#10;   and wpl.wp_code = 'LEGALADDRC'&#13;&#10;   and exists (SELECT zvalue1 FROM tmp_table)" PropData="05704C6566740384000470546F700238095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2234393338313138220000"/>
    <TfrxDOAQuery Name="odsInvoice" UserName="odsInvoice" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A5800000003000000030000000D0000003A4E4557494E564F4943454E4F0500000000000000000000000A0000003A494E564F4943454E4F050000000000000000000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="begin&#13;&#10;  if :newinvoiceno = 'Y' then&#13;&#10;    :invoiceno := TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'INVOICENUM', 'ZBNAL', 'Y' );&#13;&#10;  end if;&#13;&#10;&#13;&#10;  update op_values&#13;&#10;     set opvalue = to_number(:invoiceno)&#13;&#10;   where order_id = :order_id&#13;&#10;     and op_code = 'INVOICENUM';&#13;&#10;&#13;&#10;  if sql%rowcount = 0 then&#13;&#10;    insert into op_values(order_id, og_item_id, op_code, opvalue)&#13;&#10;    values (:order_id, null, 'INVOICENUM', to_number(:invoiceno));&#13;&#10;  end if;&#13;&#10;&#13;&#10;  commit;&#13;&#10;end;" PropData=""/>
