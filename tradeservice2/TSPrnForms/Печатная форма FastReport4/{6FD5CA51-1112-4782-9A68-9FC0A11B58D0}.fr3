<TfrxReport Version="4.12.15" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38073,6220618981" ReportOptions.Description.Text="" ReportOptions.LastChange="42997,6581554861" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  InvoiceNo, NewInvoiceNo, id : String;&#13;&#10;  bChange : boolean;                                     &#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bChange := False;                                 &#13;&#10;  InvoiceNo    := &#60;odsOrder.&#34;INVOICENO&#34;&#62;;&#13;&#10;  NewInvoiceNo := 'N';&#13;&#10;  eRegDate.Date := Date;                          &#13;&#10;    &#13;&#10;  if InvoiceNo = '0' then&#13;&#10;  begin&#13;&#10;    InvoiceNo    := &#60;odsWerk.&#34;MAXINVOICENO&#34;&#62;;&#13;&#10;   // NewInvoiceNo := 'Y';&#13;&#10;  end;&#13;&#10;&#13;&#10;  edtInvoiceNum.Text := InvoiceNo;&#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62; = 'N' then&#13;&#10;  begin&#13;&#10;    edtInvoiceNum.Enabled := False;                                 &#13;&#10;  &#13;&#10;    rbReg.Enabled    := False;&#13;&#10;    rbCnt.Enabled    := False;&#13;&#10;    rbReg.Checked    := False;&#13;&#10;    rbCnt.Checked    := False;&#13;&#10;    eRegNum.Enabled  := False;&#13;&#10;    eRegDate.Enabled := False;&#13;&#10;&#13;&#10;    eCustomerNum.Enabled  := False;&#13;&#10;    eConsigneeNum.Enabled := False;&#13;&#10;&#13;&#10;    lReg.Enabled          := False;&#13;&#10;    lRegDate.Enabled      := False;&#13;&#10;    lCntCustomer.Enabled  := False;&#13;&#10;    lCntConsignee.Enabled := False;&#13;&#10;  end&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      rbReg.Enabled    := True;&#13;&#10;      rbCnt.Enabled    := True;&#13;&#10;      rbReg.Checked    := True;&#13;&#10;    end;&#13;&#10;&#13;&#10;  ePayDocDate.text := &#60;odsOrder.&#34;PAYDOCDAT&