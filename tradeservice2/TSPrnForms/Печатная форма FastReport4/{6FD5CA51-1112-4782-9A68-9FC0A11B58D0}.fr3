;&#62;='') then&#13;&#10;        begin&#13;&#10;          Memo126.Lines.Add('Не полностью заполнены наименование и адрес грузополучателя');                          &#13;&#10;          DialogPage1.Visible := True;  &#13;&#10;        end;                  &#13;&#10;    end&#13;&#10;  else              &#13;&#10;    DialogPage1.Visible := False;&#13;&#10;&#13;&#10;  memo27.memo.text := ePayDocDate.text;&#13;&#10;  memo55.memo.text := eChiefFIO.text;&#13;&#10;  memo59.memo.text := eIP_FIO.text;&#13;&#10;  memo66.memo.text := eChiefAcc_FIO.text;&#13;&#10;    &#13;&#10;  bChange := Change_Print.Checked;        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbRegOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eRegNum.Enabled  := True;&#13;&#10;  eRegDate.Enabled := True;&#13;&#10;  eCustomerNum.Enabled  := False;&#13;&#10;  eConsigneeNum.Enabled := False;&#13;&#10;&#13;&#10;  lReg.Enabled          := True;&#13;&#10;  lRegDate.Enabled      := True;&#13;&#10;  lCntCustomer.Enabled  := False;&#13;&#10;  lCntConsignee.Enabled := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbCntOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eRegNum.Enabled  := False;&#13;&#10;  eRegDate.Enabled := False;&#13;&#10;  eCustomerNum.Enabled  := True;&#13;&#10;  eConsigneeNum.Enabled := True;&#13;&#10;&#13;&#10;  lReg.Enabled          := False;&#13;&#10;  lRegDate.Enabled      := False;&#13;&#10;  lCntCustomer.Enabled  := True;&#13;&#10;  lCntConsignee.Enabled := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;TOTALPAGES&#62; &#62; 1&#13;&#10;    then PageFooter1.Visible := True;                                         &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo63OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i : integer;                                 &#13;&#10;begin&#13;&#10;  Ins_IDOC.SetVariable('DOCID', VarToStr(&#60;odsOrder.&#34;ORDER_ID&#34;&#62;));&#13;&#10;  Ins_IDOC.SetVariable('CORID', '');      &#13;&#10;        &#13;&#10;  for i:=0 to 8-Length(InvoiceNo) do&#13;&#10;  begin&#13;&#10;    InvoiceNo := '0'+InvoiceNo;                        &#13;&#10;  end;&#13;&#10;&#13;&#10;  Ins_IDOC.SetVariable('EXT_ORIG', VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo);&#13;&#10;  Ins_IDOC.SetVariable('EXT_CORR', '');      &#13;&#10;        &#13;&#10;    &#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62;= 'Y' then&#13;&#10;    Memo63.Memo.Text := 'СЧЁТ-ФАКТУРА № '+VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo+' от '+DateToStr(eRegDate.Date)  &#13;&#10;  else&#13;&#10;    Memo63.Memo.Text := 'СЧЁТ-ФАКТУРА № '+VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo+' от '+VarToStr(&#60;odsOrder.&#34;InvoiceDate&#34;&#62;);&#13;&#10;&#13;&#10;  Ins_IDOC.SetVariable('DATE_ORIG', VarToStr(&#60;odsOrder.&#34;InvoiceDate&#34;&#62;));&#13;&#10;  Ins_IDOC.SetVariable('DATE_CORR', '');&#13;&#10;  Ins_IDOC.SetVariable('LOCNR', VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;));      &#13;&#10;        &#13;&#10;        &#13;&#10;          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo75OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;TOTALPAGES&#62; &#62; 1 then&#13;&#10;    Memo75.Visible := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo105OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsGItems.&#34;NDSTAXRATE&#34;&#62; = '0%' then&#13;&#10;    Memo105.Memo.Text := 'без НДС'&#13;&#10;  else&#13;&#10;    Memo105.Memo.Text := &#60;odsGItems.&#34;NDSTAXRATE&#34;&#62;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo106OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsGItems.&#34;SUM_NDS&#34;&#62; = '0,00' then &#13;&#10;    Memo106.Memo.Text := 'без НДС'&#13;&#10;  else&#13;&#10;    Memo106.Memo.Text := FormatFloat('### ### ##0.00р', &#60;odsGItems.&#34;SUM_NDS&#34;&#62;);       &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo19OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then begin          &#13;&#10;    Memo19.Memo.Text := &#60;dstCustomerR.&#34;NAME&#34;&#62;;&#13;&#10;    Ins_IDOC.SetVariable('NAME', &#60;dstCustomerR.&#34;NAME&#34;&#62;);        &#13;&#10;  end            &#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo19.Memo.Text := 'по реестру № '+eRegNum.Text+' от '+FormatDateTime('DD.MM.YYYY',eRegDate.Date)&#13;&#10;  else begin             &#13;&#10;    Memo19.Memo.Text := &#60;odsCustomer.&#34;NAME&#34;&#62;;&#13;&#10;    Ins_IDOC.SetVariable('NAME', &#60;odsCustomer.&#34;NAME&#34;&#62;);        &#13;&#10;  end;                &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo21OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then&#13;&#10;    Memo21.Memo.Text := &#60;dstCustomerR.&#34;ADDRESS&#34;&#62;&#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo21.Memo.Text := ''&#13;&#10;  else&#13;&#10;    Memo21.Memo.Text := &#60;odsCustomer.&#34;ADDRESS&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo22OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then begin          &#13;&#10;    Memo22.Memo.Text := VarToStr(&#60;dstCustomerR.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;dstCustomerR.&#34;KPP&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('INN', VarToStr(&#60;dstCustomerR.&#34;INN&#34;&#62;));&#13;&#10;    Ins_IDOC.SetVariable('KPP', VarToStr(&#60;dstCustomerR.&#34;KPP&#34;&#62;));        &#13;&#10;  end            &#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo22.Memo.Text := ''&#13;&#10;  else begin             &#13;&#10;    Memo22.Memo.Text := VarToStr(&#60;odsCustomer.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;odsCustomer.&#34;KPP&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('INN', VarToStr(&#60;odsCustomer.&#34;INN&#34;&#62;));&#13;&#10;    Ins_IDOC.SetVariable('KPP', VarToStr(&#60;odsCustomer.&#34;KPP&#34;&#62;));        &#13;&#10;  end;              &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo15OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsVENDOR.&#34;NAME_F&#34;&#62; &#60;&#62; '' then&#13;&#10;    Memo15.Memo.Text := VarToStr(&#60;odsVENDOR.&#34;NAME_F&#34;&#62;) + ', ' + VarToStr(&#60;odsVENDOR.&#34;ADDRESS_F&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo17OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then&#13;&#10;    Memo17.Memo.Text := VarToStr(&#60;dstConsigneeR.&#34;NAME&#34;&#62;) + ' ' + VarToStr(&#60;dstConsigneeR.&#34;ADDRESS&#34;&#62;)  &#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo17.Memo.Text := 'по реестру № '+eRegNum.Text+' от '+FormatDateTime('DD.MM.YYYY',eRegDate.Date)&#13;&#10;  else&#13;&#10;    Memo17.Memo.Text := VarToStr(&#60;odsConsignee.&#34;NAME&#34;&#62;) + ' ' + VarToStr(&#60;odsConsignee.&#34;ADDRESS&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bChange := Change_Print.Checked;                            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;// Щетинин                  &#13;&#10;// При печати исправления создаем IDOC на выгрузку в САП                                                                                                                                                                                                                                      &#13;&#10;                            &#13;&#10;  if bChange then begin&#13;&#10;    Ins_IDOC.SetVariable('order_id', &#60;odsOrder.&#34;ORDER_ID&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('REVNO', &#60;odsCng.&#34;INVCNGNUM&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('DATE_REV', &#60;odsCng.&#34;INVCNGDATE&#34;&#62;);        &#13;&#10;        &#13;&#10;    Ins_IDOC.ExecSQL;                        &#13;&#10;  end;            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo28OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;odsCng.&#34;INVCNGNUM&#34;&#62;) &#60;&#62; '' then&#13;&#10;    Memo28.Memo.Text := 'ИСПРАВЛЕНИЕ   № ' + VarToStr(&#60;odsCng.&#34;INVCNGNUM&#34;&#62;)+ ' от ' + VarToStr(&#60;odsCng.&#34;INVCNGDATE&#34;&#62;)&#13;&#10;  else&#13;&#10;    Memo28.Memo.Text := 'ИСПРАВЛЕНИЕ   № - от &#34;-&#34; -';                        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if bChange then begin&#13;&#10;    qryINVCNGNUM_NEXT.SetVariable('order_id', &#60;odsOrder.&#34;ORDER_ID&#34;&#62;);&#13;&#10;    qryINVCNGNUM_NEXT.ExecSQL;          &#13;&#10;  end;          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage2OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Change_Print.Enabled := VarToStr(&#60;dstOrders.&#34;CHNG&#34;&#62;) = 'Y';                                &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportOnRunDialogs(var Result: Boolean);&#13;&#10;begin&#13;&#10;  odsOrder.Open;&#13;&#10;  Result:=(odsOrder.FieldByName('OT_CODE').AsString &#60;&#62; 'ZBNAL');  &#13;&#10;  if not Result then&#13;&#10;    begin&#13;&#10;      odsGItems.Open;                        &#13;&#10;      odsGift.SetVariable('order_id', odsOrder.FieldByName('order_id').AsString);&#13;&#10;      odsGift.Open;&#13;&#10;      Result:=odsGift.FieldByName('GiftCount').AsInteger = odsGItems.RecordCount;&#13;&#10;      odsGift  .Close;&#13;&#10;      odsGItems.Close;                      &#13;&#10;    end;                &#13;&#10;  odsOrder.Close;&#13;&#10;&#13;&#10;  if not Result then&#13;&#10;    MessageDlg('Заказ содержит подарочные карты, документы ТОРГ-12 и Счет-фактура на продажу ПК не выдаются.', mtWarning, mbOk, 0)&#13;&#10;  else &#13;&#10;    Result := Page1.ShowModal = mrOK;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo103OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo25.Memo.Text := FormatFloat('### ### ##0.00р', &#60;odsGItems.&#34;SUM&#34;&#62;)&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnRunDialogs="ReportOnRunDialogs" PropData="">
