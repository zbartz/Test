    <TfrxDOAQuery Name="odsUtils" UserName="odsUtils" CloseDataSource="True" FieldAliases.Text="OPVALUE=OPVALUE&#13;&#10;TG_NAME=TG_NAME&#13;&#10;QNT=QNT" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033373533330000000000" SQL.Text="/*&#13;&#10;SELECT ov.opvalue, tg.tg_name, count(1) as qnt&#13;&#10;FROM op_values ov, trash_groups tg, orders o&#13;&#10;where o.order_id = :order_id&#13;&#10;and ov.order_id = o.parent_order_id&#13;&#10;and ov.op_code = 'UTIL_GROUP'&#13;&#10;and ov.opvalue like tg.tg_name||'%'&#13;&#10;group by ov.opvalue, tg.tg_name &#13;&#10;*/&#13;&#10;select distinct tg_name,opvalue, qnt from&#13;&#10;(&#13;&#10;SELECT distinct tg.tg_name,opvalue, count(1) as qnt&#13;&#10;FROM op_values ov&#13;&#10;left join o_good_item ogi on ov.og_item_id = ogi.og_item_id, trash_groups tg&#13;&#10;where ov.order_id = :order_id&#13;&#10;and ogi.entrycode in (&#13;&#10;                      select sg.good_code&#13;&#10;                      from specgoods sg &#13;&#10;                      left join spectype st on st.st_code = sg.st_code &#13;&#10;                      where st.st_code = 'TRASHD')&#13;&#10;and op_code = 'UTIL_GROUP'&#13;&#10;and (opvalue like tg.tg_name||'%' or opvalue = tg.tg_code)&#13;&#10;and &#13;&#10;(select count(*) as cnt from op_values where order_id=:order_id&#13;&#10;and op_code='UTIL_STAT' and opvalue='E0002')=0 and ov.og_item_id is not null&#13;&#10;group by opvalue, tg.tg_name&#13;&#10;union all&#13;&#10;SELECT distinct tg.tg_name,opvalue, count(1) as qnt&#13;&#10;FROM op_values ov&#13;&#10;left join o_good_item ogi on ov.og_item_id = ogi.og_item_id, trash_groups tg&#13;&#10;where ov.order_id in&#13;&#10;(select order_id from orders where chainnum in (select chainnum from orders where order_id=:order_id))&#13;&#10;and ogi.entrycode in (&#13;&#10;                      select sg.good_code&#13;&#10;                      from specgoods sg &#13;&#10;                      left join spectype st on st.st_code = sg.st_code &#13;&#10;                      where st.st_code = 'TRASHD')&#13;&#10;and op_code = 'UTIL_GROUP'&#13;&#10;and (opvalue like tg.tg_name||'%' or opvalue = tg.tg_code)&#13;&#10;and &#13;&#10;(select count(*) as cnt from op_values where order_id in&#13;&#10;(select order_id from orders where chainnum in (select chainnum from orders where order_id=:order_id))&#13;&#10;and op_code='UTIL_STAT' and opvalue='E0002')=0 and ov.og_item_id is not null&#13;&#10;group by opvalue, tg.tg_name)" PropData="05704C65667403FC000470546F700260095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223337353333220000"/>
