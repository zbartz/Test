    <TfrxDOAQuery Name="odsMain" UserName="odsMain" CloseDataSource="True" FieldAliases.Text="FULLNAME=FULLNAME&#13;&#10;OPER_P_STAFF_ID=OPER_P_STAFF_ID&#13;&#10;RKSN_AMOUN=RKSN_AMOUN&#13;&#10;RANG=RANG" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A3C00000003000000020000000B0000003A424547494E5F44415445050000000000000000000000090000003A454E445F44415445050000000000000000000000" SQL.Text="select  i.fullname, i.oper_p_staff_id,&#13;&#10;  CASE WHEN i.use_rksn = 1 THEN &#13;&#10;    i.oper_amount * (i.rksn +1) ELSE i.oper_amount END as rksn_amoun,&#13;&#10;  i.RANG as RANG     &#13;&#10;  from &#13;&#10;       (SELECT h.order_id, h.oper_date, h.order_usercode, h.SUM_SIGN,                &#13;&#10;               i.og_item_id, i.oper_code, t.oper_name, s.fullname, &#13;&#10;               i.p_staff_id AS oper_p_staff_id,&#13;&#10;               i.amount AS oper_amount,&#13;&#10;               PK_BONUS_FUND.GET_KOEF_VALUE('K_USERKSN', null, NULL, NULL, h.oper_date, 1) as use_rksn,&#13;&#10;               PK_BONUS_FUND.GET_KOEF_VALUE('K_RKSN', null, NULL, NULL,  h.oper_date, 1) as rksn,               &#13;&#10;               pr.RANG as RANG&#13;&#10;          FROM bf_oper_head h&#13;&#10;          JOIN bf_oper_item i&#13;&#10;            ON i.operh_id = h.operh_id&#13;&#10;          JOIN bf_oper_type t&#13;&#10;            ON t.oper_code = i.oper_code&#13;&#10;          JOIN p_rang pr&#13;&#10;            ON pr.ID = PK_STAFF.GET_P_STAFF_HIST_VAL(i.p_staff_id, 'TRANG', h.oper_date)&#13;&#10;          LEFT JOIN p_staff s&#13;&#10;            ON s.id = i.p_staff_id&#13;&#10;         WHERE h.oper_date &#62; to_date(:begin_date,'YYYYMMDD') and h.oper_date &#60;= to_date(:end_date,'YYYYMMDD') &#13;&#10;       ) i &#13;&#10; where i.oper_code in ('PB_GOOD','PB_ACC','PB_FLP','PB_SBERCARD','PB_KREDIT',&#13;&#10;      'PB_PDS','PB_MENTOR','PB_LU','PB_MBL','PB_ACTION')" PropData=""/>
