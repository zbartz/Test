    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="ORDER_ID=ORDER_ID&#13;&#10;GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;UNAME=UNAME&#13;&#10;UNUM_CODE=UNUM_CODE&#13;&#10;BEFQNT=BEFQNT&#13;&#10;QUANTITY=QUANTITY&#13;&#10;NEWQUANTITY=NEWQUANTITY&#13;&#10;PRICE=PRICE&#13;&#10;FULLPRICE=FULLPRICE&#13;&#10;SUM=SUM&#13;&#10;SUM_NDS=SUM_NDS&#13;&#10;SUM_TOTAL=SUM_TOTAL&#13;&#10;NDSTAXRATE=NDSTAXRATE&#13;&#10;TAXRATE=TAXRATE" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033353633390000000000" SQL.Text="select ogi.order_id,&#13;&#10;	g.good_code,&#13;&#10;       g.name,&#13;&#10;       (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;       (select num_code   from unit_measure where um_code = gu.unit_code) as unum_code,&#13;&#10;       ogi2.quantity as BefQnt,&#13;&#10;	ogi.quantity as quantity,&#13;&#10;       pk_reject.GetZRTRNRemainQnt(:order_id) as NewQuantity,&#13;&#10;       round( ogi.price2-ogi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as price,&#13;&#10;       ogi.price2-ogi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)) as fullprice,   &#13;&#10;       round(ogi.quantity * ( ogi.price2-ogi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code))), 2) as sum,&#13;&#10;	round(ogi.quantity *  ogi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as sum_nds,&#13;&#10;       round( ogi.quantity*ogi.price2, 2 ) as sum_total,&#13;&#10;       to_char(get_taxrate (g.good_code))||'%' as ndstaxrate,&#13;&#10;	get_taxrate (g.good_code) TaxRate&#13;&#10;  from good g, g_unit gu,&#13;&#10;       o_good_item ogi, o_good_item ogi2, ogi_link ol&#13;&#10; where gu.g_unit_id = ogi.g_unit_id&#13;&#10;   and ogi2.order_id = :order_id&#13;&#10;   and gu.go