    <TfrxDOAQuery Name="odsSumStr" UserName="odsSumStr" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT t1.good_code, t1.name, t1.quantity, t1.price2,t1.lsum,t1.ndssum,t1.strgname&#13;&#10;       ,sum(lsum)  over () as total_sum &#13;&#10;       ,sum(ndssum)over () as total_nds&#13;&#10;       ,STR_FUNC.FN_MONEYTOSTR(sum(ndssum)over (), NULL,&#13;&#10;                               c.shortname||'.', c.shortnamex||'.' ) total_STR&#13;&#10;FROM&#13;&#10;(SELECT t.good_code, t.name, t.quantity, t.price2,&#13;&#10;       to_char (t.quantity * t.price2, '999999990D00')  as lsum,&#13;&#10;       to_char ( round( t.quantity&#13;&#10;                * t.price2&#13;&#10;                * (  get_taxrate (t.good_code)&#13;&#10;                   / (100 + get_taxrate (t.good_code))),2),&#13;&#10;                '999999990D00')&#13;&#10;                                            as ndssum,&#13;&#10;         t.strgname&#13;&#10;         &#13;&#10;from&#13;&#10;(&#13;&#10;   select g.good_code, g.name, SUM(ogi.quantity) as quantity,&#13;&#10;         to_char (ogi.price2, '999999990D00')  as price2,         &#13;&#10;         nvl (st.shortname, 'Ð½Ðµ ÑƒÐºÐ°Ð·Ð°Ð½') as strgname,&#13;&#10;         MIN(og_item_id) as og_item_id_min&#13;&#10;   from  orders o&#13;&#10;         join o_good_item ogi on ogi.order_id = o.order_id and ogi.ignore &#60;&#62; 'Y'&#13;&#10;         join g_unit gu       on gu.g_unit_id = ogi.g_unit_id&#13;&#10;         join good g          on gu.good_code = g.good_code&#13;&#10;         left join storage st on st.storage_code = ogi.storage_code &#13;&#10;                             and st.werk_code = ogi.werk_code&#13;&#10;   where o.order_id = :order_id&#13;&#10;   group by g.good_code, g.name, st.shortname, price2) t&#13;&#10;order by t.og_item_id_min&#13;&#10;) t1&#13;&#10;join werk w     on w.is_base = 'Y'&#13;&#10;join currency c on c.currency_code = w.currency_code" PropData="05704C6566740330010470546F700240095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
