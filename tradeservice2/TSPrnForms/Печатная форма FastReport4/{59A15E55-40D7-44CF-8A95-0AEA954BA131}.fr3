    <TfrxDOAQuery Name="odsWerk" UserName="odsWerk" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A430000000300000002000000090000003A4F524445525F49440500000006000000333633343100000000000C0000003A56454E444F525F54595045050000000000000000000000" SQL.Text="SELECT&#13;&#10;  (SELECT&#13;&#10;     ps.family&#13;&#10;     || ' '&#13;&#10;     || substr (ps.fname, 0, 1)&#13;&#10;     || '.'&#13;&#10;     || substr (ps.lname, 0, 1)&#13;&#10;     || '.'&#13;&#10;   FROM p_staff ps&#13;&#10;   WHERE ps.id=pk_franch.get_chief_id('ORDERS',:order_id,'DIRECTOR',:VENDOR_TYPE)&#13;&#10;  ) AS director,&#13;&#10;  (SELECT&#13;&#10;     ps.family&#13;&#10;     || ' '&#13;&#10;     || substr (ps.fname, 0, 1)&#13;&#10;     || '.'&#13;&#10;     || substr (ps.lname, 0, 1)&#13;&#10;     || '.'&#13;&#10;   FROM p_staff ps&#13;&#10;   WHERE ps.id=pk_franch.get_chief_id('ORDERS',:order_id,'CHIEFACC',:VENDOR_TYPE)&#13;&#10;  ) AS chiefacc,&#13;&#10;  w.werk_code,&#13;&#10;  pms.mvalue AS note,&#13;&#10;  (SELECT shortname FROM currency c WHERE c.currency_code = w.currency_code) AS shortname,&#13;&#10;  (SELECT shortnamex FROM currency c WHERE c.currency_code = w.currency_code) AS shortnamex&#13;&#10;FROM werk w, prn_macrosubst pms&#13;&#10;WHERE w.is_base = 'Y'&#13;&#10;  AND pms.mcode = 'BNAL_NOTE'" PropData=""/>
