    <TfrxDOAQuery Name="dstWerk" UserName="dstWerk" CloseDataSource="True" FieldAliases.Text="" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="SELECT&#13;&#10;  pk_franch.for_price_list('NAMEBASE') AS namebase,&#13;&#10;  pk_franch.for_price_list('FULLNAME') AS director,&#13;&#10;  (SELECT MAX(zvalue3) FROM tmp_table) AS price_DT,&#13;&#10;  (SELECT MAX(zvalue5) FROM tmp_table) AS price_name,&#13;&#10;  (SELECT shortname FROM currency c WHERE c.currency_code = w.currency_code) AS shortname,&#13;&#10;  (SELECT shortnamex FROM currency c WHERE c.currency_code = w.currency_code) AS shortnamex&#13;&#10;FROM&#13;&#10;  werk w&#13;&#10;WHERE&#13;&#10;  w.is_base = 'Y'" PropData="05704C65667402700470546F700234095661726961626C65730100"/>
