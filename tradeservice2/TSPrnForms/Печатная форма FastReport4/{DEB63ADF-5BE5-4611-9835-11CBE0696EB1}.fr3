    <TfrxDOAQuery Name="dstMain" UserName="dstMain" CloseDataSource="True" FieldAliases.Text="" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="SELECT /*+RULE*/&#13;&#10;       gm.mecat || ' ' || gm.name as gm_name,&#13;&#10;       g.good_code,&#13;&#10;       g.name as good_name,&#13;&#10;       tt.zvalue2 AS price&#13;&#10;  FROM g_mecat gm, good g, g_unit gu , tmp_table tt&#13;&#10; WHERE gu.g_unit_id = tt.zvalue1&#13;&#10;   AND g.good_code = GU.good_code&#13;&#10;   AND g.mecat = gm.mecat&#13;&#10; ORDER BY g.mecat, tt.zvalue4" PropData="05704C65667402340470546F700234095661726961626C65730100"/>
