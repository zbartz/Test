    <TfrxDOAQuery Name="adrWerk" UserName="adrWerk" CloseDataSource="True" FieldAliases.Text="WERK_CODE=WERK_CODE&#13;&#10;FULL_FIS_ADRESS=FULL_FIS_ADRESS&#13;&#10;FULL_U_ADRESS=FULL_U_ADRESS&#13;&#10;WERK_NAME_F=WERK_NAME_F&#13;&#10;WERK_CODE_1=WERK_CODE_1" BCDToCurrency="False" Master="dstMainMain" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT WERK_CODE, werk_name_f||address_f||case when addr_from_TTN.phone_ub is not null then ' òåë. ' end ||phone_ub&#13;&#10;||case when addr_from_TTN.fax_f is not null then ' ôàêñ ' end||fax_f as full_fis_adress,&#13;&#10;--âçÿò òåëåôîí þð.ëèöà, ò.ê. òåëåôîí ôèç ëèöà - ñïðàâî÷íàÿ&#13;&#10;werk_name_u||address_u || case when addr_from_TTN.phone_ub is not null then ' òåë. ' end ||phone_ub &#13;&#10;||case when addr_from_TTN.fax_ub is not null then ' ôàêñ ' end||fax_ub AS full_u_adress,&#13;&#10;werk_name_f, werk_code&#13;&#10;FROM&#13;&#10;--äàëåå âçÿòà ñòàíäàðòíàÿ âûáîðêà èç ÒÒÍ&#13;&#10;(SELECT&#13;&#10;  w.werk_code AS werk_code,&#13;&#10;  --(select a.namebase from address a where a.address_id = abnl.address_id) AS werk_name_u,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'LEGALADDR')  AS werk_name_u,&#13;&#10;  TS_INFO.getaddressstring(abnl.address_id) AS address_u,&#13;&#10;  --TS_INFO.getwerkname( w.werk_code, 'PHYSADDR' ) AS werk_name_f,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'PHYSADDR') AS werk_name_f,&#13;&#10;  TS_INFO.getaddressstring(abnf.address_id, 'ZCSHBFRA') AS address_f,&#13;&#10;    TS_INFO.GetPhoneString(abnf.address_id, 'TEL') as phone_f,&#13;&#10;  TS_INFO.GetPhoneString(abnf.address_id, 'FAX') as fax_f,&#13;&#10;  --TS_INFO.getwerkname( w.werk_code, 'LEGALADDRC' ) AS werk_name_ub,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'LEGALADDRC')  AS werk_name_ub,&#13;&#10;  TS_INFO.getaddressstring(abnb.address_id, 'ZCSHBFRA') AS address_ub,&#13;&#10;  TS_INFO.GetPhoneString(abnb.address_id, 'TEL') as phone_ub,&#13;&#10;  TS_INFO.GetPhoneString(abnb.address_id, 'FAX') as fax_ub,&#13;&#10;  abnl.bankname as bankname_l,&#13;&#10;  abnl.inn as inn_l,&#13;&#10;  abnl.kpp as kpp_1,&#13;&#10;  abnl.bik as bik_l,&#13;&#10;  abnl.sacnt as sacnt_l,&#13;&#10;  abnl.okpo as okpo_l,&#13;&#10;  abnl.koracnt as koracnt_l,&#13;&#10;  abnf.bankname as bankname_f,&#13;&#10;  abnf.inn as inn_f,&#13;&#10;  abnf.kpp as kpp_f,&#13;&#10;  abnf.bik as bik_f,&#13;&#10;  abnf.sacnt as sacnt_f,&#13;&#10;  abnf.okpo as okpo_f,&#13;&#10;  abnf.koracnt as koracnt_f,&#13;&#10;  abnb.bankname as bankname_b,&#13;&#10;  abnb.inn as inn_b,&#13;&#10;  abnb.kpp as kpp_b,&#13;&#10;  abnb.bik as bik_b,&#13;&#10;  abnb.sacnt as sacnt_b,&#13;&#10;  abnb.okpo as okpo_b,&#13;&#10;  abnb.koracnt as koracnt_b,&#13;&#10;  pk_franch.get_chief_names('ORDERS',o.order_id,'DIRECTOR','LEGALADDRC') AS director,&#13;&#10;  pk_franch.get_chief_names('ORDERS',o.order_id,'CHIEFACC','LEGALADDRC') AS chiefacc&#13;&#10;FROM werk w, addr_bninfo abnl, addr_bninfo abnf, addr_bninfo abnb,&#13;&#10;     orders o&#13;&#10;WHERE w.is_base = 'Y'&#13;&#10;  AND o.order_id = :order_id&#13;&#10;  AND abnl.address_id(+) = pk_franch.get_addr_id('ORDERS',o.order_id,'LEGALADDR')&#13;&#10;  AND abnf.address_id(+) = pk_franch.get_addr_id('ORDERS',o.order_id,'PHYSADDR')&#13;&#10;  AND abnb.address_id(+) = pk_franch.get_addr_id('ORDERS',o.order_id,'LEGALADDRC')&#13;&#10;  AND NOT EXISTS (SELECT zvalue1 FROM tmp_table)&#13;&#10;UNION ALL&#13;&#10;SELECT&#13;&#10; w.werk_code AS werk_code,&#13;&#10;  (select a.namebase from address a where a.address_id = abnl.address_id) AS werk_name_u,&#13;&#10;  TS_INFO.getaddressstring(abnl.address_id) AS address_u,&#13;&#10;  TS_INFO.getwerkname( w.werk_code, 'PHYSADDR' ) AS werk_name_f,&#13;&#10;  TS_INFO.getaddressstring(abnf.address_id, 'ZCSHBFRA') AS address_f,&#13;&#10;     TS_INFO.GetPhoneString(abnf.address_id, 'TEL'