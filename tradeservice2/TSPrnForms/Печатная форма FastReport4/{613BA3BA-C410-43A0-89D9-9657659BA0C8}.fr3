    <TfrxDOAQuery Name="dstMain" UserName="dstMain" CloseDataSource="True" FieldAliases.Text="ROWNUM=ROWNUM&#13;&#10;ORDER_ID=ORDER_ID&#13;&#10;OG_ITEM_ID=OG_ITEM_ID&#13;&#10;STORAGE_CODE=STORAGE_CODE&#13;&#10;DOCDATE=DOCDATE&#13;&#10;GOOD_NAME=GOOD_NAME&#13;&#10;GOOD_CODE=GOOD_CODE&#13;&#10;AC_NUM=AC_NUM&#13;&#10;UMNAME=UMNAME&#13;&#10;QUANTITY=QUANTITY&#13;&#10;SALEPRICE=SALEPRICE&#13;&#10;TOTALPRICE=TOTALPRICE&#13;&#10;WEIGHT=WEIGHT&#13;&#10;INTWEIGHT=INTWEIGHT&#13;&#10;BRAND=BRAND&#13;&#10;SERNUM=SERNUM&#13;&#10;DEFECTS=DEFECTS&#13;&#10;OUTERVIEW=OUTERVIEW&#13;&#10;COMPLECT=COMPLECT&#13;&#10;CUSTOMER=CUSTOMER&#13;&#10;SALEDATE=SALEDATE" BCDToCurrency="False" Master="dstMainMain" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A250000000300000001000000090000003A4F524445525F494403000000040000008489000000000000" SQL.Text="SELECT&#13;&#10;      rownum, &#13;&#10;      T.order_id AS ORDER_ID,&#13;&#10;T.OG_ITEM_ID,&#13;&#10;T.STORAGE_CODE,&#13;&#10;&#13;&#10;      T.docdate as DOCDATE,&#13;&#10;      T.good_name AS good_name, --Íàèìåíîâàíèå&#13;&#10;      T.good_code AS good_code,--Êîä &#13;&#10;      T.ac_num AS ac_num, --íîìåð ÊÒ&#13;&#10;      T.umname AS umname,--åäþèçìåðåíèÿ&#13;&#10;      T.quantity AS quantity,--êîëè÷åñòâî&#13;&#10;decode(      &#13;&#10;(select count(*) FROM good g1 WHERE g1.good_code =t.good_code and g1.mecat in ('502040101', '522040101')),&#13;&#10;0, T.saleprice,&#13;&#10;(&#13;&#10;select max(gp1.absval) from orders o1 &#13;&#10;join o_good_item ogi1 on o1.order_id = ogi1.order_id&#13;&#10;join g_unit gu1 on gu1.g_unit_id = ogi1.g_unit_id&#13;&#10;join g_price gp1 on gp1.g_unit_id = gu1.g_unit_id and gp1.ct_code = 'ZROZ' where o1.order_id = t.order_id&#13;&#10;)&#13;&#10;) as saleprice,&#13;&#10;decode(      &#13;&#10;(select count(*) FROM good g1 WHERE g1.good_code =t.good_code and g1.mecat in ('502040101', '522040101')),&#13;&#10;0,TO_NUMBER(T.quantity)*TO_NUMBER(T.saleprice),&#1