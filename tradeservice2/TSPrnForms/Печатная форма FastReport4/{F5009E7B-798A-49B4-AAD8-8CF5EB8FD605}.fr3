    <TfrxDOAQuery Name="odsOrder" UserName="odsOrder" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033313936360000000000" SQL.Text="select&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;  nvl((select operationdate&#13;&#10;       from o_status_log&#13;&#10;       where order_id = :order_id&#13;&#10;         and os_code IN('ZBNAL_SHPD','ZPMNT_SALE')), o.docdate) as InvoiceDate,&#13;&#10;  nvl((select max(opvalue) from op_values opv&#13;&#10;       where opv.order_id = :order_id&#13;&#10;         and op_code = 'INVOICENUM'), 0 ) as InvoiceNo,&#13;&#10;  o.*,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCDAT') as PayDocDat,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCNUM') as PayDocNum,&#13;&#10;  case when (SELECT count(*) FROM tmp_table)&#62;1 then 'Y' else 'N' end as multy&#13;&#10;from orders o,staff s&#13;&#10;where o.order_id = :order_id&#13;&#10;  and o.createusercode = s.user_code(+)" PropData="05704C65667402380470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223331393636220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;UNAME=UNAME&#13;&#10;QUANTITY=QUANTITY&#13;&#10;PRICE=PRICE&#13;&#10;SUM=SUM&#13;&#10;SUM_NDS=SUM_NDS&#13;&#10;SUM_TOTAL=SUM_TOTAL&#13;&#10;NDSTAXRATE=NDSTAXRATE&#13;&#10;GTD=GTD&#13;&#10;COUNTRY=COUNTRY" BCDToCurrency="False" Master="dstOrders" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="select g.good_code,&#13;&#10;       g.name,&#13;&#10;       (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;       gi.quantity as quantity,&#13;&#10;       round( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as price,&#13;&#10;       round(gi.quantity * ( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code))), 2) as sum,&#13;&#10;       round(gi.quantity *  gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as sum_nds,&#13;&#10;       round( gi.quantity*gi.price2, 2 ) as sum_total,&#13;&#10;       to_char(get_taxrate (g.good_code))||'%' as ndstaxrate,&#13;&#10;       gi.gtd,&#13;&#10;       gi.country&#13;&#10;  from good g, g_unit gu,&#13;&#10;       (select ogi.g_unit_id,&#13;&#10;               -- Åñëè íåò ÃÒÄ òî êîë-âî áåðåì èç òîâ. ïîçèöèè&#13;&#10;               -- Åñëè ÃÒÄ åñòü òî êîãäà ïîëó÷àåì ïîñëåäíþþ ïîçèöèþ ïî ÃÒÄ - äîáàâëÿåì&#13;&#10;               -- îñòàòîê îò íåðàñïðåäåëåííîãî êîë-âà, äëÿ íå ïîñëåäíåé ïîçèöèè ïî ÃÒÄ&#13;&#10;               -- áåðåì êîë-âî ïî òàáëèöå ðàñïðåäåëåíèÿ&#13;&#10;               nvl&#13;&#10;                   (decode(row_number () over (partition by ogil.og_item_id order by ogil.og_item_id),&#13;&#10;                          (select count (*) from ogi_lot where og_item_id = ogi.og_item_id), &#13;&#10;                          ogil.quantity + (ogi.quantity - (select sum (quantity) from ogi_lot where og_item_id = ogi.og_item_id)),&#13;&#10;                          ogil.quantity),&#13;&#10;                   ogi.quantity) &#13;&#10;                 as quantity,&#13;&#10;               ogi.price2,&#13;&#10;               (case when lower(l.country) like '%ðîññèÿ%' then '' else l.lot_value end) &#13;&#10;                 as gtd,&#13;&#10;               l.country&#13;&#10;          from lot l, ogi_lot ogil, o_good_item ogi&#13;&#10;         where ogi.order_id = :order_id&#13;&#10;           and ogil.og_item_id(+) = ogi.og_item_id&#13;&#10;           and l.lot_id(+) = ogil.lot_id&#13;&#10;           and ogi.ignore &#60;&#62; 'Y'&#13;&#10;         order by ogi.og_item_id&#13;&#10;        ) gi&#13;&#10; where gu.g_unit_id = gi.g_unit_id&#13;&#10;   and gu.good_code = g.good_code&#13;&#10;   and gi.quantity &#60;&#62; 0&#13;&#10;/*----------------------------------------------------------------------------*/&#13;&#10;/*   and (not exists (select 1 from tsoptions t where t.optcode = 'SAPACCSTDATE'&#13;&#10;                                                and to_date(t.value,'dd.mm.yyyy') &#60; trunc(sysdate))&#13;&#10;        or                                   &#13;&#10;        not exists (select 1 from tsoptions t where t.optcode = 'SEND_CHGTD_TO_XI' and t.value = 'Y'))&#13;&#10;union all&#13;&#10;select g.good_code,&#13;&#10;       g.name,&#13;&#10;       (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;       gi.quantity as quantity,&#13;&#10;       round( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as price,&#13;&#10;       round(gi.quantity * ( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code))), 2) as sum,&#13;&#10;       round(gi.quantity *  gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2 ) as sum_nds,&#13;&#10;       round( gi.quantity*gi.price2, 2 ) as sum_total,&#13;&#10;       to_char(get_taxrate (g.good_code))||'%' as ndstaxrate,&#13;&#10;       gi.gtd,&#13;&#10;       gi.country&#13;&#10;  from good g, &#13;&#10;       g_unit gu, &#13;&#10;       (select ogi.g_unit_id,&#13;&#10;               ogi.quantity,&#13;&#10;               ogi.price2,&#13;&#10;               (case when lower(l.country) like '%ðîññèÿ%' then '' else l.lot_value end) &#13;&#10;                 as gtd,&#13;&#10;               l.country&#13;&#10;          from o_good_item ogi, ogi_lot ogil, lot l&#13;&#10;         where ogi.order_id = :order_id&#13;&#10;           and ogil.og_item_id = ogi.og_item_id&#13;&#10;           and l.lot_id = ogil.lot_id&#13;&#10;           and ogi.ignore &#60;&#62; 'Y'&#13;&#10;           and l.lot_id = (select max(lot_id) from ogi_lot where og_item_id = ogi.og_item_id and quantity = ogi.quantity)&#13;&#10;         union all   &#13;&#10;        select ogi.g_unit_id,&#13;&#10;               ogi.quantity,&#13;&#10;               ogi.price2,&#13;&#10;               null as gtd,&#13;&#10;               null as country&#13;&#10;          from o_good_item ogi --, ogi_lot ogil, lot l&#13;&#10;         where ogi.order_id = :order_id&#13;&#10;           and ogi.ignore &#60;&#62; 'Y'&#13;&#10;           and not exists (select 1 from ogi_lot ogil where ogil.og_item_id = ogi.og_item_id)&#13;&#10;        ) gi&#13;&#10;where gu.g_unit_id = gi.g_unit_id&#13;&#10;   and gu.good_code = g.good_code&#13;&#10;   and gi.quantity &#60;&#62; 0&#13;&#10;   and exists (select 1 from tsoptions t where t.optcode = 'SAPACCSTDATE'&#13;&#10;                                           and to_date(t.value,'dd.mm.yyyy') &#60; trunc(sysdate))&#13;&#10;   and exists (select 1 from tsoptions t where t.optcode = 'SEND_CHGTD_TO_XI' and t.value = 'Y')*/&#13;&#10;/*----------------------------------------------------------------------------*/" PropData="05704C65667402380470546F7003A400095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653