    <TfrxDOAQuery Name="odsContragent" UserName="odsContragent" CloseDataSource="True" FieldAliases.Text="WS=WS&#13;&#10;WS_DEST=WS_DEST" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="select &#13;&#10;  ogi.WERK_CODE AS WS,&#13;&#10;  case when o.ot_code = 'GISSUE' then&#13;&#10;    (&#13;&#10;      select c.outerid&#13;&#10;      from o_contragents oc, contragents c&#13;&#10;      where &#13;&#10;        oc.order_id = o.parent_order_id&#13;&#10;        and oc.cnttype_code= 'CONSIGNEE'&#13;&#10;        and c.contragent_id = oc.contragent_id   &#13;&#10;        and rownum = 1    &#13;&#10;    )&#13;&#10;  else  &#13;&#10;    (&#13;&#10;      select c.outerid&#13;&#10;      from o_contragents oc, contragents c&#13;&#10;      where &#13;&#10;        oc.order_id = o.order_id&#13;&#10;        and oc.cnttype_code= 'CONSIGNEE'&#13;&#10;        and c.contragent_id = oc.contragent_id &#13;&#10;        and rownum = 1     &#13;&#10;    )&#13;&#10;  end as WS_DEST&#13;&#10;  &#13;&#10;from &#13;&#10;  o_good_item ogi, orders o  &#13;&#10;where &#13;&#10;  ogi.order_id = :order_id&#13;&#10;  and o.order_id = ogi.order_id&#13;&#10;  and ogi.quantity &#60;&#62; 0  " PropData="05704C6566740304010470546F70031601095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
