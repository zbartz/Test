    <TfrxDOAQuery Name="adrWerk" UserName="adrWerk" CloseDataSource="True" FieldAliases.Text="WERK_CODE=WERK_CODE&#13;&#10;FULL_FIS_ADRESS=FULL_FIS_ADRESS&#13;&#10;FULL_U_ADRESS=FULL_U_ADRESS&#13;&#10;WERK_NAME_F=WERK_NAME_F" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT werk_code, werk_name_f||address_f||case when addr_from_TTN.phone_ub is not null then ' òåë. ' end ||phone_ub&#13;&#10;||case when addr_from_TTN.fax_f is not null then ' ôàêñ ' end||fax_f as full_fis_adress,&#13;&#10;--âçÿò òåëåôîí þð.ëèöà, ò.ê. òåëåôîí ôèç ëèöà - ñïðàâî÷íàÿ&#13;&#10;werk_name_u||address_u || case when addr_from_TTN.phone_ub is not null then ' òåë. ' end ||phone_ub &#13;&#10;||case when addr_from_TTN.fax_ub is not null then ' ôàêñ ' end||fax_ub AS full_u_adress,&#13;&#10;werk_name_f&#13;&#10;FROM&#13;&#10;--äàëåå âçÿòà ñòàíäàðòíàÿ âûáîðêà èç ÒÒÍ&#13;&#10;(SELECT&#13;&#10;  w.werk_code AS werk_code,&#13;&#10;  --(select a.namebase from address a where a.address_id = abnl.address_id) AS werk_name_u,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'LEGALADDR')  AS werk_name_u,&#13;&#10;  TS_INFO.getaddressstring(abnl.address_id) AS address_u,&#13;&#10;  --TS_INFO.getwerkname( w.werk_code, 'PHYSADDR' ) AS werk_name_f,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'PHYSADDR') AS werk_name_f,&#13;&#10;  TS_INFO.getaddressstring(abnf.address_id, 'ZCSHBFRA') AS address_f,&#13;&#10;    TS_INFO.GetPhoneString(abnf.address_id, 'TEL') as phone_f,&#13;&#10;  TS_INFO.GetPhoneString(abnf.address_id, 'FAX') as fax_f,&#13;&#10;  --TS_INFO.getwerkname( w.werk_code, 'LEGALADDRC' ) AS werk_name_ub,&#13;&#10;  pk_franch.get_werkname('ORDERS',o.order_id,'LEGALADDRC')  AS werk_name_ub,&#13;&#10;  TS_INFO.getaddressstring(abnb.address_id, 'ZCSHBFRA') AS address_ub,&#13;&#10;  TS_INFO.G