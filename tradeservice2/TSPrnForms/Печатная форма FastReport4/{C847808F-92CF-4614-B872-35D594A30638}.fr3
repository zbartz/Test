    <TfrxDOAQuery Name="dstMain" UserName="dstMain" CloseDataSource="True" FieldAliases.Text="ROWNUM=ROWNUM&#13;&#10;ORDER_ID=ORDER_ID&#13;&#10;OG_ITEM_ID=OG_ITEM_ID&#13;&#10;STORAGE_CODE=STORAGE_CODE&#13;&#10;DOCDATE=DOCDATE&#13;&#10;GOOD_NAME=GOOD_NAME&#13;&#10;GOOD_CODE=GOOD_CODE&#13;&#10;AC_NUM=AC_NUM&#13;&#10;UMNAME=UMNAME&#13;&#10;QUANTITY=QUANTITY&#13;&#10;SALEPRICE=SALEPRICE&#13;&#10;TOTALPRICE=TOTALPRICE&#13;&#10;WEIGHT=WEIGHT&#13;&#10;INTWEIGHT=INTWEIGHT&#13;&#10;BRAND=BRAND&#13;&#10;SERNUM=SERNUM&#13;&#10;DEFECTS=DEFECTS&#13;&#10;OUTERVIEW=OUTERVIEW&#13;&#10;COMPLECT=COMPLECT&#13;&#10;DLVSTORAGE=DLVSTORAGE&#13;&#10;CUSTOMER=CUSTOMER&#13;&#10;SALEDATE=SALEDATE&#13;&#10;ZN=ZN&#13;&#10;KLIENT=KLIENT&#13;&#10;IMEI=IMEI" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A250000000300000001000000090000003A4F524445525F49440300000004000000B61C010000000000" SQL.Text="SELECT&#13;&#10;      rownum, &#13;&#10;      T.order_id AS ORDER_ID,&#13;&#10;      T.OG_ITEM_ID AS OG_ITEM_ID,&#13;&#10;      T.STORAGE_CODE,&#13;&#10;      T.docdate as DOCDATE,&#13;&#10;      T.good_name AS good_name, --Íàèìåíîâàíèå&#13;&#10;      T.good_code AS good_code,--Êîä &#13;&#10;      T.ac_num AS ac_num, --íîìåð ÊÒ&#13;&#10;      T.umname AS umname,--åäþèçìåðåíèÿ&#13;&#10;      T.quantity AS quantity,--êîëè÷åñòâî&#13;&#10;decode(      &#13;&#10;(&#13;&#10;  select count(*) from good g1&#13;&#10;  join g_unit gu1 on g1.good_code = gu1.good_code&#13;&#10;  where (g1.mecat in &#13;&#10;  (select gmp1.mecat from g_mecat_group gmp1&#13;&#10;  start with gmp1.mecat in (select value from tsoptions ts where ts.optcode = 'TREESAP')&#13;&#10;  connect by prior gmp1.mecat = gmp1.parent) or&#13;&#10;  g1.mecat in ('502040101', '522040101')) and g1.good_code = t.good_code&#13;&#10;),&#13;&#10;0, T.saleprice,&#13;&#10;(&#13;&#10;select max(gp1.absval) from orders o1 &#13;&#10;join o_good_item ogi1 on o1.order_id = ogi1.order_id&#13;&#10;join g_unit gu1 on gu1.g_unit_id = ogi1.g_unit_id&#13;&#10;join g_price gp1 on gp1.g_unit_id = gu1.g_unit_id and gp1.ct_code = 'ZROZ' where o1.order_id = t.order_id&#13;&#10;)&#13;&#10;) as saleprice,&#13;&#10;decode(      &#13;&#10;(select count(*) FROM good g1 WHERE g1.good_code =t.good_code and g1.mecat in ('502040101', '522040101')),&#13;&#10;0,TO_NUMBER(T.quantity)*TO_NUMBER(T.saleprice),&#13;&#10;(&#13;&#10;select max(gp1.absval) from orders o1 &#13;&#10;join o_good_item ogi1 on o1.order_id = ogi1.order_id&#13;&#10;join g_unit gu1 on gu1.g_unit_id = ogi1.g_unit_id&#13;&#10;join g_price gp1 on gp1.g_unit_id = gu1.g_unit_id and gp1.ct_code = 'ZROZ' where o1.order_id = t.order_id&#13;&#10;)) as totalprice,&#13;&#10;      T.weight AS weight,--ìàññà&#13;&#10;      T.intweight,--êðóãëàÿ ìàññà&#13;&#10;      T.brand AS brand,--Áðýíä&#13;&#10;      T.sernum,&#13;&#10;      T.defects,&#13;&#10;      T.outerview,&#13;&#10;      T.complect,&#13;&#10;      T.dlvstorage,&#13;&#10;      T.customer,    &#13;&#10;      T.saledate ,&#13;&#10;&#13;&#10;T.ZN, &#13;&#10;decode(t.ot_code,'ZPRTRK','',t.klient) klient , t.imei,t.rn&#13;&#10;&#13;&#10;--âëîæåíèå äëÿ òîãî, ÷òîáû ïîñ÷èòàòü totalprice&#13;&#10;      FROM&#13;&#10;            (SELECT &#13;&#10;            o.order_id AS ORDER_ID,&#13;&#10;            ogi.og_item_id,&#13;&#10;            st.STRG_KIND AS STORAGE_CODE,&#13;&#10;            o.docdate as DOCDATE,&#13;&#10;            g.NAME AS good_name, --Íàèìåíîâàíèå&#13;&#10;            g.good_code AS good_code,--Êîä &#13;&#10;            ogi.ac_num AS ac_num, --íîìåð ÊÒ&#13;&#10;            um.short_name AS umname,--åäþèçìåðåíèÿ&#13;&#10;            ogi.quantity AS quantity,--êîëè÷åñòâî&#13;&#10;                       nvl((SELECT decode(opv.opvalue, 0, null) FROM op_values opv&#13;&#10;                         WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                         AND opv.op_code='SALEPRICE'),ogi.price1) AS saleprice,&#13;&#10;            gu.weight AS weight,--ìàññà&#13;&#10;            TRUNC(gu.weight) AS intweight,--êðóãëàÿ ìàññà&#13;&#10;            g.brand_code AS brand,--Áðýíä&#13;&#10;                         (SELECT opv.opvalue FROM op_values opv&#13;&#10;                         WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                         AND opv.op_code='SERNUM') AS sernum,&#13;&#10;                             (SELECT opv.opvalue FROM op_values opv&#13;&#10;                             WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                             AND opv.op_code='DEFECTS') AS defects,&#13;&#10;                         (SELECT opv.opvalue FROM op_values opv&#13;&#10;                         WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                         AND opv.op_code='OUTERVIEW') AS outerview,&#13;&#10;                             (SELECT opv.opvalue FROM op_values opv&#13;&#10;                             WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                             AND opv.op_code='COMPLECT') AS complect,&#13;&#10;                         (SELECT opv.opvalue FROM op_values opv&#13;&#10;                         WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                         AND opv.op_code='CUSTOMER') AS customer,&#13;&#10;                             CASE WHEN o.ot_code = 'GISSUE' THEN&#13;&#10;                              dlv.opvalue &#13;&#10;                             ELSE  &#13;&#10;                             (&#13;&#10;                               SELECT opv.opvalue FROM op_values opv&#13;&#10;                               WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                               AND opv.op_code='DLVSTORAGE'     &#13;&#10;                             )  &#13;&#10;                             END AS dlvstorage,       &#13;&#10;                         (SELECT &#13;&#10;          CASE WHEN opv.opvalue = '30.12.1899' THEN NULL&#13;&#10;          ELSE opv.opvalue END&#13;&#10;          FROM op_values opv&#13;&#10;                             WHERE opv.og_item_id=ogi.og_item_id&#13;&#10;                             AND opv.op_code='SALEDATE') AS saledate ,&#13;&#10;&#13;&#10;  dcnum.docout_id  as ZN,&#13;&#10;         a.namebase  as klient ,&#13;&#10;         pi.imei  as IMEI, o1.ot_code,&#13;&#10;row_number() over (partition by nvl(dcnum.docout_id,o1.order_id) order by o1.createdatetime desc) rn &#13;&#10;&#13;&#10;                             &#13;&#10;            --coalesce (opv.og_item_id,0)&#13;&#10;            FROM o_good_item ogi&#13;&#10;            join g_unit gu on gu.g_unit_id = ogi.g_unit_id &#13;&#10;            join good g on g.good_code = gu.good_code &#13;&#10;            join unit_measure um on um.um_code = gu.unit_code&#13;&#10;            join orders o on o.order_id = ogi.order_id&#13;&#10;            join storage st on st.STRG_ID = ogi.strg_id&#13;&#10;            left join  (select distinct &#13;&#10;                             opv.opvalue, &#13;&#10;                             l.og_child_item og_item_id&#13;&#10;                      from orders o, op_values opv, o_good_item ogi1, ogi_link l&#13;&#10;                      where o.order_id = :order_id&#13;&#10;                        and ogi1.order_id = o.parent_order_id&#13;&#10;                        and opv.og_item_id = ogi1.og_item_id&#13;&#10;                        and ogi1.og_item_id = l.og_parent_item&#13;&#10;                        and opv.op_code='DLVSTORAGE') dlv  on  dlv.og_item_id = ogi.og_item_id&#13;&#10;&#13;&#10;             left join o_good_item ogi1 on ogi1.ac_num=ogi.ac_num&#13;&#10;             left join orders o1 on o1.order_id=ogi1.order_id&#13;&#10;             left join docoutnum dcnum on (dcnum.order_id=o1.order_id and dcnum.op_code='CISNUMBER') &#13;&#10;             left join address a on a.address_id=o1.address_id&#13;&#10;             left join ogi_pi_link lnk on  (lnk.og_item_id = ogi1.og_item_id and lnk.werk_code=get_werk_base())&#13;&#10;             left join product_item pi on lnk.pi_guid = pi.pi_guid&#13;&#10;&#13;&#10;            WHERE ogi.order_id = :order_id&#13;&#10;&#13;&#10;      &#13;&#10;       &#13;&#10;      and o1.ot_code in('ZPRTRK','ZDELR','ZRTRN')&#13;&#10;            and o1.os_code not in ('DOC_STORN')&#13;&#10;	     and o1.createdatetime&#60;=o.createdatetime&#13;&#10;&#13;&#10;) T where t.rn=1" PropData="05704C65667402380470546F700234095661726961626C657301010C490000002044617461547970653D226674496E746567657222204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223732383836220000"/>
