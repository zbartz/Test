eString(abnf.address_id, 'FAX') as fax_f,&#13;&#10;  TS_INFO.getwerkname( w.werk_code, 'LEGALADDRC' ) AS werk_name_ub,&#13;&#10;  TS_INFO.getaddressstring(abnb.address_id, 'ZCSHBFRA') AS address_ub,&#13;&#10;   TS_INFO.GetPhoneString(abnb.address_id, 'TEL') as phone_ub,&#13;&#10;   TS_INFO.GetPhoneString(abnb.address_id, 'FAX') as fax_ub,&#13;&#10;  abnl.bankname as bankname_l,&#13;&#10;  abnl.inn as inn_l,&#13;&#10;  abnl.kpp as kpp_1,&#13;&#10;  abnl.bik as bik_l,&#13;&#10;  abnl.sacnt as sacnt_l,&#13;&#10;  abnl.okpo as okpo_l,&#13;&#10;  abnl.koracnt as koracnt_l,&#13;&#10;  abnf.bankname as bankname_f,&#13;&#10;  abnf.inn as inn_f,&#13;&#10;  abnf.kpp as kpp_f,&#13;&#10;  abnf.bik as bik_f,&#13;&#10;  abnf.sacnt as sacnt_f,&#13;&#10;  abnf.okpo as okpo_f,&#13;&#10;  abnf.koracnt as koracnt_f,&#13;&#10;  abnb.bankname as bankname_b,&#13;&#10;  abnb.inn as inn_b,&#13;&#10;  abnb.kpp as kpp_b,&#13;&#10;  abnb.bik as bik_b,&#13;&#10;  abnb.sacnt as sacnt_b,&#13;&#10;  abnb.okpo as okpo_b,&#13;&#10;  abnb.koracnt as koracnt_b,&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.'&#13;&#10;     from p_staff ps, wp_link wp&#13;&#10;    where wp.werk_code=get_werk_base &#13;&#10;      and wp.wp_code='LEGALADDRC'&#13;&#10;      and ps.id=wp.director_id) AS director,&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.'&#13;&#10;     from p_staff ps, wp_link wp&#13;&#10;    where wp.werk_code=get_werk_base &#13;&#10;      and wp.wp_code='LEGALADDRC'&#13;&#10;      and ps.id=wp.chiefacc_id) AS chiefacc&#13;&#10;FROM werk w, addr_bninfo abnl, addr_bninfo abnf, addr_bninfo abnb&#13;&#10;WHERE w.is_base = 'Y'&#13;&#10;  AND abnl.address_id = (select wp.address_id from wp_link wp&#13;&#10;                             where get_werk_base = wp.werk_code&#13;&#10;                               and wp.wp_code = 'LEGALADDR') &#13;&#10;  AND abnf.address_id = (select wp.address_id from wp_link wp&#13;&#10;         