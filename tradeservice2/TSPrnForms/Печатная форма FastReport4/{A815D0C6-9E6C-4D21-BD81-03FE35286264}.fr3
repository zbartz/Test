    <TfrxDOAQuery Name="odsOrder" UserName="odsOrder" CloseDataSource="True" FieldAliases.Text="STAFF_ID=STAFF_ID&#13;&#10;NAME=NAME&#13;&#10;INVOICEDATE=INVOICEDATE&#13;&#10;INVOICENO=INVOICENO&#13;&#10;SAPORDNUM=SAPORDNUM&#13;&#10;ORDER_ID=ORDER_ID&#13;&#10;DOCDATE=DOCDATE&#13;&#10;OT_CODE=OT_CODE&#13;&#10;CHAINNUM=CHAINNUM&#13;&#10;OS_CODE=OS_CODE&#13;&#10;ADDRESS_ID=ADDRESS_ID&#13;&#10;FINISH_DATE=FINISH_DATE&#13;&#10;DESIREDDT=DESIREDDT&#13;&#10;HASLOCALITEMS=HASLOCALITEMS&#13;&#10;HASREMOTEITEMS=HASREMOTEITEMS&#13;&#10;CREATEDATETIME=CREATEDATETIME&#13;&#10;CREATEUSERCODE=CREATEUSERCODE&#13;&#10;CHANGEDATETIME=CHANGEDATETIME&#13;&#10;CHANGEUSERCODE=CHANGEUSERCODE&#13;&#10;SRCDOBJCODE=SRCDOBJCODE&#13;&#10;OUTERID=OUTERID&#13;&#10;PARENT_ORDER_ID=PARENT_ORDER_ID&#13;&#10;STRG_ID=STRG_ID&#13;&#10;STORAGE_CODE=STORAGE_CODE&#13;&#10;WERK_CODE=WERK_CODE&#13;&#10;CONTRAGENT_ID=CONTRAGENT_ID&#13;&#10;CURRENCY=CURRENCY&#13;&#10;WL_ID=WL_ID&#13;&#10;PAYDOCDAT=PAYDOCDAT&#13;&#10;PAYDOCNUM=PAYDOCNUM&#13;&#10;MULTY=MULTY" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033343233380000000000" SQL.Text="select&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;  nvl((select operationdate&#13;&#10;       from o_status_log&#13;&#10;       where order_id = :order_id&#13;&#10;         and os_code IN('ZBNAL_SHPD','ZPMNT_SALE')), o.docdate) as InvoiceDate,&#13;&#10;  nvl((select max(opvalue) from op_values opv&#13;&#10;       where opv.order_id = :order_id&#13;&#10;         and op_code = 'INVOICENUM'), 0 ) as InvoiceNo,&#13;&#10;  pk_common.GetDocOutNum(o.parent_order_id,'SAPORDNUM') as SAPORDNUM,&#13;&#10;  o.*,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCDAT') as PayDocDat,&#13;&#10;  get_op_values(o.order_id,null,'PAYMDOCNUM') as PayDocNum,&#13;&#10;  case when (SELECT count(*) from tmp_table)&#62;1 then 'Y' else 'N' end as multy&#13;&#10;  --NVL( (SELECT MAX('Y') FROM tmp_table), 'N' ) as multy&#13;&#10;from orders o,staff s&#13;&#10;where o.order_id = :order_id&#13;&#10;  and o.createusercode = s.user_code(+)" PropData="05704C65667402380470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223334323338220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;UNAME=UNAME&#13;&#10;NUM_CODE=NUM_CODE&#13;&#10;QUANTITY=QUANTITY&#13;&#10;PRICE=PRICE&#13;&#10;SUM=SUM&#13;&#10;SUM_NDS=SUM_NDS&#13;&#10;SUM_TOTAL=SUM_TOTAL&#13;&#10;NDSTAXRATE=NDSTAXRATE&#13;&#10;GTD=GTD&#13;&#10;COUNTRY=COUNTRY&#13;&#10;COUNTRY_ISO=COUNTRY_ISO&#13;&#10;COUNTRY_NAME=COUNTRY_NAME" BCDToCurrency="False" Master="dstOrders" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="select g.good_code,&#13;&#10;       g.name,&#13;&#10;       (select short_name from unit_measure where um_code = gu.unit_code) as uname,&#13;&#10;       (select num_code from unit_measure where um_code = gu.unit_code) as num_code,&#13;&#10;       gi.quantity as quantity,&#13;&#10;       round(gi.price2 - gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2) as price,&#13;&#10;       round(gi.quantity * ( gi.price2-gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code))), 2) as sum,&#13;&#10;       round(gi.quantity *  gi.price2*get_taxrate (g.good_code)/(100+get_taxrate (g.good_code)), 2) as sum_nds,&#13;&#10;       round(gi.quantity * gi.price2, 2) as sum_total,&#13;&#10;       to_char(get_taxrate (g.good_code))||'%' as ndstaxrate,&#13;&#10;       gi.gtd,&#13;&#10;       gi.country, &#13;&#10;       gi.country_iso,&#13;&#10;       gi.country_name&#13;&#10;  from good g &#13;&#10;  join g_unit gu on gu.good_code = g.good_code&#13;&#10;  join (&#13;&#10;     select gi_in.g_unit_id,  sum(gi_in.quantity) as quantity, gi_in.price2,  gi_in.gtd, gi_in.country, gi_in.country_iso, gi_in.country_name&#13;&#10;     from(&#13;&#10;           select  &#13;&#10;               ogi.g_unit_id,&#13;&#10;               -- Если нет ГТД то кол-во берем из тов. позиции&#13;&#10;               -- Если ГТД есть то когда получаем последнюю позицию по ГТД - добавляем&#13;&#10;               -- остаток от нераспределенного кол-ва, для не последней позиции по ГТД&#13;&#10;               -- берем кол-во по таблице распределения     &#13;&#10;			   &#13;&#10;               ( case  when  (lower(l.country) like lower('%россия%')) or (ogil.quantity is null)  &#13;&#10;                 then  ogi.quantity&#13;&#10;                 else -- Разбиваем по ГТД партиям, но если партий не хватает, то в последнюю добавляем недостающий товар &#13;&#10;                    decode(row_number () over (partition by ogi.g_unit_id, ogil.og_item_id order by ogil.og_item_id),&#13;&#10;                          (select count (1) from ogi_lot where og_item_id = ogi.og_item_id ),&#13;&#10;                          (ogil.quantity + (ogi.quantity - (select sum (quantity) from ogi_lot where og_item_id = ogi.og_item_id))),&#13;&#10;                           ogil.quantity)&#13;&#10;                  end)    as quantity, &#13;&#10;               ogi.price2,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else l.lot_value &#13;&#10;                end) as gtd,&#13;&#10;               l.country,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else&#13;&#10;                 (select ISO from country c where lower(c.country_name) = lower(l.country) ) &#13;&#10;                end) as country_iso,&#13;&#10;               (case when lower(l.country) like '%россия%' then '' else&#13;&#10;                 (select COUNTRY_NAME from country c where lower(c.country_name) = lower(l.country) ) &#13;&#10;                end) as country_name               &#13;&#10;          from o_good_item ogi&#13;&#10;          left join ogi_lot ogil on  ogil.og_item_id = ogi.og_item_id&#13;&#10;          left join lot l        on l.lot_id = ogil.lot_id&#13;&#10;         where 1=1&#13;&#10;           and ogi.order_id = :order_id--257474--5041583--:order_id&#13;&#10;           and ogi.ignore &#60;&#62; 'Y'&#13;&#10;         ) gi_in&#13;&#10;       group by gi_in.g_unit_id,  gi_in.price2,  gi_in.gtd, gi_in.country, gi_in.country_iso, gi_in.country_name&#13;&#10;  ) gi&#13;&#10;  on  gi.g_unit_id = gu.g_unit_id &#13;&#10; where gi.quantity &#60;&#62; 0&#13;&#10; order by gi.g_unit_id" PropData="05704C65667402380470546F7003A300095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsWerk" UserName="odsWerk" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="select (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = pk_franch.get_chief_id('ORDERS',:order_id,'DIRECTOR','LEGALADDRC')&#13;&#10;               ) as director,&#13;&#10;       (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = pk_franch.get_chief_id('ORDERS',:order_id,'CHIEFACC','LEGALADDRC')&#13;&#10;               ) as chiefacc,&#13;&#10;       TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'INVOICENUM', 'ZBNAL', 'N' ) maxinvoiceno,&#13;&#10;       w.werk_code&#13;&#10;  from werk w&#13;&#10; where w.is_base = 'Y'&#13;&#10;   and not exists (SELECT zvalue1 FROM tmp_table)&#13;&#10;union all&#13;&#10;select (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#10;          from p_staff ps&#13;&#10;         where ps.id = wpl.director_id&#13;&#10;               ) as director,&#13;&#10;       (select    ps.family&#13;&#10;               || ' '&#13;&#10;               || substr (ps.fname, 0, 1)&#13;&#10;               || '.'&#13;&#10;               || substr (ps.lname, 0, 1)&#13;&#10;               || '.'&#13;&#1