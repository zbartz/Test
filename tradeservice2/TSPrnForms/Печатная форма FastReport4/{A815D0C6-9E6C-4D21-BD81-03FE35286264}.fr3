<TfrxReport Version="4.12.15" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38073,6220618981" ReportOptions.Description.Text="" ReportOptions.LastChange="43285,6761729051" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  InvoiceNo, NewInvoiceNo, id : String;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  InvoiceNo     := &#60;odsOrder.&#34;INVOICENO&#34;&#62;;&#13;&#10;  NewInvoiceNo  := 'N';&#13;&#10;  eRegDate.Date := Date;&#13;&#10;  eDovDate.Date := Date;      &#13;&#10;    &#13;&#10;  if InvoiceNo = '0' then&#13;&#10;  begin&#13;&#10;    InvoiceNo    := &#60;odsWerk.&#34;MAXINVOICENO&#34;&#62;;&#13;&#10;   // NewInvoiceNo := 'Y';&#13;&#10;  end;&#13;&#10;&#13;&#10;  edtInvoiceNum.Text := InvoiceNo;&#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62; = 'N' then&#13;&#10;  begin&#13;&#10;    edtInvoiceNum.Enabled := False;                                 &#13;&#10;  &#13;&#10;    rbReg.Enabled    := False;&#13;&#10;    rbCnt.Enabled    := False;&#13;&#10;    rbReg.Checked    := False;&#13;&#10;    rbCnt.Checked    := False;&#13;&#10;    eRegNum.Enabled  := False;&#13;&#10;    eRegDate.Enabled := False;&#13;&#10;&#13;&#10;    eCustomerNum.Enabled  := False;&#13;&#10;    eConsigneeNum.Enabled := False;&#13;&#10;&#13;&#10;    lReg.Enabled          := False;&#13;&#10;    lRegDate.Enabled      := False;&#13;&#10;    lCntCustomer.Enabled  := False;&#13;&#10;    lCntConsignee.Enabled := False;&#13;&#10;  end&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      rbReg.Enabled    := True;&#13;&#10;      rbCnt.Enabled    := True;&#13;&#10;      rbReg.Checked    := True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bOKOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eConsigneeNum.Text := &#60;odsOrder.&#34;CONTRAGENT_ID&#34;&#62;;             &#13;&#10;  InvoiceNo := edtInvoiceNum.Text;&#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62; = 'N' then&#13;&#10;  begin&#13;&#10;    id := &#60;odsOrder.&#34;ORDER_ID&#34;&#62;;&#13;&#10;    odsInvoice.Close;&#13;&#10;    odsInvoice.SetVariable('order_id',id);&#13;&#10;    odsInvoice.SetVariable('invoiceno',InvoiceNo);&#13;&#10;    odsInvoice.SetVariable('newinvoiceno',NewInvoiceNo);&#13;&#10;    odsInvoice.ExecSQL;&#13;&#10;  end&#13;&#10;  else&#13;&#10;    if rbCnt.Checked then&#13;&#10;    begin&#13;&#10;      dstCustomerR.Close;&#13;&#10;      dstCustomerR.SetVariable('CONTRAGENT_ID',eCustomerNum.Text );&#13;&#10;      dstCustomerR.Open;&#13;&#10;&#13;&#10;      dstConsigneeR.Close;&#13;&#10;      dstConsigneeR.SetVariable('CONTRAGENT_ID',eConsigneeNum.Text );&#13;&#10;      dstConsigneeR.Open;&#13;&#10;    end;&#13;&#10;&#13;&#10;  if (&#60;odsOrder.&#34;OT_CODE&#34;&#62; = 'ZBNAL') and (&#60;odsOrder.&#34;MULTY&#34;&#62; = 'N') then&#13;&#10;    begin                         &#13;&#10;      if (&#60;odsCustomer.&#34;NAME&#34;&#62;='') or (&#60;odsCustomer.&#34;ADDRESS&#34;&#62;='') then&#13;&#10;        begin&#13;&#10;          Memo126.Lines.Add('Не полностью заполнены наименование и адрес покупателя');                            &#13;&#10;          DialogPage1.Visible := True;    &#13;&#10;        end;                  &#13;&#10;      if (&#60;odsCustomer.&#34;KPP&#34;&#62;='') then&#13;&#10;        begin&#13;&#10;          Memo126.Lines.Add('Не заполнен КПП покупателя');                          &#13;&#10;          DialogPage1.Visible := True;  &#13;&#10;        end;                  &#13;&#10;      if (&#60;odsConsignee.&#34;NAME&#34;&#62;='') or (&#60;odsConsignee.&#34;ADDRESS&#34;&#62;='') then&#13;&#10;        begin&#13;&#10;          Memo126.Lines.Add('Не полностью заполнены наименование и адрес грузополучателя');                          &#13;&#10;          DialogPage1.Visible := True;  &#13;&#10;        end;                  &#13;&#10;    end&#13;&#10;  else              &#13;&#10;    DialogPage1.Visible := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbRegOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eRegNum.Enabled  := True;&#13;&#10;  eRegDate.Enabled := True;&#13;&#10;  eCustomerNum.Enabled  := False;&#13;&#10;  eConsigneeNum.Enabled := False;&#13;&#10;&#13;&#10;  lReg.Enabled          := True;&#13;&#10;  lRegDate.Enabled      := True;&#13;&#10;  lCntCustomer.Enabled  := False;&#13;&#10;  lCntConsignee.Enabled := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbCntOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eRegNum.Enabled  := False;&#13;&#10;  eRegDate.Enabled := False;&#13;&#10;  eCustomerNum.Enabled  := True;&#13;&#10;  eConsigneeNum.Enabled := True;&#13;&#10;&#13;&#10;  lReg.Enabled          := False;&#13;&#10;  lRegDate.Enabled      := False;&#13;&#10;  lCntCustomer.Enabled  := True;&#13;&#10;  lCntConsignee.Enabled := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  if &#60;TOTALPAGES&#62; &#62; 1&#13;&#10;//    then PageFooter1.Visible := True;                                         &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo63OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i : integer;                                 &#13;&#10;begin&#13;&#10;//  Ins_IDOC.SetVariable('DOCID', VarToStr(&#60;odsOrder.&#34;ORDER_ID&#34;&#62;));&#13;&#10;//  Ins_IDOC.SetVariable('CORID', '');      &#13;&#10;        &#13;&#10;  for i:=0 to 8-Length(InvoiceNo) do&#13;&#10;  begin&#13;&#10;    InvoiceNo := '0'+InvoiceNo;                        &#13;&#10;  end;&#13;&#10;&#13;&#10;//  Ins_IDOC.SetVariable('EXT_ORIG', VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo);&#13;&#10;//  Ins_IDOC.SetVariable('EXT_CORR', '');      &#13;&#10;        &#13;&#10;    &#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62;= 'Y' then&#13;&#10;    Memo63.Memo.Text :=VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo+' от '+DateToStr(eRegDate.Date)  &#13;&#10;  else&#13;&#10;    Memo63.Memo.Text :=VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;)+'/'+ '0' +InvoiceNo+' от '+VarToStr(&#60;odsOrder.&#34;InvoiceDate&#34;&#62;);&#13;&#10;&#13;&#10;//  Ins_IDOC.SetVariable('DATE_ORIG', VarToStr(&#60;odsOrder.&#34;InvoiceDate&#34;&#62;));&#13;&#10;//  Ins_IDOC.SetVariable('DATE_CORR', '');&#13;&#10;//  Ins_IDOC.SetVariable('LOCNR', VarToStr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;));      &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo75OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  if &#60;TOTALPAGES&#62; &#62; 1 then&#13;&#10;//    Memo75.Visible := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo105OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsGItems.&#34;NDSTAXRATE&#34;&#62; = '0%' then&#13;&#10;    Memo105.Memo.Text := 'без НДС'&#13;&#10;  else&#13;&#10;    Memo105.Memo.Text := &#60;odsGItems.&#34;NDSTAXRATE&#34;&#62;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo106OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsGItems.&#34;SUM_NDS&#34;&#62; = '0,00' then &#13;&#10;    Memo106.Memo.Text := 'без НДС'&#13;&#10;  else&#13;&#10;    Memo106.Memo.Text := &#60;odsGItems.&#34;SUM_NDS&#34;&#62;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo19OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then begin          &#13;&#10;    Memo19.Memo.Text := &#60;dstCustomerR.&#34;NAME&#34;&#62;;&#13;&#10;    Ins_IDOC.SetVariable('NAME', &#60;dstCustomerR.&#34;NAME&#34;&#62;);        &#13;&#10;  end            &#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo19.Memo.Text := 'по реестру № '+eRegNum.Text+' от '+FormatDateTime('DD.MM.YYYY',eRegDate.Date)&#13;&#10;  else begin             &#13;&#10;    Memo19.Memo.Text := &#60;odsCustomer.&#34;NAME&#34;&#62;;&#13;&#10;    Ins_IDOC.SetVariable('NAME', &#60;odsCustomer.&#34;NAME&#34;&#62;);        &#13;&#10;  end;                &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo21OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then&#13;&#10;    Memo21.Memo.Text := &#60;dstCustomerR.&#34;ADDRESS&#34;&#62;&#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo21.Memo.Text := ''&#13;&#10;  else&#13;&#10;    Memo21.Memo.Text := &#60;odsCustomer.&#34;ADDRESS&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure mCustomerInnKpp1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then begin          &#13;&#10;    mCustomerInnKpp1.Memo.Text := VarToStr(&#60;dstCustomerR.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;dstCustomerR.&#34;KPP&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('INN', VarToStr(&#60;dstCustomerR.&#34;INN&#34;&#62;));&#13;&#10;    Ins_IDOC.SetVariable('KPP', VarToStr(&#60;dstCustomerR.&#34;KPP&#34;&#62;));        &#13;&#10;  end            &#13;&#10;  else if rbReg.Checked then&#13;&#10;    mCustomerInnKpp1.Memo.Text := ''&#13;&#10;  else begin             &#13;&#10;    mCustomerInnKpp1.Memo.Text := VarToStr(&#60;odsCustomer.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;odsCustomer.&#34;KPP&#34;&#62;);&#13;&#10;    Ins_IDOC.SetVariable('INN', VarToStr(&#60;odsCustomer.&#34;INN&#34;&#62;));&#13;&#10;    Ins_IDOC.SetVariable('KPP', VarToStr(&#60;odsCustomer.&#34;KPP&#34;&#62;));        &#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo15OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsVENDOR.&#34;NAME_F&#34;&#62; &#60;&#62; '' then&#13;&#10;    Memo15.Memo.Text := VarToStr(&#60;odsVENDOR.&#34;NAME_F&#34;&#62;) + ', ' + VarToStr(&#60;odsVENDOR.&#34;ADDRESS_F&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo17OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then&#13;&#10;    Memo17.Memo.Text := VarToStr(&#60;dstConsigneeR.&#34;NAME&#34;&#62;) + ' ' + VarToStr(&#60;dstConsigneeR.&#34;ADDRESS&#34;&#62;)  &#13;&#10;  else if rbReg.Checked then&#13;&#10;    Memo17.Memo.Text := 'по реестру № '+eRegNum.Text+' от '+FormatDateTime('DD.MM.YYYY',eRegDate.Date)&#13;&#10;  else&#13;&#10;    Memo17.Memo.Text := VarToStr(&#60;odsConsignee.&#34;NAME&#34;&#62;) + ' ' + VarToStr(&#60;odsConsignee.&#34;ADDRESS&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo28OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;odsCng.&#34;INVCNGNUM&#34;&#62;) &#60;&#62; '' then&#13;&#10;    Memo28.Memo.Text := VarToStr(&#60;odsCng.&#34;INVCNGNUM&#34;&#62;)+ ' от ' + VarToStr(&#60;odsCng.&#34;INVCNGDATE&#34;&#62;)&#13;&#10;  else&#13;&#10;    Memo28.Memo.Text := ' - от &#34;-&#34; -';                        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportOnRunDialogs(var Result: Boolean);&#13;&#10;begin&#13;&#10;  odsOrder.Open;&#13;&#10;  Result:=(odsOrder.FieldByName('OT_CODE').AsString &#60;&#62; 'ZBNAL');  &#13;&#10;  if not Result then&#13;&#10;    begin&#13;&#10;      odsGItems.Open;                        &#13;&#10;      odsGift.SetVariable('order_id', odsOrder.FieldByName('order_id').AsString);&#13;&#10;      odsGift.Open;&#13;&#10;      Result:=odsGift.FieldByName('GiftCount').AsInteger = odsGItems.RecordCount;&#13;&#10;      odsGift  .Close;&#13;&#10;      odsGItems.Close;                      &#13;&#10;    end;                &#13;&#10;  odsOrder.Close;&#13;&#10;&#13;&#10;  if not Result then&#13;&#10;    MessageDlg('Заказ содержит подарочные карты, документы ТОРГ-12 и Счет-фактура на продажу ПК не выдаются.', mtWarning, mbOk, 0)&#13;&#10;  else &#13;&#10;    Result := Page1.ShowModal = mrOK;&#13;&#10;&#13;&#10;  bOKOnBeforePrint(self);              &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo74OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var strDov: String;                                             &#13;&#10;begin&#13;&#10;  strDov :='';                     &#13;&#10;  if eDovNum.Text &#60;&#62; '' then begin             &#13;&#10;    strDov := 'По доверенности № '+eDovNum.Text+' от '+FORMATDATETIME('DD.MM.YYYY',eDovDate.Date);  &#13;&#10;    if eDovVid.Text &#60;&#62; '' then&#13;&#10;      strDov := strDov+', выданной '+eDovVid.Text;&#13;&#10;&#13;&#10;    Memo74.Memo.Text :=strDov;                        &#13;&#10;  end;              &#13;&#10;end;&#13;&#10;&#13;&#10;procedure mCustomerInnKpp2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if rbCnt.Checked then begin          &#13;&#10;    mCustomerInnKpp2.memo.text := &#60;dstCustomerR.&#34;NAME&#34;&#62;+ ' ' + VarToStr(&#60;dstCustomerR.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;dstCustomerR.&#34;KPP&#34;&#62;);&#13;&#10;  end            &#13;&#10;  else if rbReg.Checked then&#13;&#10;    mCustomerInnKpp2.memo.text := 'по реестру № '+eRegNum.Text+' от '+FormatDateTime('DD.MM.YYYY',eRegDate.Date)                  &#13;&#10;  else begin             &#13;&#10;    mCustomerInnKpp2.memo.text := &#60;odsCustomer.&#34;NAME&#34;&#62; + ' ' + VarToStr(&#60;odsCustomer.&#34;INN&#34;&#62;)+' / '+VarToStr(&#60;odsCustomer.&#34;KPP&#34;&#62;);                                &#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo22OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;odsOrder.&#34;MULTY&#34;&#62; = 'Y' then&#13;&#10;    Memo22.Memo.Text := vartostr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;) + '/' + edtInvoiceNum.Text&#13;&#10;  else&#13;&#10;    if &#60;odsOrder.&#34;OT_CODE&#34;&#62; = 'GISSUE' then&#13;&#10;      Memo22.Memo.Text := vartostr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;) + '/' + vartostr(&#60;odsOrder.&#34;SAPORDNUM&#34;&#62;)&#13;&#10;    else&#13;&#10;      Memo22.Memo.Text := vartostr(&#60;odsWerk.&#34;WERK_CODE&#34;&#62;) + '/' + vartostr(&#60;odsOrder.&#34;ORDER_ID&#34;&#62;);                                        &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnRunDialogs="ReportOnRunDialogs" PropData="">
