    <TfrxDOAQuery Name="Ins_IDOC" UserName="Ins_IDOC" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="" SQL.Text="DECLARE&#13;&#10;  vDOCDATA VARCHAR2(1000);&#13;&#10;  vIDOC VARCHAR2(1);&#13;&#10;&#13;&#10;BEGIN &#13;&#10;  vDOCDATA := 'DOCID='||:DOCID||chr(13)||&#13;&#10;              'CORID='||:CORID||chr(13)||&#13;&#10;              'REVNO='||:REVNO||chr(13)||&#13;&#10;              'EXT_ORIG='||:EXT_ORIG||chr(13)||&#13;&#10;              'EXT_CORR='||:EXT_CORR||chr(13)||&#13;&#10;              'DATE_ORIG='||TO_CHAR(TO_DATE(:DATE_ORIG, 'dd.mm.yyyy'), 'yyyy-mm-dd')||chr(13)||&#13;&#10;              'DATE_CORR='||:DATE_CORR||chr(13)||&#13;&#10;              'DATE_REV='||TO_CHAR(TO_DATE(:DATE_REV, 'dd.mm.yyyy'), 'yyyy-mm-dd')||chr(13)||&#13;&#10;              'LOCNR='||:LOCNR||chr(13)||&#13;&#10;              'NAME='||:NAME||chr(13)||&#13;&#10;              'INN='||:INN||chr(13)||&#13;&#10;              'KPP='||:KPP; &#13;&#10;&#13;&#10;  &#13;&#10;  SELECT a.VALUE&#13;&#10;  INTO vIDOC&#13;&#10;  FROM tsoptions a&#13;&#10;  WHERE a.optcode = 'CHANGE_SF_KSF_TO_SAP';&#13;&#10;&#13;&#10;  IF vIDOC = 'Y' THEN  &#13;&#10;    INSERT INTO idoc_mgrlist&#13;&#10;    (idoc_typecode, table_name, srcdobjcode,&#13;&#10;         srckeyvalue, dstdobjcode, dstkeyvalue, docstatus,&#13;&#10;         changedatetime, createdatetime, user_comment, triescount,&#13;&#10;         start_date, docdata&#13;&#10;    )&#13;&#10;    VALUES&#13;&#10;    ('EXPORT_SF_KSF_TO_SAP', 'ORDERS', 'TS',&#13;&#10;         :order_id, 'SAP', 'SF', '64',&#13;&#10;         sysdate, sysdate, '', 0,&#13;&#10;         sysdate, vDOCDATA&#13;&#10;    );&#13;&#10;&#13;&#10;    COMMIT; &#13;&#10;  END IF;&#13;&#10;END; " PropData=""/>
    <TfrxDOAQuery Name="qryINVCNGNUM_NEXT" UserName="odsNextINVCNGNUM " CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="DECLARE&#13;&#10;  max_num NUMBER;&#13;&#10;BEGIN&#13;&#10;  SELECT NVL(MAX(OPVALUE), 0)&#13;&#10;  INTO max_num&#13;&#10;  FROM OP_VALUES&#13;&#10;  WHERE ORDER_ID = :ORDER_ID&#13;&#10;    AND OP_CODE = 'INVCNGNUM';&#13;&#10;    &#13;&#10;  UPDATE OP_VALUES SET  &#13;&#10;    OPVALUE = TO_CHAR(max_num + 1)    &#13;&#10;  WHERE ORDER_ID = :ORDER_ID&#13;&#10;    AND OP_CODE = 'INVCNGNUM';&#13;&#10;    &#13;&#10;  IF SQL%ROWCOUNT = 0 THEN &#13;&#10;    INSERT INTO OP_VALUES&#13;&#10;      (order_id, og_item_id, op_code, opvalue)&#13;&#10;    VALUES&#13;&#10;      (:order_id, null, 'INVCNGNUM', TO_CHAR(max_num + 1));&#13;&#10;  END IF;  &#13;&#10;  &#13;&#10;  UPDATE OP_VALUES SET  &#13;&#10;    OPVALUE = TO_CHAR(SYSDATE, 'dd.mm.yyyy')    &#13;&#10;  WHERE ORDER_ID = :ORDER_ID&#13;&#10;    AND OP_CODE = 'INVCNGDATE';&#13;&#10;    &#13;&#10;  IF SQL%ROWCOUNT = 0 THEN &#13;&#10;    INSERT INTO OP_VALUES&#13;&#10;      (order_id, og_item_id, op_code, opvalue)&#13;&#10;    VALUES&#13;&#10;      (:order_id, null, 'INVCNGDATE', TO_CHAR(SYSDATE, 'dd.mm.yyyy'));&#13;&#10;  END IF;  &#13;&#10;  &#13;&#10;  COMMIT;&#13;&#10;END;" PropData="05704C65667403B0000470546F70030701095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsCng" UserName="odsCng" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="SELECT&#13;&#10;(SELECT get_op_values(o.ORDER_ID, null, 'INVCNGNUM') FROM DUAL) INVCNGNUM,&#13;&#10;(SELECT get_op_values(o.ORDER_ID, null, 'INVCNGDATE') FROM DUAL) INVCNGDATE&#13;&#10;FROM ORDERS o&#13;&#10;WHERE o.ORDER_ID = :ORDER_ID" PropData="05704C6566740341010470546F70030701095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsGift" UserName="odsGift" CloseDataSource="False" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000313031333234390000000000" SQL.Text="select Count(*) as GiftCount&#13;&#10;from orders o&#13;&#10;left join o_good_item ogi on ogi.order_id = o.order_id&#13;&#10;left join g_unit gu on gu.g_unit_id = ogi.g_unit_id&#13;&#10;left join specgoods sg on sg.good_code = gu.good_code&#13;&#10;where (o.ot_code = 'ZBNAL') and&#13;&#10;      (sg.st_code = 'GIFT_CARDS') and&#13;&#10;      (o.order_id = :order_id)" PropData="05704C65667403C0010470546F70030001095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2231303133323439220000"/>
