    <TfrxDOAQuery Name="odsDelivery" UserName="odsDelivery" CloseDataSource="True" FieldAliases.Text="DELIVERY=DELIVERY&#13;&#10;JOBS=JOBS" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A2A0000000300000001000000090000003A4F524445525F4944050000000900000031353835373238200000000000" SQL.Text="select (da.area_name || ' (из ' || areas.cnt || ') Окно выдачи ' || dd.title ) as delivery,&#13;&#10; (select count(*) from orders o&#13;&#10;   join orders o1 on o1.order_id = o.parent_order_id &#13;&#10;   join orders o2 on o2.parent_order_id = o1.order_id &#13;&#10;  where o.order_id = dq.order_id and o2.ot_code = 'ZJOB') as jobs&#13;&#10;from delivery_queue dq&#13;&#10; join delivery_desk dd on dq.desk_id = dd.desk_id&#13;&#10; join delivery_area da on dd.area_id = da.area_id&#13;&#10; join (select dqu.order_id, count(*) cnt&#13;&#10;         from delivery_queue dqu &#13;&#10;        where dqu.order_id = :order_id&#13;&#10;       group by dqu.order_id) areas&#13;&#10;  on dq.order_id = areas.order_id&#13;&#10;where dq.order_id = :order_id" PropData="05704C656674035C010470546F70038000095661726961626C657301010C4B0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223135383537323820220000"/>
    <TfrxDOAQuery Name="odsCardNum" UserName="odsCardNum" CloseDataSource="True" FieldAliases.Text="CARD_NMBR=CARD_NMBR" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000313538353732380000000000" SQL.Text="SELECT 'Бонусная карта № ' ||&#13;&#10;       RPAD(substr(v.opvalue, 1, 4), LENGTH(v.opvalue) - 4, 'X') || &#13;&#10;       substr(v.opvalue, LENGTH(v.opvalue) - 3, 4)&#13;&#10;       AS CARD_NMBR&#13;&#10;  FROM op_values v&#13;&#10; WHERE v.order_id = :order_id&#13;&#10;   AND v.op_code = 'CARD_NMBR'&#13;&#10;   AND v.opvalue IS NOT NULL" PropData="05704C65667403AC010470546F70038000095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2231353835373238220000"/>
    <TfrxDOAQuery Name="dstLogo" UserName="dstLogo" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="SELECT&#13;&#10;  tsdf.* --df_value&#13;&#10;FROM&#13;&#10;  tsys_datafiles tsdf,&#13;&#10;  (SELECT wt.wbltype FROM w_type wt, werk w WHERE wt.wtype_code = w.wtype_code AND w.is_base = 'Y') wtc&#13;&#10;WHERE&#13;&#10;  tsdf.df_guid IN (&#13;&#10;  '{1F73DB37-363A-4A98-8846-E9CE9857B27C}',&#13;&#10;  '{031FE881-FC68-41A7-BAF6-E342B446448B}')&#13;&#10;  AND tsdf.df_note = wtc.wbltype" PropData="05704C65667403F4010470546F70038000095661726961626C65730100"/>
