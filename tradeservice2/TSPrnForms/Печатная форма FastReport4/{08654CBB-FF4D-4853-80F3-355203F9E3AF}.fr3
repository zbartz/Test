nvl(oprnt.createusercode,o.createusercode)&#13;&#10;    and ps.id = s.p_staff_id) as salesman,&#13;&#10;  a.*&#13;&#10;from&#13;&#10;  address a, addr_title t, orders_link ol, orders o, addr_doctype dt, orders oprnt, werk w, w_type wt&#13;&#10;where&#13;&#10;  a.address_id in&#13;&#10;  ( select address_id from orders where order_id = :order_id )&#13;&#10;  and a.title_code = t.title_code(+)&#13;&#10;  and o.address_id = a.address_id&#13;&#10;  and o.order_id = :order_id&#13;&#10;  and ol.child_order_id(+) = o.order_id&#13;&#10;  and a.doctype_code = dt.doctype_code(+)&#13;&#10;  and oprnt.order_id(+) = ol.parent_order_id&#13;&#10;  and w.is_base = 'Y'&#13;&#10;  AND wt.wtype_code = w.wtype_code " PropData="05704C65667403AC010470546F700238095661726961626C657301010C460000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22313036220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="GOOD_CODE=GOOD_CODE&#13;&#10;NAME=NAME&#13;&#10;QUANTITY1=QUANTITY1&#13;&#10;QUANTITY=QUANTITY&#13;&#10;PRICE2=PRICE2&#13;&#10;AMOUNT=AMOUNT&#13;&#10;GQ_CODE=GQ_CODE&#13;&#10;STRGNAME=STRGNAME&#13;&#10;GQ_CODEEAN13=GQ_CODEEAN13&#13;&#10;GQ_CODE39=GQ_CODE39&#13;&#10;BONUS_SUM=BONUS_SUM&#13;&#10;LOYL_PAY_SUM=LOYL_PAY_SUM&#13;&#10;PAY_SUM=PAY_SUM" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A250000000300000001000000090000003A4F524445525F494405000000040000003130360000000000" SQL.Text="SELECT good_code, name, quantity1, quantity, price2, amount, gq_code, strgname, gq_codeean13, gq_code39, bonus_sum,&#13;&#10;       -- Убираем лишние рубли от округления&#13;&#10;       CASE WHEN loyl_pay_all_sum - SUM(loyl_pay_sum) OVER (ORDER BY og_item_id) &#60; 0&#13;&#10;         THEN loyl_pay_sum + loyl_pay_all_sum - SUM(loyl_pay_sum) OVER (ORDER BY og_item_id) &#13;&#10;         ELSE loyl_pay_sum&#13;&#10;         END AS loyl_pay_sum,&#13;&#10;       amount - &#13;&#10;       CASE WHEN loyl_pay_all_sum - SUM(loyl_pay_sum) OVER (ORDER BY og_item_id) &#60; 0&#13;&#10;         THEN loyl_pay_sum + loyl_pay_all_sum - SUM(loyl_pay_sum) OVER (ORDER BY og_item_id) &#13;&#10;         ELSE loyl_pay_sum&#13;&#10;         END AS pay_sum&#13;&#10;  FROM (SELECT ogi.og_item_id,&#13;&#10;               g.good_code,&#13;&#10;               g.name,&#13;&#10;               ogi.quantity AS quantity1,&#13;&#10;               tqu.quantity,&#13;&#10;               ogi.price2 AS price2,&#13;&#10;               tqu.quantity * ogi.price2 AS amount,&#13;&#10;               CASE&#13;&#10;                  WHEN ogi.st_code = 'STD' AND ogi.price1 = ogi.price2 THEN&#13;&#10;                   '22' || lpad(TRIM(g.good_code), 8, '0') ||&#13;&#10;                   TRIM(substr(TO_CHAR(tqu.quantity, '09'), 2, 2))&#13;&#10;                  WHEN (ogi.st_code &#60;&#62; 'STD' OR&#13;&#10;                       (ogi.st_code = 'STD' AND ogi.price1 &#60;&#62; ogi.price2)) THEN&#13;&#10;                   '139' || lpad(TRIM(g.good_code), 8, '0') || '01' ||&#13;&#10;                   TRIM(TO_CHAR(NVL(ogi.price2, 0) * 100, '00000009')) ||&#13;&#10;                   TRIM(TO_CHAR(tqu.quantity, '09'))&#13;&#10;                  ELSE&#13;&#10;                   '000000000000'&#13;&#10;                END AS gq_code,&#13;&#10;               '/ Склад: ' || st.shortname || ' /' AS strgname,&#13;&#10;               digittoean13('22' || lpad(TRIM(g.good_code), 8, '0') ||&#13;&#10;                            TRIM(substr(TO_CHAR(tqu.quantity, '09'), 2, 2))) AS gq_codeean13,&#13;&#10;               digittocode39('139' || lpad(TRIM(g.good_code), 8, '0') || '01' ||&#13;&#10;                             TRIM(TO_CHAR(NVL(ogi.price2, 0) * 100, '00000009')) ||&#13;&#10;                             TRIM(TO_CHAR(tqu.quantity, '09'))) AS gq_code39,&#13;&#10;               nvl(c.amount, 0) * tqu.quantity AS bonus_sum,&#13;&#10;               -- Сумма баллов приходящаяся на эту позицию с округлением до целых&#13;&#10;               CASE&#13;&#10;                 WHEN NVL(f.docsum, 0) &#62; 0 AND pk_orders.can_good_pay_loylty(g.good_code) = 'Y'&#13;&#10;                 THEN ROUND(f.docsum * ogi.amount / &#13;&#10;                   SUM(DECODE(pk_orders.can_good_pay_loylty(g.good_code), 'Y', ogi.amount, 0)) OVER ())&#13;&#10;                 ELSE 0&#13;&#10;                 END loyl_pay_sum,&#13;&#10;               NVL(f.docsum, 0) AS loyl_pay_all_sum             &#13;&#10;          FROM o_good_item ogi&#13;&#10;          JOIN &#13;&#10;           (SELECT ogit.g_unit_id, ogit.parent_og_item_id, NVL(SUM(ogit.quantity),0) as quantity&#13;&#10;            FROM o_good_item ogi&#13;&#10;            JOIN o_good_item ogit on ogit.parent_og_item_id = ogi.og_item_id &#13;&#10;            JOIN orders o on o.order_id = ogit.order_id&#13;&#10;            WHERE ogi.order_id = :order_id   &#13;&#10;             AND ogit.g_unit_id = ogi.g_unit_id&#13;&#10;             AND o.os_code = 'ZTASK_DN'&#13;&#10;             AND o.storage_code is null&#13;&#10;             AND NOT EXISTS &#13;&#10;               (SELECT NULL &#13;&#10;                  FROM op_values ov &#13;&#10;                 WHERE ov.order_id = ogit.order_id&#13;&#10;                   AND ov.opvalue in ('DVTN_WRONG', 'DVTN_DEFECT', 'DVTN_REJECT', 'DVTN_KIT_RET', 'DVTN_DEBARCADER'))&#13;&#10;                GROUP BY ogit.g_unit_id, ogit.parent_og_item_id, ogi.quantity) tqu on tqu.parent_og_item_id = ogi.og_item_id&#13;&#10;          JOIN g_unit gu&#13;&#10;            ON gu.g_unit_id = ogi.g_unit_id&#13;&#10;          JOIN good g&#13;&#10;            ON g.good_code = gu.good_code&#13;&#10;          LEFT JOIN storage st&#13;&#10;            ON st.storage_code = ogi.storage_code&#13;&#10;           AND st.werk_code = ogi.werk_code&#13;&#10;          LEFT JOIN money_fict f&#13;&#10;            ON f.order_id = ogi.order_id&#13;&#10;           AND f.mtnmbr = get_tsoptions('CRM_CARD_MTNMBR')&#13;&#10;           AND f.is_paid = 'N'&#13;&#10;          LEFT JOIN o_condition c&#13;&#10;            ON c.order_id = ogi.order_id&#13;&#10;           AND c.og_item_id = ogi.og_item_id&#13;&#10;           AND c.sapcode = 'CMSS'          &#13;&#10;         WHERE ogi.order_id = :order_id&#13;&#10;           AND ogi.ignore &#60;&#62; 'Y'&#13;&#10;           AND ogi.quantity &#62; 0&#13;&#10;           ORDER BY ogi.og_item_id) t1&#13;&#10;                  " PropData="05704C65667403C8000470546F700238095661726961626C657301010C460000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D22313036220000"/>
