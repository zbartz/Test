    <TfrxDOAQuery Name="dstTotals" UserName="dstTotals" CloseDataSource="True" FieldAliases.Text="QUANTITY=QUANTITY&#13;&#10;INTWEIGHT=INTWEIGHT&#13;&#10;INTNETWEIGHT=INTNETWEIGHT&#13;&#10;VOLUME=VOLUME&#13;&#10;CNT=CNT" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="with part as (select opv.order_id part_id, sum(ogi2.quantity) part_qnt, ogi2.g_unit_id g_unit_id, ogi2.og_item_id  from docoutnum opv&#13;&#10;          join docoutnum opv2&#13;&#10;          on opv2.order_id &#60;&#62; opv.order_id and opv2.op_code = 'SAPORDNUM' and opv2.docout_id = opv.docout_id&#13;&#10;          join orders o2&#13;&#10;          on o2.order_id = opv2.order_id and o2.ot_code = 'ZDLVGI' &#13;&#10;          join orders o3&#13;&#10;          on o3.chainnum = o2.chainnum and o3.ot_code = 'GISSUE'&#13;&#10;          join o_good_item ogi2&#13;&#10;          on ogi2.order_id = o3.order_id &#13;&#10;          join o_good_item ogi&#13;&#10;          on ogi.order_id = opv.order_id and ogi2.g_unit_id = ogi.g_unit_id &#13;&#10;          join orders o &#13;&#10;          on o.order_id = opv.order_id and o.ot_code = 'ZADDR' and o.os_code = 'ZADDR_PART'&#13;&#10;          where opv.order_id IN (SELECT zvalue1 FROM tmp_table)&#13;&#10;           and opv.op_code = 'SAPORDNUM'&#13;&#10;            and o3.os_code not in ('DOC_STORN') and o2.os_code not in ('DOC_STORN')&#13;&#10;            and ogi2.quantity &#62; 0&#13;&#10;          group by opv.order_id, ogi2.g_unit_id, ogi2.og_item_id&#13;&#10;)&#13;&#10;SELECT sum(quantity) as quantity, sum(intweight) as intweight, sum(intnetweight) as intnetweight, sum(volume) as volume, sum(cnt) as cnt  &#13;&#10;  FROM (&#13;&#10;       SELECT &#13;&#10;       tt.ot_code, tt.os_code,&#13;&#10;       case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;              SUM(p.part_qnt)  &#13;&#10;       else   SUM(ogi.quantity)&#13;&#10;       END AS quantity,&#13;&#10;       &#13;&#10;       TRUNC&#13;&#10;       (SUM( CASE WHEN gu.w_unit = 'G' THEN&#13;&#10;                      case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                           gu.weight*p.part_qnt*(0.001) &#13;&#10;                      else gu.weight*ogi.quantity*(0.001)&#13;&#10;                  END&#13;&#10;                  when gu.w_unit = 'KG' THEN&#13;&#10;                      case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                           gu.weight*p.part_qnt&#13;&#10;                      else gu.weight*ogi.quantity&#13;&#10;                      END&#13;&#10;         END)) AS intweight,&#13;&#10;       TRUNC(SUM(&#13;&#10;               CASE&#13;&#10;                 WHEN gu.w_unit = 'G'  THEN&#13;&#10;                      case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                           gu.weight_net*p.part_qnt*(0.001)&#13;&#10;                      else gu.weight_net*ogi.quantity*(0.001)&#13;&#10;                      end&#13;&#10;                 WHEN gu.w_unit = 'KG' THEN&#13;&#10;                      case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                           gu.weight_net*p.part_qnt&#13;&#10;                      else gu.weight_net*ogi.quantity&#13;&#10;                      end&#13;&#10;         END)) AS intnetweight,&#13;&#10;               TRUNC(SUM(&#13;&#10;                 case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                     ts_sys.fn_convert_unit_measure(gu.lenght, gu.lhw_unit, 'M')&#13;&#10;                     * ts_sys.fn_convert_unit_measure(gu.height, gu.lhw_unit, 'M')&#13;&#10;                     * ts_sys.fn_convert_unit_measure(gu.width, gu.lhw_unit, 'M') &#13;&#10;                     * p.part_qnt&#13;&#10;                 else&#13;&#10;                     ts_sys.fn_convert_unit_measure(gu.lenght, gu.lhw_unit, 'M')&#13;&#10;                     * ts_sys.fn_convert_unit_measure(gu.height, gu.lhw_unit, 'M')&#13;&#10;                     * ts_sys.fn_convert_unit_measure(gu.width, gu.lhw_unit, 'M') &#13;&#10;                     * ogi.quantity&#13;&#10;                 end )) AS volume,&#13;&#10;         case when tt.ot_code = 'ZADDR' and tt.os_code = 'ZADDR_PART' then&#13;&#10;                COUNT(p.og_item_id)&#13;&#10;         else   COUNT(ogi.og_item_id)&#13;&#10;         END AS cnt&#13;&#10;         FROM (SELECT o.order_id, &#13;&#10;                       o.ot_code,&#13;&#10;                       o.os_code,&#13;&#10;                       pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM') AS SAPORDNUM&#13;&#10;                  FROM orders o&#13;&#10;                 WHERE o.order_id IN (SELECT zvalue1 FROM tmp_table)&#13;&#10;--                   AND (o.outerid is not null or o.outerid &#60;&#62; '')&#13;&#10;                     /*TS-7985 -&#62; */&#13;&#10;                     AND (&#13;&#10;                          (o.haslocalitems = 'Y')&#13;&#10;                          OR&#13;&#10;                          (&#13;&#10;                           (o.hasremoteitems = 'Y')&#13;&#10;                           AND&#13;&#10;                           EXISTS (&#13;&#10;                                   SELECT 1 FROM orders zdlvgi&#13;&#10;                                            LEFT JOIN docoutnum ov_on on ov_on.order_id = zdlvgi.order_id&#13;&#10;                                    WHERE zdlvgi.ot_code = 'ZDLVGI'&#13;&#10;                                      AND ov_on.op_code = 'SAPORDNUM'&#13;&#10;                                      AND ov_on.docout_id = pk_common.GetDocOutNum(o.parent_order_id,'SAPORDNUM')&#13;&#10;                                  )&#13;&#10;                          )&#13;&#10;                        )&#13;&#10;                     /*TS-7985 &#60;- */&#13;&#10;                   AND (o.os_code IN ('ZADDR_CRTD', 'ZADDR_DLVR', 'ZADDR_PART', 'GISSUE_CR')&#13;&#10;      OR o.ot_code IN ('ZPRTRK','RTWORC','ZDELR','ZRTRN','ZDLVGI'))&#13;&#10;      AND NOT EXISTS (select 1 from op_values where ORDER_ID = o.parent_order_id and op_code='DLV_VAR' &#13;&#10;                                  and opvalue='Доставка в магазин для самовывоза')&#13;&#10;                UNION ALL&#13;&#10;                SELECT o.order_id, &#13;&#10;                       o.ot_code,&#13;&#10;                       o.os_code,&#13;&#10;                       pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM') AS SAPORDNUM&#13;&#10;                  FROM orders o &#13;&#10;                 WHERE o.order_id = :ORDER_ID&#13;&#10;--                   AND (o.outerid is not null or o.outerid &#60;&#62; '')&#13;&#10;                     /*TS-7985 -&#62; */&#13;&#10;                     AND (&#13;&#10;                          (o.haslocalitems = 'Y')&#13;&#10;                          OR&#13;&#10;                          (&#13;&#10;                           (o.hasremoteitems = 'Y')&#13;&#10;                           AND&#13;&#10;                           EXISTS (&#13;&#10;                                   SELECT 1 FROM orders zdlvgi&#13;&#10;                                            LEFT JOIN docoutnum ov_on on ov_on.order_id = zdlvgi.order_id&#13;&#10;                                    WHERE zdlvgi.ot_code = 'ZDLVGI'&#13;&#10;                                      AND ov_on.op_code = 'SAPORDNUM'&#13;&#10;                                      AND ov_on.docout_id = pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM')&#13;&#10;                                  )&#13;&#10;                          )&#13;&#10;                        )&#13;&#10;                     /*TS-7985 &#60;- */&#13;&#10;                   AND NOT EXISTS (SELECT 1 FROM tmp_table)&#13;&#10;                   AND o.os_code IN ('DOC_STORN', 'ZADDR_DLVR','GISSUE_CR','GRCPT_CR','ZDLVGR_CR','ZDLVGR_FIN','ZDLVGI_FIN','ZDLVGI_CR')&#13;&#10;                   AND NOT EXISTS (select 1 from op_values where ORDER_ID = o.parent_order_id and op_code='DLV_VAR' &#13;&#10;                                  and opvalue='Доставка в магазин для самовывоза')&#13;&#10;               ) tt &#13;&#10;          JOIN o_good_item ogi&#13;&#10;            ON ogi.order_id = tt.order_id&#13;&#10;          JOIN g_unit gu&#13;&#10;            ON gu.g_unit_id = ogi.g_unit_id&#13;&#10;          JOIN good g&#13;&#10;            ON g.good_code = gu.good_code&#13;&#10;           AND g.g_type_code &#60;&#62; 'DIEN'&#13;&#10;          left JOIN part p&#13;&#10;            ON p.part_id = tt.order_id and gu.g_unit_id = p.g_unit_id&#13;&#10;       group by tt.ot_code, tt.os_code&#13;&#10;       ) Z&#13;&#10; WHERE quantity IS NOT NULL" PropData="05704C65667402300470546F700274095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="dstCarrier" UserName="dstCarrier" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A200000000300000001000000080000003A43415252494552050000000000000000000000" SQL.Text="SELECT&#13;&#10;  c.basename,&#13;&#10;  ad.post_code||', '||ad.city||', '||TS_INFO.getaddressstring(c.address_id) AS addr,&#13;&#10;  abn.bankname,&#13;&#10;  abn.inn ,&#13;&#10;  abn.kpp ,&#13;&#10;  abn.bik ,&#13;&#10;  abn.sacnt ,&#13;&#10;  abn.koracnt&#13;&#10;FROM contragents c, addr_bninfo abn, address ad&#13;&#10;WHERE c.contragent_id = (select PK_PRINT.GET_ACTUAL_CONTRAGENT(c.contragent_id)&#13;&#10;                           from contragents c, contragents_roles cr&#13;&#10;                          where cr.cnttype_code='CARRIER'&#13;&#10;                            and cr.contragent_id=c.contragent_id&#13;&#10;                            and c.contragent_id||' '||c.basename = :Carrier)&#13;&#10;  AND abn.address_id = c.address_id&#13;&#10;  AND abn.address_id = ad.address_id" PropData="05704C6566740394000470546F7003C000095661726961626C657301010C390000002044617461547970653D226674537472696E6722204E616D653D22434152524945522220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsConsegnee" UserName="odsConsegnee" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A290000000300000001000000090000003A4F524445525F49440500000008000000313430373339340000000000" SQL.Text="select&#13;&#10;  c.basename as name,&#13;&#10;  a.post_code || ', '&#13;&#10;  || a.city || ', '&#13;&#10;  || a.street &#13;&#10;  || case when not a.HOUSE_NUM is null then ', д.' || a.house_num end&#13;&#10;  || case when not a.HOUSE_CORP is null then ', корп.' || a.HOUSE_CORP end&#13;&#10;  || case when not a.building is null then ', стр.' || a.building end&#13;&#10;  || case when not a.room_number is null then ', к.' || a.room_number end as address,&#13;&#10;  c.inn,&#13;&#10;  c.kpp,&#13;&#10;  c.okato,&#13;&#10;  ' ' || 'ИНН/КПП: ' || abn.inn || '/' || abn.kpp || abn.BANKNAME || ' БИК: ' || abn.BIK ||  ' Р/С: ' || abn.SACNT || ' Кор/С: ' || decode(abn.KORACNT,'0',' ',abn.KORACNT) as binfo,&#13;&#10;  abn.*,&#13;&#10;  (select ac.value from addr_comm ac&#13;&#10;    where ac.address_id=a.address_id&#13;&#10;      and ac.ct_code='TEL'&#13;&#10;      and rownum=1) as tel&#13;&#10;from address a, addr_bninfo abn, contragents c, o_contragents oc, orders o&#13;&#10;where o.order_id = :ORDER_ID&#13;&#10;  --and o.ot_code = 'GISSUE'&#13;&#10;  and (oc.order_id = o.parent_order_id or oc.order_id = o.order_id)&#13;&#10;  and oc.cnttype_code in  ('CONSIGNEE')&#13;&#10;  and c.contragent_id = PK_PRINT.GET_ACTUAL_CONTRAGENT(oc.contragent_id)&#13;&#10;  and a.address_id = c.address_id&#13;&#10;  and abn.address_id(+) = c.address_id&#13;&#10;order by oc.cnttype_code desc   " PropData="05704C656674032C010470546F7003A400095661726961626C657301010C4A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D2231343037333934220000"/>
