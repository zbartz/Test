    <TfrxDOAQuery Name="qryGetNextNum" UserName="qryGetNextNum" CloseDataSource="True" FieldAliases.Text="TTNNUM=TTNNUM&#13;&#10;TTNREGNUM=TTNREGNUM" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" Params.PropData="04446174610A230000000300000001000000070000003A5245544E554D0300000004000000B700000000000000" SQL.Text="begin&#13;&#10;  :RetNum := TS_DOCNUMBERS.GetNextNumber( 'ORDERS', 'TTNNUM', 'ZADDR','Y' );&#13;&#10;end;" PropData="05704C6566740374010470546F700238095661726961626C657301010C450000002044617461547970653D226674496E746567657222204E616D653D225245544E554D2220506172616D547970653D227074556E6B6E6F776E222056616C75653D22313833220000"/>
    <TfrxDOAQuery Name="odsNum" UserName="odsNum" CloseDataSource="True" FieldAliases.Text="TTNNUM=TTNNUM&#13;&#10;TRNNUM=TRNNUM" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="SELECT MAX(num.cur_number) AS TTNNUM, &#13;&#10;MAX(rtrnnum.cur_number) AS TRNNUM&#13;&#10;  FROM o_docnumbers num, o_docnumbers rtrnnum&#13;&#10; WHERE num.dn_tablename = 'ORDERS'&#13;&#10;AND num.op_code = 'TTNNUM'&#13;&#10;AND num.ot_code = 'ZADDR'&#13;&#10;AND rtrnnum.dn_tablename = 'ORDERS'&#13;&#10;AND rtrnnum.op_code = 'TTNNUM'&#13;&#10;AND rtrnnum.ot_code = 'ZRTRN'" PropData="05704C65667403A4010470546F700274095661726961626C65730100"/>
    <TfrxDOAQuery Name="dstReg" UserName="dstReg" CloseDataSource="True" FieldAliases.Text="RN=RN&#13;&#10;CREATEDATETIME=CREATEDATETIME&#13;&#10;ORDER_ID=ORDER_ID&#13;&#10;Z_ORDER_ID=Z_ORDER_ID&#13;&#10;DESIREDDT=DESIREDDT&#13;&#10;OT_CODE=OT_CODE&#13;&#10;O_AMNT=O_AMNT&#13;&#10;QUANTITY=QUANTITY&#13;&#10;GOOD_CODE=GOOD_CODE&#13;&#10;WARENAME=WARENAME&#13;&#10;FIO=FIO&#13;&#10;ADDRSTR=ADDRSTR&#13;&#10;CITY=CITY&#13;&#10;DISTRICT=DISTRICT&#13;&#10;STREET=STREET&#13;&#10;HOUSE=HOUSE&#13;&#10;BUILDING=BUILDING&#13;&#10;ROOM=ROOM&#13;&#10;LOT_VALUE=LOT_VALUE&#13;&#10;LOT_COUNTRY=LOT_COUNTRY" BCDToCurrency="False" Master="dstMain" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="-- reestr PF&#13;&#10;with part as (select opv.order_id part_id, sum(ogi2.quantity) part_qnt, ogi2.g_unit_id g_unit_id from docoutnum opv&#13;&#10;          join docoutnum opv2&#13;&#10;          on opv2.order_id &#60;&#62; opv.order_id and opv2.op_code = 'SAPORDNUM' and opv2.docout_id = opv.docout_id&#13;&#10;          join orders o2&#13;&#10;          on o2.order_id = opv2.order_id and o2.ot_code = 'ZDLVGI' &#13;&#10;          join orders o3&#13;&#10;          on o3.chainnum = o2.chainnum and o3.ot_code = 'GISSUE'&#13;&#10;          join o_good_item ogi2&#13;&#10;          on ogi2.order_id = o3.order_id &#13;&#10;          join o_good_item ogi&#13;&#10;          on ogi.order_id = opv.order_id and ogi2.g_unit_id = ogi.g_unit_id &#13;&#10;          join orders o &#13;&#10;          on o.order_id = opv.order_id and o.ot_code = 'ZADDR' and o.os_code = 'ZADDR_PART'&#13;&#10;          where opv.order_id IN (SELECT zvalue1 FROM tmp_table)&#13;&#10;           and opv.op_code = 'SAPORDNUM'&#13;&#10;            and o3.os_code not in ('DOC_STORN') and o2.os_code not in ('DOC_STORN')&#13;&#10;          group by opv.order_id, ogi2.g_unit_id&#13;&#10;)&#13;&#10;&#13;&#10;select rl.*&#13;&#10;from&#13;&#10;(SELECT /*+rule */&#13;&#10;  ROWNUM AS rn,&#13;&#10;  O.CREATEDATETIME,&#13;&#10;  o.ORDER_ID AS ORDER_ID,&#13;&#10;  Pk_Orders.GetOrderId(o.order_id, 'ZORDR') as z_order_id,&#13;&#10;  O.DesiredDT AS DesiredDT,&#13;&#10;  o.ot_code AS ot_code,&#13;&#10;  o.os_code as os_code,&#13;&#10;  CASE&#13;&#10;    WHEN O.OS_CODE='ZADDR_STRN' THEN -GI.AMOUNT&#13;&#10;    ELSE GI.AMOUNT&#13;&#10;  END AS O_AMNT,&#13;&#10;  CASE&#13;&#10;    WHEN O.OS_CODE='ZADDR_STRN' THEN -GI.QUANTITY&#13;&#10;    ELSE GI.QUANTITY&#13;&#10;  END AS QUANTITY,&#13;&#10;  G.GOOD_CODE AS GOOD_CODE,&#13;&#10;  G.NAME AS WARENAME,&#13;&#10;  A.NAMEBASE AS FIO,&#13;&#10;  TS_INFO.GetAddressString( O.ADDRESS_ID ) AS AddrStr,&#13;&#10;  A.CITY AS CITY,&#13;&#10;  A.DISTRICT AS DISTRICT,&#13;&#10;  A.STREET AS STREET,&#13;&#10;  A.HOUSE_NUM AS HOUSE,&#13;&#10;  A.BUILDING AS BUILDING,&#13;&#10;  A.ROOM_NUMBER AS ROOM,&#13;&#10;  (SELECT MAX(lot_value) FROM lot l, ogi_lot ogl&#13;&#10;    WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_value,&#13;&#10;  (SELECT MAX(country) FROM lot l, ogi_lot ogl&#13;&#10;    WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_country&#13;&#10;FROM orders o, o_good_item GI, GOOD G, G_UNIT GU, ADDRESS A&#13;&#10;     ,(SELECT zvalue1 FROM tmp_table GROUP BY zvalue1) t&#13;&#10;WHERE o.order_id = t.zvalue1&#13;&#10;--  AND (o.outerid is not null or o.outerid &#60;&#62; '')&#13;&#10;    /*TS-7985 -&#62; */&#13;&#10;    AND (&#13;&#10;         (o.haslocalitems = 'Y')&#13;&#10;         OR&#13;&#10;         (&#13;&#10;          (o.hasremoteitems = 'Y')&#13;&#10;          AND&#13;&#10;          EXISTS (&#13;&#10;                  SELECT 1 FROM orders zdlvgi&#13;&#10;                  LEFT JOIN docoutnum ov_on on ov_on.order_id = zdlvgi.order_id&#13;&#10;                  WHERE zdlvgi.ot_code = 'ZDLVGI'&#13;&#10;                        AND ov_on.op_code = 'SAPORDNUM'&#13;&#10;                        AND ov_on.docout_id = pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM')&#13;&#10;                 )&#13;&#10;          )&#13;&#10;        )&#13;&#10;    /*TS-7985 &#60;- */&#13;&#10;  AND GI.IGNORE&#60;&#62;'Y'&#13;&#10;  AND O.ORDER_ID= GI.ORDER_ID&#13;&#10;  AND GI.G_UNIT_ID=GU.G_UNIT_ID&#13;&#10;  AND GU.GOOD_CODE=G.GOOD_CODE&#13;&#10;  AND A.ADDRESS_ID (+) = O.ADDRESS_ID&#13;&#10;  AND GI.QUANTITY &#60;&#62; 0&#13;&#10;  AND o.ot_code &#60;&#62; 'ZADDR'  &#13;&#10;  AND g.g_type_code &#60;&#62; 'DIEN'     &#13;&#10;&#13;&#10;&#13;&#10;UNION ALL&#13;&#10;SELECT /*+rule */&#13;&#10;  ROWNUM AS rn,&#13;&#10;  O.CREATEDATETIME,&#13;&#10;  o.ORDER_ID AS ORDER_ID,&#13;&#10;  Pk_Orders.GetOrderId(o.order_id, 'ZORDR') as z_order_id,&#13;&#10;  O.DesiredDT AS DesiredDT,&#13;&#10;  o.ot_code AS ot_code,&#13;&#10;  o.os_code as os_code,&#13;&#10;  GI.quantity*round( GI.price2, 2 ) AS O_AMNT,&#13;&#10;  GI.QUANTITY AS QUANTITY,&#13;&#10;  G.GOOD_CODE AS GOOD_CODE,&#13;&#10;  G.NAME AS WARENAME,&#13;&#10;  A.NAMEBASE AS FIO,&#13;&#10;  TS_INFO.GetAddressString( O.ADDRESS_ID ) AS AddrStr,&#13;&#10;  A.CITY AS CITY,&#13;&#10;  A.DISTRICT AS DISTRICT,&#13;&#10;  A.STREET AS STREET,&#13;&#10;  A.HOUSE_NUM AS HOUSE,&#13;&#10;  A.BUILDING AS BUILDING,&#13;&#10;  A.ROOM_NUMBER AS ROOM,&#13;&#10;  (SELECT MAX(lot_value) FROM lot l, ogi_lot ogl&#13;&#10;   WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_value,&#13;&#10;  (SELECT MAX(country) FROM lot l, ogi_lot ogl&#13;&#10;   WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_country&#13;&#10;FROM orders o, o_good_item GI, GOOD G, G_UNIT GU, ADDRESS A&#13;&#10;WHERE o.order_id = :order_id&#13;&#10;  --and o.ot_code='GISSUE'&#13;&#10;  --AND GI.IGNORE&#60;&#62;'Y'&#13;&#10;--  AND (o.outerid is not null or o.outerid &#60;&#62; '')&#13;&#10;    /*TS-7985 -&#62; */&#13;&#10;    AND (&#13;&#10;         (o.haslocalitems = 'Y')&#13;&#10;         OR&#13;&#10;         (&#13;&#10;          (o.hasremoteitems = 'Y')&#13;&#10;          AND&#13;&#10;          EXISTS (&#13;&#10;                  SELECT 1 FROM orders zdlvgi&#13;&#10;                  LEFT JOIN docoutnum ov_on on ov_on.order_id = zdlvgi.order_id&#13;&#10;                  WHERE zdlvgi.ot_code = 'ZDLVGI'&#13;&#10;                        AND ov_on.op_code = 'SAPORDNUM'&#13;&#10;                        AND ov_on.docout_id =  pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM')&#13;&#10;                 )&#13;&#10;          )&#13;&#10;        )&#13;&#10;    /*TS-7985 &#60;- */&#13;&#10;  AND O.ORDER_ID= GI.ORDER_ID&#13;&#10;  AND GI.G_UNIT_ID=GU.G_UNIT_ID&#13;&#10;  AND GU.GOOD_CODE=G.GOOD_CODE&#13;&#10;  AND A.ADDRESS_ID (+) = O.ADDRESS_ID&#13;&#10;  AND GI.QUANTITY &#60;&#62; 0&#13;&#10;  AND NOT EXISTS (SELECT zvalue1 FROM tmp_table)&#13;&#10;  AND g.g_type_code &#60;&#62; 'DIEN'     &#13;&#10;&#13;&#10;&#13;&#10;UNION ALL&#13;&#10;SELECT /*+rule */&#13;&#10;  ROWNUM AS rn,&#13;&#10;  O.CREATEDATETIME,&#13;&#10;  o.ORDER_ID AS ORDER_ID,&#13;&#10;  Pk_Orders.GetOrderId(o.order_id, 'ZORDR') as z_order_id,&#13;&#10;  O.DesiredDT AS DesiredDT,&#13;&#10;  o.ot_code AS ot_code,&#13;&#10;  o.os_code as os_code,&#13;&#10;  CASE&#13;&#10;    WHEN O.OS_CODE='ZADDR_STRN' THEN -GI.AMOUNT&#13;&#10;    ELSE GI.AMOUNT&#13;&#10;  END AS O_AMNT,&#13;&#10;  CASE&#13;&#10;    WHEN O.OS_CODE='ZADDR_STRN' THEN -GI.QUANTITY&#13;&#10;    WHEN O.OT_CODE='ZADDR' AND O.OS_CODE = 'ZADDR_PART' THEN p.part_qnt&#13;&#10;    ELSE GI.QUANTITY&#13;&#10;  END AS QUANTITY,&#13;&#10;  G.GOOD_CODE AS GOOD_CODE,&#13;&#10;  G.NAME AS WARENAME,&#13;&#10;  A.NAMEBASE AS FIO,&#13;&#10;  TS_INFO.GetAddressString( O.ADDRESS_ID ) AS AddrStr,&#13;&#10;  A.CITY AS CITY,&#13;&#10;  A.DISTRICT AS DISTRICT,&#13;&#10;  A.STREET AS STREET,&#13;&#10;  A.HOUSE_NUM AS HOUSE,&#13;&#10;  A.BUILDING AS BUILDING,&#13;&#10;  A.ROOM_NUMBER AS ROOM,&#13;&#10;  (SELECT MAX(lot_value) FROM lot l, ogi_lot ogl&#13;&#10;    WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_value,&#13;&#10;  (SELECT MAX(country) FROM lot l, ogi_lot ogl&#13;&#10;    WHERE l.lot_id = ogl.lot_id AND ogl.og_item_id = GI.OG_ITEM_ID) AS lot_country&#13;&#10;FROM orders o&#13;&#10;     join o_good_item GI&#13;&#10;     on O.ORDER_ID= GI.ORDER_ID AND GI.IGNORE&#60;&#62;'Y' AND GI.QUANTITY &#60;&#62; 0&#13;&#10;     join G_UNIT GU&#13;&#10;     on GI.G_UNIT_ID=GU.G_UNIT_ID&#13;&#10;     join GOOD G&#13;&#10;     on GU.GOOD_CODE=G.GOOD_CODE &#13;&#10;     &#13;&#10;     join&#13;&#10;     (SELECT zvalue1 FROM tmp_table GROUP BY zvalue1) t&#13;&#10;    on o.order_id = t.zvalue1&#13;&#10;    left join ADDRESS A&#13;&#10;     on A.ADDRESS_ID = O.ADDRESS_ID&#13;&#10;    left join part p&#13;&#10;     on p.part_id = o.order_id and gi.g_unit_id = p.g_unit_id&#13;&#10;    &#13;&#10;WHERE o.os_code in ('ZADDR_CRTD', 'ZADDR_DLVR', 'ZADDR_PART') &#13;&#10;--  AND (o.outerid is not null or o.outerid &#60;&#62; '')&#13;&#10;    /*TS-7985 -&#62; */&#13;&#10;    AND (&#13;&#10;         (o.haslocalitems = 'Y')&#13;&#10;         OR&#13;&#10;         (&#13;&#10;          (o.hasremoteitems = 'Y')&#13;&#10;          AND&#13;&#10;          EXISTS (&#13;&#10;                  SELECT 1 FROM orders zdlvgi&#13;&#10;                  LEFT JOIN docoutnum ov_on on ov_on.order_id = zdlvgi.order_id&#13;&#10;                  WHERE zdlvgi.ot_code = 'ZDLVGI'&#13;&#10;                        AND ov_on.op_code = 'SAPORDNUM'&#13;&#10;                        AND ov_on.docout_id =  pk_common.GetDocOutNum(o.parent_order_id, 'SAPORDNUM')&#13;&#10;                 )&#13;&#10;          )&#13;&#10;        )&#13;&#10;    /*TS-7985 &#60;- */&#13;&#10;  AND g.g_type_code &#60;&#62; 'DIEN'     &#13;&#10;&#13;&#10;ORDER BY 2 ASC) rl&#13;&#10;&#13;&#10;where (not exists (select 1&#13;&#10;  from orders o, op_values ov&#13;&#10; where o.order_id = rl.order_id&#13;&#10;   and ov.order_id = o.parent_order_id&#13;&#10;   and op_code = 'DLV_VAR'&#13;&#10;   and opvalue = 'Доставка в магазин для самовывоза'&#13;&#10;))&#13;&#10;and (&#13;&#10;       (&#13;&#10;               (rl.ot_code not in ('ZADDR')) or (rl.os_code not in ('ZADDR_PART'))&#13;&#10;       )&#13;&#10;    or&#13;&#10;       (&#13;&#10;               exists (&#13;&#10;                           --&#13;&#10;                           select 1 from docoutnum opv&#13;&#10;                           join docoutnum opv2&#13;&#10;                          on opv2.order_id &#60;&#62; opv.order_id and opv2.op_code = 'SAPORDNUM' and opv2.docout_id = opv.docout_id&#13;&#10;                           join orders o2&#13;&#10;                          on o2.order_id = opv2.order_id and o2.ot_code = 'ZDLVGI'&#13;&#10;                           join orders o3&#13;&#10;                          on o3.chainnum = o2.chainnum and o3.ot_code = 'GISSUE'&#13;&#10;                           join o_good_item ogi2&#13;&#10;                          on ogi2.order_id = o3.order_id and ogi2.quantity &#62; 0 --and ogi2.g_unit_id = GI.G_UNIT_ID&#13;&#10;                           join g_unit g_u&#13;&#10;                          on g_u.g_unit_id = ogi2.g_unit_id&#13;&#10;                           join good gd&#13;&#10;                          on G_U.GOOD_CODE= gd.GOOD_CODE&#13;&#10;                          where opv.order_id = rl.order_id and opv.op_code = 'SAPORDNUM' and gd.good_code = rl.GOOD_CODE&#13;&#10;                                              and o3.os_code not in ('DOC_STORN')  and o2.os_code not in ('DOC_STORN')&#13;&#10;&#13;&#10;                      )&#13;&#10;       )&#13;&#10;    )" PropData="05704C656674022C0470546F7003F800095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
    <TfrxDOAQuery Name="odsOrder" UserName="odsOrder" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A210000000300000001000000090000003A4F524445525F4944050000000000000000000000" SQL.Text="select t.*, substr(t.SAPORDNUM,0,2) as sapordcode&#13;&#10;from (select o.*,&#13;&#10;       nvl(pk_common.GetDocOutNum(o.parent_order_id,'SAPORDNUM'),&#13;&#10;       pk_common.GetDocOutNum(o.order_id,'SAPORDNUM')) as SAPORDNUM,&#13;&#10;       'N' AS multy&#13;&#10;  from orders o &#13;&#10; where o.order_id=:order_id&#13;&#10;   and not exists (SELECT zvalue1 FROM tmp_table)&#13;&#10;   and not exists (select 1&#13;&#10;  from orders o, op_values ov&#13;&#10; where o.order_id = :order_id&#13;&#10;   and ov.order_id = o.parent_order_id&#13;&#10;   and op_code = 'DLV_VAR'&#13;&#10;   and opvalue = 'Доставка в магазин для самовывоза')&#13;&#10;union all&#13;&#10;select o.*,&#13;&#10;       null as SAPORDNUM,&#13;&#10;       'Y' AS multy&#13;&#10;  from orders o --, tmp_table &#13;&#10; where o.order_id=(SELECT max(zvalue1) FROM tmp_table)) t&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;/*select o.*,&#13;&#10;       get_op_values(o.parent_order_id,null,'SAPORDNUM') as SAPORDNUM,&#13;&#10;       NVL( (SELECT MAX('Y') FROM tmp_table), 'N' ) AS multy&#13;&#10;  from orders o &#13;&#10; where o.order_id=:order_id&#13;&#10;*/" PropData="05704C65667402440470546F70034C01095661726961626C657301010C3A0000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
