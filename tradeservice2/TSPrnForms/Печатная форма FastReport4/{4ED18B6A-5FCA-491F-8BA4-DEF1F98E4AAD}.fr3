    <TfrxDOAQuery Name="odsAddr" UserName="odsAddr" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="select&#13;&#10;  o.ot_code AS OT_CODE,&#13;&#10;  w.werk_code||' \ '||o.ORDER_ID as ORDER_ID_ZORDR,&#13;&#10;  w.werk_code||' \ '||oprnt.order_id as ORDER_ID_ZADDR,&#13;&#10;  case when (o.DESIREDDT is null) then&#13;&#10;  (select opvalue from op_values where order_id = o.order_id&#13;&#10;  and og_item_id is null and op_code = 'DLVDATE') else to_char(o.DESIREDDT, 'dd.mm.yyyy') end as DESIREDDT,&#13;&#10;  a.POST_CODE || case when not a.POST_CODE is null then ', ' else '' end ||&#13;&#10;  a.REGION || case when not a.REGION is null then ', ' else '' end ||&#13;&#10;  a.CITY || case when not a.CITY is null then ', ' else '' end ||&#13;&#10;  a.DISTRICT || case when not a.DISTRICT is null then ', ' else '' end ||&#13;&#10;  a.STREET || case when not a.STREET is null then ', ' else '' end ||&#13;&#10;  a.HOUSE_NUM || case when not a.HOUSE_NUM is null then ', ' else '' end ||&#13;&#10;  case when not a.house_corp is null then 'корп. '||a.house_corp||', ' else '' end ||&#13;&#10;  case when not a.BUILDING is null then 'стр. '||a.BUILDING||', ' else '' end ||&#13;&#10;  case when not a.ROOM_NUMBER is null then 'кв.' else '' end || a.ROOM_NUMBER&#13;&#10;    AS FULLADDR,&#13;&#10;  t.NAME||' '||a.namebase AS FULLNAME,&#13;&#10;  a.PASSPORTNUMBER||', '||a.PASSPORTGIVEN as PSPRTINFO,&#13;&#10;  dt.name||': '||a.DOCVALUE as DOCINFO,&#13;&#10;  '(Документ доставки № '||o.ORDER_ID||' )' as PRNT_INFO,&#13;&#10;  (select ps.family||' '||substr(ps.fname,0,1)||'.'||substr(ps.lname,0,1)||'.'&#13;&#10;   from staff s, p_staff ps&#13;&#10;  where s.user_code = nvl(oprnt.createusercode,o.createusercode)&#13;&#10;    and ps.id = s.p_staff_id) as salesman,&#13;&#10;  a.*&#13;&#10;from&#13;&#10;  address a, addr_title t, orders_link ol, orders o, addr_doctype dt, orders oprnt, werk w&#13;&#10;where&#13;&#10;  a.address_id in&#13;&#10;  ( select address_id from orders where order_id = :order_id )&#13;&#10;  and a.title_code = t.title_code(+)&#13;&#10;  and o.address_id = a.address_id&#13;&#10;  and o.order_id = :order_id&#13;&#10;  and ol.child_order_id(+) = o.order_id&#13;&#10;  and a.doctype_code = dt.doctype_code(+)&#13;&#10;  and oprnt.order_id(+) = ol.parent_order_id&#13;&#10;  and w.is_base = 'Y'" PropData="05704C65667403AC010470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsGItems" UserName="odsGItems" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  g.good_code, g.name, ogi.quantity,&#13;&#10;  TO_CHAR(ogi.price2,'999999990D00') AS price2,&#13;&#10;  TO_CHAR((ogi.quantity*ogi.price2),'999999990D00') AS sum,&#13;&#10;  CASE&#13;&#10;    WHEN ogi.st_code='STD' AND ogi.price1 = ogi.price2-- AND 1=0 -- SPECIAL TEMPORARY CONDITION&#13;&#10;    THEN&#13;&#10;      '22'||lpad(trim(g.good_code),8,'0')&#13;&#10;      ||trim(substr(to_char(ogi.quantity,'09'),2,2))&#13;&#10;    WHEN (ogi.st_code&#60;&#62;'STD' OR (ogi.st_code='STD' AND ogi.price1 &#60;&#62; ogi.price2))-- OR 1=1 -- SPECIAL TEMPORARY CONDITION&#13;&#10;    --  AND NOT&#13;&#10;    --  (SELECT gb_code FROM g_barcode WHERE good_unit_id = gu.g_unit_id AND bc_format='E2')&#13;&#10;    --  IS NULL&#13;&#10;    THEN&#13;&#10;      '139'||lpad(trim(g.good_code),8,'0')||'01'||&#13;&#10;      TRIM(to_char( NVL(ogi.price2,0)*100,'00000009'))||&#13;&#10;      TRIM(to_char( ogi.quantity, '09') )&#13;&#10;    ELSE '000000000000'&#13;&#10;  END&#13;&#10;AS GQ_CODE,&#13;&#10;  '/ Склад: '||st.shortname||' /' as STRGNAME,&#13;&#10;  DIGITTOEAN13('22'||lpad(trim(g.good_code),8,'0')&#13;&#10;    ||trim(substr(to_char(ogi.quantity,'09'),2,2))) as GQ_CODEEAN13,&#13;&#10;  DIGITTOCODE39('139'||lpad(trim(g.good_code),8,'0')||'01'||&#13;&#10;    TRIM(to_char( NVL(ogi.price2,0)*100,'00000009'))||&#13;&#10;    TRIM(to_char( ogi.quantity, '09') )) as GQ_CODE39&#13;&#10;FROM&#13;&#10;  good g, o_good_item ogi, g_unit gu, storage st&#13;&#10;WHERE&#13;&#10;  gu.g_unit_id = ogi.g_unit_id&#13;&#10;  AND gu.good_code = g.good_code&#13;&#10;  AND ogi.order_id = :order_id&#13;&#10;  AND ogi.ignore &#60;&#62; 'Y'&#13;&#10;  AND st.storage_code(+) = ogi.storage_code&#13;&#10;  AND st.werk_code(+) = ogi.werk_code&#13;&#10;ORDER BY ogi.og_item_id" PropData="05704C65667403C8000470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsAComm" UserName="odsAComm" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="odsAddr" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="address_id" Params.PropData="04446174610A2300000003000000010000000B0000003A414444524553535F4944050000000000000000000000" SQL.Text="select&#13;&#10;  ct.name,&#13;&#10;  ac.value,&#13;&#10;  case&#13;&#10;    when not ac.additional_info is null then ' / '||ac.additional_info||' /'&#13;&#10;  end as additional_info&#13;&#10;from&#13;&#10;  addr_comm ac, comm_type ct&#13;&#10;where&#13;&#10;  ac.address_id = :address_id&#13;&#10;  and ac.ct_code = ct.ct_code&#13;&#10;  and ac.ct_code &#60;&#62; 'INT'&#13;&#10;order by ac.comm_priority asc" PropData="05704C65667403F4010470546F700238095661726961626C657301010C3C0000002044617461547970653D226674537472696E6722204E616D653D22414444524553535F49442220506172616D547970653D227074556E6B6E6F776E220000"/>
