    <TfrxDOAQuery Name="odsPrnMacroSubst" UserName="odsPrnMacroSubst" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="select mvalue&#13;&#10;  from prn_macrosubst&#13;&#10; where mcode = 'DELIVERY_COND'" PropData="05704C656674034C020470546F700238095661726961626C65730100"/>
    <TfrxDOAQuery Name="odsInfo" UserName="odsInfo" CloseDataSource="True" FieldAliases.Text="MCODE=MCODE&#13;&#10;MCOMMENT=MCOMMENT&#13;&#10;MOEMTYPE=MOEMTYPE&#13;&#10;MVALUE=MVALUE" BCDToCurrency="False" Session="DBM.OraSession" Active="False" Debug="False" SQL.Text="select '                                                                  &#13;&#10;                                                                       Отказ от товара надлежащего качества&#13;&#10;Согласно ст. 25 и 26.1 п.4 Закона РФ &#34;О защите прав потребителей&#34; при дистанционной торговле (в т.ч. продажи через Интернет) потребитель вправе отказаться от товара надлежащего качества в течение СЕМИ ДНЕЙ.&#13;&#10;Если к товару был приложен подарок, он должен быть передан вместе с товаром, при этом подарок тоже не должен иметь следов эксплуатации, должен быть сохранен его товарный вид, потребительские свойства, а также фабричная упаковка.&#13;&#10;Если при рассмотрении Вашего требования о возврате товара будет установлено, что товар имеет дефекты (трещины, царапины, сколы, механические повреждения . лжен быть сохранен его товарный вид, потребительские свойства, а также фабричная упаковка.за исключением скрытых производственных дефектов), находится в неполной комплектации или не в заводской упаковке, то обмен (возврат) произведен не будет. Не производится обмен товара надлежащего качества, &#13;&#10;имеющего индивидуально-определенные свойства, если указанный товар может быть использован исключительно приобретающим его потребителем.&#13;&#10;                                                                       Отказ от товара ненадлежащего качества&#13;&#10;Замена или возврат товара с выявленным в процессе эксплуатации существенным недостатком* производится на общих основаниях и в сроки, установленные Законом РФ &#34;О защите прав потребителей&#34;, только при наличии заключения авторизованного сервисного центра (номера телефонов указаны в гарантийном талоне). На основании данного заключения по Вашему требованию мы обменяем этот товар на аналогичный, или вернем уплаченные Вами за товар деньги.&#13;&#10;В случае если в результате экспертизы товара установлено, что его недостатки возникли по вине Покупателя, закон предусматривает обязанность Покупателя возместить продавцу расходы, связанные с проведением экспертизы. &#13;&#10;* &#34;Существенный недостаток товара - неустранимый недостаток или недостаток, который не может быть устранен без несоразмерных затрат времени, или выявляется неоднократно, или проявляется вновь после его устранения, или другие подобные недостатки&#34;. Выдержка из преамбулы к Закону РФ &#34;О защите прав потребителей&#34;.&#13;&#10;&#13;&#10;По всем вышеуказанным вопросам обращаться в Интернет-магазин по телефону 8-800-555-00-00 или по электронной почте info@eldorado.ru&#13;&#10;&#13;&#10;ВНИМАНИЕ!!!  Подключение технически сложных изделий должно произволиться квалифицированными сепцалистами&#13;&#10; Не пользуйтесь услугами сомнительных организаций и частных лиц,  не имеющих  лицензии на проведение монтажа. &#13;&#10;По всем вопросам установки и сервисного обслуживания обращайтесь в call-ценр по Установке техники и сервисному обслуживанию:&#13;&#10; т.8 800 555 11 11 или пишите на адрес service@eldorado.ru                                   ' AS MVALUE&#13;&#10;from dual" PropData="05704C65667403A0020470546F700238095661726961626C65730100"/>
    <TfrxDOAQuery Name="odsORDER" UserName="odsORDER" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  s.staff_id,&#13;&#10;  s.name,&#13;&#10;  '*'||LPAD(o.order_id,12,'0')||DIGITTOCODE39(o.order_id)||'*' as orderid39,&#13;&#10;  '*'||LPAD(o.order_id,12,'0')||'*' as orderid,&#13;&#10;  o.*,&#13;&#10;  NVL( (SELECT MAX('Y') FROM o_good_item ogi, g_unit gu&#13;&#10;        WHERE ogi.order_id = o.order_id&#13;&#10;          AND gu.g_unit_id = ogi.g_unit_id&#13;&#10;          AND gu.good_code = '99000112'), 'N' )AS is_sogl&#13;&#10;FROM orders o, staff s&#13;&#10;WHERE o.order_id=:order_id&#13;&#10;  AND o.createusercode=s.user_code(+)" PropData="05704C65667402340470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsAddrShop" UserName="odsAddrShop" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT&#13;&#10;  T.WBLTYPE,&#13;&#10;  --a.namebase as CLNT_NAME,&#13;&#10;  pk_franch.get_werkname('ORDERS',:order_id,'PHYSADDR') as CLNT_NAME,&#13;&#10;  a.CITY || case when not a.CITY is null then ', ' else '' end ||&#13;&#10;  a.STREET || case when not a.STREET is null then ', ' else '' end ||&#13;&#10;  a.HOUSE_NUM || case when not a.HOUSE_NUM is null then ', ' else '' end ||&#13;&#10;  case when not a.house_corp is null then 'корп. '||a.house_corp||', ' else '' end ||&#13;&#10;  case when not a.BUILDING is null then 'стр. '||a.BUILDING||', ' else '' end ||&#13;&#10;  case when not a.ROOM_NUMBER is null then 'к.' else '' end || a.ROOM_NUMBER&#13;&#10;    AS CLNT_ADDR,&#13;&#10;  'ИНН '||abn.INN as INN,&#13;&#10;  ac.value as tel&#13;&#10;FROM&#13;&#10;  address a, addr_bninfo abn, addr_comm ac&#13;&#10;  ,(SELECT wt.* FROM w_type wt, werk w WHERE wt.wtype_code = w.wtype_code AND w.is_base = 'Y') T&#13;&#10;WHERE a.address_id = pk_franch.get_addr_id('ORDERS',:order_id, 'PHYSADDR')&#13;&#10;  AND abn.address_id(+) = a.address_id&#13;&#10;  AND ac.address_id(+) = a.address_id&#13;&#10;  AND ac.ct_code(+) = 'TEL'" PropData="05704C656674027C0470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
    <TfrxDOAQuery Name="odsITOGO" UserName="odsITOGO" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="frDB_BaseTable" Session="DBM.OraSession" Active="False" Debug="False" MasterFields="order_id" Params.PropData="04446174610A270000000300000001000000090000003A4F524445525F4944050000000600000033363334310000000000" SQL.Text="SELECT &#13;&#10;  TO_CHAR(SUM(amount),'999999990D00') AS ITOGO&#13;&#10;FROM&#13;&#10;  o_good_item&#13;&#10;WHERE&#13;&#10;  order_id = :order_id&#13;&#10;  AND IGNORE&#60;&#62;'Y'" PropData="05704C6566740310010470546F700238095661726961626C657301010C480000002044617461547970653D226674537472696E6722204E616D653D224F524445525F49442220506172616D547970653D227074556E6B6E6F776E222056616C75653D223336333431220000"/>
